definition
      ITEM_TREE[at0000] occurrences matches {0..1} matches { -- demographic data about a person and his/her biometric identifier
 		items cardinality matches {1..*; ordered} matches {
 			CLUSTER[at0001] matches {  -- Birth data
       				items matches {
            				ELEMENT[at0010] occurrences matches {1..1} matches {  -- Birth date
                				value matches {
                    					DV_DATE matches {*}
                				}
            				}
 				        ELEMENT[at0011] occurrences matches {0..*} matches {  -- Date of birth follow up indicator
				                value matches {
 					                DV_BOOLEAN matches {*}   
				                }
				        }
          				ELEMENT[at0012] occurrences matches {0..1} matches {  -- Country of birth
                				value matches {
                    					DV_CODED_TEXT matches {
                        					defining_code matches {[ac0001]}
                    					}
				                }
				        }
		 		        ELEMENT[at0013] occurrences matches {0..1} matches {  -- State
  		             			value matches {
					               	DV_CODED_TEXT matches {  
		                        			defining_code matches {[ac0002]}
 				                   	}
		        		        }
		        		}
				        ELEMENT[at0014] occurrences matches {0..1} matches {  -- City
					         value matches {
				                 	DV_CODED_TEXT matches {  
				                        	defining_code matches {[ac0003]}
				                    	}
				                }
				        }
				        ELEMENT[at0015] occurrences matches {0..1} matches {  -- Plurality
				                value matches {
				    			DV_TEXT matches {*}
				                    	DV_CODED_TEXT matches { 
					                        defining_code matches {[ac0004]}
					            	}
				                }
				        }
				        ELEMENT[at0016] occurrences matches {0..1} matches {  -- Birth order
				                value matches {
				    			DV_TEXT matches {*}
					                DV_CODED_TEXT matches {
 					                       defining_code matches {[ac0005]}
					                }
				                }
				        }
			                ELEMENT[at0017] occurrences matches {0..*} matches {  -- sex
                     				value matches {
				    			 DV_TEXT matches {*}
				                         DV_CODED_TEXT matches { 
				                                defining_code matches {
 			                                   		       [local::
                        			            			at0310,
 						                                at0311,
                        			            			at0312,
                        			            			at0313]
								}
                        				  }
                     				}
 			                }			
					ELEMENT[at0018] matches {	-- registry office
						value matches {
							DV_TEXT matches {*}
						}
					}
            				allow_archetype CLUSTER[at0006] occurrences matches {0..1} matches {
                				include
            					        archetype_id/value matches {/openEHR-DEMOGRAPHIC-CLUSTER\.birth_data_additional_detail[a-zA-Z0-9_-]*\.v0/}
          				}
			        }
			}
	    		CLUSTER[at0002] matches {	-- death demographic data
				items matches {	
					ELEMENT[at0021] occurrences matches {1} matches { -- death date
						value matches { 
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- source of notification
						value matches {
				    			DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
										[local::
										at0220,     -- 1 - Registry
										at0221,     -- 2 - Healthcare provider
										at0222,     -- 3 - Relative
										at0223,     -- 4 - Other
										at0224]     -- 9 - Unknown    
								}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- country of death
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}  -- country codes
							}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {	-- state/territory/province
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0002]}  -- state codes
							}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- city/town/locality
						value matches {
				    			DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {[ac0003]}    -- city codes
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- death certificate number
						value matches {
							DV_TEXT matches {*}
						}
					}
		
				 }
	    		}
			CLUSTER[at0004] matches { -- biometric identifier
				items matches {
					ELEMENT[at0041] occurrences matches {1} matches { -- Type of biometric identifier
						value matches {
							DV_TEXT matches {*} 
							DV_CODED_TEXT matches {
								defining_code matches {[local::
 		                                      			                at0410,     -- 01 - Finger print—rolled
                		                       			                at0411,     -- 02 - Finger print—flat
                		                       			                at0412,     -- 03 - Facial features
                		                      			                at0413,     -- 04 - Voice
                		                       			                at0414,     -- 05 - Iris
                		                       			                at0415,     -- 06 - Retina
                		                      			                at0416,     -- 07 - Hand geometry
                		                       			                at0417,     -- 08 - Signature dynamics
                		                       			                at0418,     -- 09 - Keystroke dynamics
                		                      			                at0419,     -- 10 - Lip movement
                		                       			                at0420,     -- 11 - Thermal face image
                		                       			                at0421,     -- 12 - Thermal hand image
                		                      			                at0422,     -- 13 - Gait
                		                       			                at0423,     -- 14 - Blood type (including rh factor)
                		                       			                at0424,     -- 15 - DNA
									                at0425]	    -- 99 - Unknown
										        }
							}
						}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {	-- Biometric data template
						value matches {
							DV_TEXT matches {*} 
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {	-- Biometric enrolment quality
						value matches {
							DV_PROPORTION matches {*}
						}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {	-- Biometric creating authority
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0045] occurrences matches {0..*} matches {	-- Location of biometric identifier
						value matches {
				    			DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {[ac0008]}
 							}
						}
					}
					ELEMENT[at0046] occurrences matches {0..1} matches { -- Biometric device identification
						value matches  {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches { -- Biometric version 
						value matches  {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0048] occurrences matches {0..1} matches { -- Biometric date of creation
						value matches { 
							DV_DATE matches {*}
						}
					}	
				}
			}
			ELEMENT[at0031] occurrences matches {0..*} matches {  -- gender
                     		value matches {
	                       		DV_TEXT matches {*}				
				        DV_CODED_TEXT matches { 
				                                defining_code matches {
 			                                   		       [local::
                        			            			at0310,
 						                                at0311,
                        			            			at0312,
                        			            			at0313]
								}
                        		}
                     		}
 			}			
       			ELEMENT[at0032] occurrences matches {0..*} matches {  -- mother's family name
	                       value matches {
                        		DV_TEXT matches {*}  
                               }
                  	}
			ELEMENT[at0033] occurrences matches {0..*} matches {  -- Marital Status
                        	value matches {
	                       		DV_TEXT matches {*}				
                        		DV_CODED_TEXT matches { 
						defining_code matches {[ac0006]}
                        		}
 			        }
 			}
 			ELEMENT[at0034] occurrences matches {0..*} matches {  -- Ethnic group
			        value matches {
	                       		DV_TEXT matches {*}						
					DV_CODED_TEXT matches { 
                                		defining_code matches {[ac0007]}
 		                        }
  	                       }
 	                 }
 	                 ELEMENT[at0035] occurrences matches {0..*} matches {  -- identification comments
                        	value matches {
			               DV_TEXT matches {*}
                     		}
                  	 }
            		allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {
		                include
                		    archetype_id/value matches {/openEHR-DEMOGRAPHIC-CLUSTER\.person_identifier[a-zA-Z0-9_-]*\.v0/}
            		}
         	}
     	}



