							at0060,    -- Large
							at0065,    -- Small
							at0076]    -- Debris
						}
					}
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0044] occurrences matches {0..*} matches {    -- First polar body morphology
				value matches {
					DV_TEXT matches {*}
