			}
			ELEMENT[at0070] occurrences matches {0..1} matches {    -- Specimen identifier
				value matches {
					DV_IDENTIFIER matches {*}
					DV_URI matches {*}
				}
			}
			allow_archetype CLUSTER[at0098] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
		}
	}



