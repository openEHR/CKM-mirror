					}
					ELEMENT[at0063] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0064] occurrences matches {0..1} matches {    -- Filter passed
						value matches {
							DV_BOOLEAN matches {*}
