definition
	CLUSTER[at0000] matches {    -- WHO histological grade of urothelial neoplasms (2004/2016)
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Grade
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0009,    -- Papillary urothelial neoplasm of low malignant potential (PUNLMP)
							at0007,    -- Low-grade papillary urothelial carcinoma
							at0008]    -- High-grade papillary urothelial carcinoma
						}
					}
				}
			}
		}
	}


