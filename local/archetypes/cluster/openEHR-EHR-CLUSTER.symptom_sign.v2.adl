		["ca"] = <
			items = <
				["at0000"] = <
					text = <"Símptoma/Signe">
					description = <"Observació notificada d’una alteració física o mental en un pacient.">
				>
				["at0001"] = <
					text = <"Nom del símptoma/signe">
					description = <"El nom del símptoma o signe notificat.">
					comment = <"Es recomana programar el nom del símptoma amb una terminologia quan sigui possible.">
				>
				["at0002"] = <
					text = <"Informació">
					description = <"Informació descriptiva sobre el símptoma o signe notificat.">
				>
				["at0003"] = <
					text = <"Patró">
					description = <"Informació descriptiva sobre el patró del símptoma o signe durant aquest episodi.">
					comment = <"Per exemple: el dolor es pot descriure com a constant o intermitent.">
				>
				["at0017"] = <
					text = <"Efecte">
					description = <"Efecte percebut del factor de modificació en el símptoma o signe.">
				>
				["at0018"] = <
					text = <"Factor de modificació">
					description = <"Dada sobre com un factor específic afecta el símptoma o signe identificat durant aquest episodi.">
				>
				["at0019"] = <
					text = <"Factor">
					description = <"Nom del factor de modificació.">
					comment = <"Exemples de factors de modificació: gitar-se damunt de múltiples coixins, menjar o l’administració d’una medicació específica.">
				>
				["at0021"] = <
					text = <"Categoria de gravetat">
					description = <"Categoria que representi la gravetat general del símptoma o signe.">
					comment = <"Definir valors com lleu, moderat o greu de forma que es puguin aplicar a múltiples símptomes o signes i que permetin que múltiples usuaris els interpretin i registrin de forma consistent no és fàcil. Algunes organitzacions amplien més el conjunt de valors amb la inclusió de valors com «Trivial» i «Molt greu», o «Lleu-Moderat» i «Moderat-Greu». Això augmenta la dificultat de les definicions i pot també agreujar els problemes d’exactitud entre els registradors. L’ús de «Mortal» i «Letal» també es considera sovint com a part d’aquest conjunt de valors, encara que, des d’un punt de vista lògic, en realitat pot reflectir el resultat més que la gravetat. Tenint en compte això, continuar amb una llista ben definida, però més breu és preferible, i per això s’ofereix el conjunt lleu/moderat/greu, encara que elegir altre text permet que altres conjunts de valors s’incloguin dins d’aquest element de dades en una plantilla.">
				>
				["at0023"] = <
					text = <"Lleu">
					description = <"La intensitat del símptoma o signe no interfereix amb el desenvolupament de les activitats habituals.">
				>
				["at0024"] = <
					text = <"Moderat">
					description = <"La intensitat del símptoma o signe interfereix amb el desenvolupament de les activitats habituals.">
				>
				["at0025"] = <
					text = <"Greu">
					description = <"La intensitat del símptoma o signe impedeix el desenvolupament de les activitats habituals.">
				>
				["at0028"] = <
					text = <"Durada de l’episodi">
					description = <"La durada d’aquest episodi del símptoma o signe des de l’aparició inicial.">
					comment = <"Si la «Data/hora de l’aparició» i la «Data/hora de la resolució» s’utilitzen en sistemes, aquest element de dades es pot calcular, o, alternativament, es pot considerar redundant en aquesta situació. El tipus de dades en text s’utilitza per a registrar els intervals de la duració programada, com «0-7 dies, 1-2 setmanes, 2 setmanes o més».">
				>
				["at0031"] = <
					text = <"Nombre dels episodis anteriors">
					description = <"El nombre de vegades que aquest símptoma o signe ha ocorregut prèviament.">
				>
				["at0037"] = <
					text = <"Descripció de l'episodi">
					description = <"Informació descriptiva sobre el curs del símptoma o signe durant aquest episodi.">
					comment = <"Per exemple: una descripció d’un text sobre l’aparició immediata del símptoma, activitats que agreujaren o alleugeraren el símptoma, si està millorant o agreujant, i com es resolgué al llarg de les setmanes.">
				>
				["at0056"] = <
					text = <"Informació">
					description = <"Informació descriptiva de l'efecte del factor de modificació en el símptoma o signe.">
				>
				["at0057"] = <
					text = <"Descripció d'episodis anteriors">
					description = <"Informació descriptiva de qualsevol o tots els episodis anterior.">
					comment = <"Per exemple: freqüència/periodicitat - per hora, dia, setmana, mes, any; i regularitat. Pot incloure una comparació amb aquest episodi.">
				>
				["at0063"] = <
					text = <"Símptoma/signe associat">
					description = <"Dades estructurades de qualsevol símptoma o signe associat que sigui concurrent.">
					comment = <"En els sistemes clínics connectats, és possible que ja s'hagin registrat dins del EHR símptomes o signes associats. Els sistemes permeten al metge fer LINK a símptomes/signes associats rellevants. No obstant això, en un sistema o missatge sense LINKs a dades existents o amb un nou pacient, es poden incloure instàncies addicionals de l'arquetip símptoma per a representar símptomes/signes associats.">
				>
				["at0146"] = <
					text = <"Episodis anteriors">
					description = <"Dades estructurades del símptoma o signe durant un episodi anterior.">
					comment = <"En els sistemes clínics connectats, és possible que ja s'hagin registrat dins del EHR episodis anteriors. Els sistemes permeten al metge fer LINK a episodis anteriors rellevants. No obstant això, en un sistema o missatge sense LINKs a dades existents o amb un nou pacient, es poden incloure instàncies addicionals de l'arquetip símptoma per a representar episodis anteriors. Es recomana que les noves instàncies de l'arquetip Símptoma inserides en aquest SLOT representen un o molts episodis anteriors només a aquesta instància del símptoma.">
				>
				["at0147"] = <
					text = <"Zona estructurada del cos">
					description = <"Zona estructurada del cos on s'ha notificat el símptoma o signe.">
					comment = <"Si la ubicació anatòmica està inclosa en el Nom del símptoma mitjançant codis precoordinats, l’ús d’aquest SLOT esdevé redundant. Si es registra la ubicació anatòmica utilitzant l’element de dades «Zona del cos», aleshores l’ús d’arquetips de tipus CLUSTER en aquest SLOT no està permès – registrar només la «Zona simple del cos» O la «Zona estructurada del cos», però no ambdues.">
				>
				["at0151"] = <
					text = <"Zona del cos">
					description = <"Zona particular del cos on s'ha notificat el símptoma o signe.">
					comment = <"Esdeveniments d’aquest element de dades estan configurats en 0..* per a permetre la separació de múltiples zones del cos en una plantilla si així es vol. Això permet la representació de situacions clíniques on un símptoma o signe s’hagi de registrar en múltiples ubicacions o per a identificar tant la zona d’origen com la zona distal de la radiació del dolor quan tots els altres aspectes com l’impacte i la duració són idèntics. Si els requisits per a registrar la zona del cos es determinen durant el temps d’execució mitjançant l’aplicació o requereixen un modelatge més complex, com ubicacions relatives, aleshores s’ha d’utilitzar CLUSTER.anatomical_location o CLUSTER.relative_location dins del SLOT «Ubicació anatòmica precisa» d’aquest arquetip.
Si la ubicació anatòmica està inclosa en el Nom del símptoma mitjançant codis precoordinats, aquest element de dades esdevé redundant. Si la ubicació anatòmica es registra utilitzant el SLOT «Zona estructurada del cos», aleshores l’ús d’aquest element de dades no està permès – registrar només la «Zona particular del cos» O la «Zona estructurada del cos», però no ambdues.">
				>
				["at0152"] = <
					text = <"Aparició de l’episodi">
					description = <"L’aparició del símptoma o signe d’aquest episodi.">
					comment = <"Encara que les dates parcials estan permeses, la data i hora exactes de l’aparició es poden registrar si fos necessari. Si s’experimenta aquest símptoma o signe per primera vegada o és una reaparició, aquesta data s’utilitza per a representar l’aparició d’aquest episodi. Si aquest símptoma o signe està en curs, aquest element de dades pot ser redundant si s’ha registrat amb anterioritat.">
				>
				["at0153"] = <
					text = <"Dades específiques">
					description = <"Els elements de dades específics que es necessiten addicionalment per a registrar com a únics els atributs dels símptomes o signes identificats.">
					comment = <"Per exemple: escala CTCAE.">
				>
				["at0154"] = <
					text = <"Dada del factor">
					description = <"Dada estructurada sobre el factor associat amb el símptoma o signe identificat.">
				>
				["at0155"] = <
					text = <"Impacte">
					description = <"Descripció de l'impacte d'aquest símptoma o signe.">
					comment = <"L'avaluació de l'impacte pot considerar la severitat, duració i freqüència del símptoma, així com el tipus d'impacte, incloent-hi, però no limitat a l'impacte funcional, social i emocional. Els esdeveniments d'aquest element de dades estan configurats en 0..* per a permetre la separació de múltiples tipus d'impactes en una plantilla si així es vol. Exemples d’un impacte funcional originats per la pèrdua d'escolta poden incloure: «Dificultat per a Escoltar en un Ambient Tranquil», «Dificultat per a Escoltar la Televisió o la Ràdio»; «Dificultat per a Escoltar una Conversació de Grup»; i «Dificultat per a Escoltar quan es parla per Telèfon».">
				>
				["at0156"] = <
					text = <"Cap efecte">
					description = <"El factor no té cap efecte en el símptoma o signe.">
				>
				["at0158"] = <
					text = <"Agreuja">
					description = <"El factor augmenta la gravetat o impacte del símptoma o signe.">
				>
				["at0159"] = <
					text = <"Alleuja">
					description = <"El factor disminueix la gravetat o impacte del símptoma o signe, però no el resol totalment.">
				>
				["at0161"] = <
					text = <"Data/hora de la resolució">
					description = <"El moment de la cessació d'aquest episodi del símptoma o signe.">
					comment = <"Si «Data/hora de l’aparició» i «Duració» s'utilitzen en sistemes, aquest element de dades es pot calcular o, alternativament, es pot considerar redundant. Encara que les dades parcials estan permeses, la data i hora exactes de la resolució es poden registrar, si fos necessari.">
				>
				["at0163"] = <
					text = <"Observacions">
					description = <"Informació addicional sobre un símptoma o signe no recollida en altres caselles.">
				>
				["at0164"] = <
					text = <"Temporització de l’aparició">
					description = <"Temporització de l’aparició i desenvolupament del símptoma o signe.">
					comment = <"El tipus d’aparició es pot programar amb una terminologia, si així es vol. Per exemple: gradual o abrupte.">
				>
				["at0165"] = <
					text = <"Factor desencadenant">
					description = <"Dades sobre els factors específics associats amb el desencadenant del símptoma o signe.">
					comment = <"Per exemple: tombar-se provoca cremor d'estómac, o pujar un turó provoca claudicació.">
				>
				["at0170"] = <
					text = <"Factor">
					description = <"Nom de l'esdeveniment sanitari, símptoma, signe notificat o altre factor.">
					comment = <"Per exemple: l'aparició d'altre símptoma; tombar-se, o pujar un turó.">
				>
				["at0171"] = <
					text = <"Interval de temps">
					description = <"Interval de temps entre l'esdeveniment o l'aparició del factor i l'aparició del símptoma o signe.">
				>
				["at0175"] = <
					text = <"Circumstancialitat">
					description = <"Categoria d’aquest episodi per al símptoma o signe identificat.">
				>
				["at0176"] = <
					text = <"Nou">
					description = <"Un episodi nou del símptoma o signe – sigui el primer cas o una reaparició quan l’episodi anterior ja estava totalment resolt.">
				>
				["at0177"] = <
					text = <"Indeterminat">
					description = <"No és possible determinar si aquest esdeveniment del símptoma o signe és nou o està en curs.">
				>
				["at0178"] = <
					text = <"En curs">
					description = <"Aquest símptoma o signe segueix en curs, particularment en un únic episodi continu.">
				>
				["at0180"] = <
					text = <"Progressió">
					description = <"Descripció de la progressió del símptoma o signe al moment de presentar l’informe.">
					comment = <"Esdeveniments d’aquest element de dades estan configurats en 0..* per a permetre que múltiples tipus de progressió estiguin separats en una plantilla si així es vol – per exemple, gravetat o freqüència. El Tipus de dades en text s’afegeix com a opció, per a mantenir altres conjunts de valors que no siguin els que estan inclosos en l’arquetip.">
				>
				["at0181"] = <
					text = <"Millorament">
					description = <"La gravetat del símptoma o signe en conjunt ha millorat durant aquest episodi.">
				>
				["at0182"] = <
					text = <"Inalterat">
					description = <"La gravetat del símptoma o signe en conjunt no ha canviat durant aquest episodi.">
				>
				["at0183"] = <
					text = <"Agreujament">
					description = <"La gravetat del símptoma o signe en conjunt ha agreujat durant aquest episodi.">
				>
				["at0184"] = <
					text = <"Resolt">
					description = <"La gravetat del símptoma o signe s’ha resolt.">
				>
				["at0185"] = <
					text = <"Informació">
					description = <"Informació descriptiva sobre l'efecte del factor en el símptoma o signe identificat.">
				>
				["at0186"] = <
					text = <"Esdeveniment">
					description = <"Tipus d’esdeveniment per aquest símptoma o signe.">
				>
				["at0187"] = <
					text = <"Primer cas">
					description = <"Aquest és el primer cas d’aquest símptoma o signe.">
				>
				["at0188"] = <
					text = <"Reaparició">
					description = <"Nou esdeveniment del mateix símptoma o signe després que es resolgués un episodi anterior.">
				>
				["at0189"] = <
					text = <"Caràcter">
					description = <"Paraula o oració breu que descrigui la natura del símptoma o signe.">
					comment = <"Per exemple: el dolor es pot descriure com «persistent», «roent» o «com una descàrrega elèctrica»; el mal de cap pot ser «palpitant» o «constant». Es recomana programar amb una terminologia externa quan sigui possible.">
				>
				["at0190"] = <
					text = <"Factor resolutiu">
					description = <"Dades sobre els factors específics associats amb la resolució del símptoma o signe.">
					comment = <"Per exemple: mantindre una postura dreta deté la cremor d'estómac, o reposar deté la claudicació.">
				>
				["at0193"] = <
					text = <"Factor">
					description = <"Nom de l'esdeveniment sanitari, símptoma, signe notificat o altre factor.">
					comment = <"Per exemple: mantindre una postura dreta o reposar.">
				>
				["at0194"] = <
					text = <"Dada del factor">
					description = <"Dada estructurada sobre el factor associat amb el símptoma o signe identificat.">
				>
				["at0195"] = <
					text = <"Interval de temps">
					description = <"Interval de temps entre l'esdeveniment o l'aparició del factor i la resolució del símptoma o signe.">
				>
				["at0196"] = <
					text = <"Informació">
					description = <"Informació descriptiva sobre l'efecte del factor en el símptoma o signe identificat.">
				>
				["at0197"] = <
					text = <"Dada del factor">
					description = <"Dada estructurada sobre el factor associat amb el símptoma o signe identificat.">
				>
				["at0198"] = <
					text = <"Classificació de gravetat">
					description = <"Escala de classificació numèrica que representa la gravetat general del símptoma o signe.">
					comment = <"El pacient pot classificar la gravetat registrant una puntuació; per exemple, de 0 (és a dir, símptoma absent), a 10 (és a dir, el símptoma és tan greu com la persona se’l pot imaginar).">
				>
				["at0200"] = <
					text = <"Nadir">
					description = <"Data/hora quan un símptoma o signe monofàsic i progressiu arribà a la seva intensitat o impacte funcional màxims.">
					comment = <"Per exemple: la data quan els signes neurològics de la malaltia de Guillain-Barré estaven en el moment de major intensitat.">
				>
			>
		>
