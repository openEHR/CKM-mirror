definition
	CLUSTER[at0000] matches {    -- Specimen
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0029] occurrences matches {0..1} matches {    -- Specimen type
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Laboratory specimen identifier
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0088] occurrences matches {0..*} matches {    -- External identifier
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0034] occurrences matches {0..1} matches {    -- Date/time received
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0008] occurrences matches {0..*} matches {    -- Sampling context
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- Physical properties
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.physical_properties(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.physical_properties(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.body_fluid(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.body_fluid(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Collection method
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0079] occurrences matches {0..1} matches {    -- Collection description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0087] occurrences matches {0..1} matches {    -- Source site
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {    -- Structured source site
				include
