					comment = <"Till exempel detaljer om nålbiopsier vid prostatacancer, där både beställningen och svaret innehåller många specifika detaljer om provmaterialet.">
				>
				["at0085"] = <
					text = <"Detaljer om provbehållare">
					description = <"Detaljer om provbehållare som har använts.">
				>
