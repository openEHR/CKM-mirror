				>
				["at0070"] = <
					text = <"Provtagaridentifierare">
					description = <"Identifierare för den person eller organisation som ansvarar för provtagningen.">
					comment = <"Detta dataelement är avsett att användas när provmaterialet redan har samlats in och den faktiska provtagaren är känd.">
				>
				["at0071"] = <
					text = <"Detaljer om provtagaren">
					description = <"Den person eller organisation som ansvarar för provtagningen.">
				>
