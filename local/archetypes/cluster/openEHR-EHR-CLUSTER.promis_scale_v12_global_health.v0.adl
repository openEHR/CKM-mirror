definition
	CLUSTER[at0000] matches {    -- PROMIS Scale v1.2 - Global Health
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- In general, how would you rate your health?
				value matches {
					1|[local::at0040],
					2|[local::at0041],
					3|[local::at0042],
					4|[local::at0043],
					5|[local::at0044]
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- In general, how would you rate your quality of life?
				value matches {
					1|[local::at0030],
					2|[local::at0031],
					3|[local::at0032],
					4|[local::at0033],
					5|[local::at0034]
				}
			}
			ELEMENT[at0045] occurrences matches {0..1} matches {    -- In general, how would you rate your physical health?
				value matches {
					1|[local::at0052],
					2|[local::at0053],
					3|[local::at0054],
					4|[local::at0055],
					5|[local::at0056]
				}
			}
			ELEMENT[at0046] occurrences matches {0..1} matches {    -- In general, how would you rate your mental health, including your mood and your ability to think?
				value matches {
					1|[local::at0058],
					2|[local::at0059],
					3|[local::at0060],
					4|[local::at0061],
					5|[local::at0062]
				}
			}
			ELEMENT[at0047] occurrences matches {0..1} matches {    -- In general, how would you rate your satisfaction with your social activities and relationships?
				value matches {
					1|[local::at0063],
					2|[local::at0064],
					3|[local::at0065],
					4|[local::at0066],
					5|[local::at0067]
				}
			}
			ELEMENT[at0048] occurrences matches {0..1} matches {    -- In general, please rate how well you carry out your usual social activities and roles. (This includes activities at home, at work and in your community, and responsibilities as a parent, child, spouse, employee, friend, etc.)
				value matches {
					1|[local::at0068],
					2|[local::at0069],
					3|[local::at0070],
					4|[local::at0071],
					5|[local::at0072]
				}
			}
			ELEMENT[at0049] occurrences matches {0..1} matches {    -- To what extent are you able to carry out your everyday physical activities such as walking, climbing stairs, carrying groceries, or moving a chair?
				value matches {
					1|[local::at0073],
					2|[local::at0074],
					3|[local::at0075],
					4|[local::at0076],
					5|[local::at0077]
				}
			}
			ELEMENT[at0050] occurrences matches {0..1} matches {    -- In the past 7 days: How often have you been bothered by emotional problems such as feeling anxious, depressed or irritable?
				value matches {
					1|[local::at0078],
					2|[local::at0079],
					3|[local::at0080],
					4|[local::at0081],
					5|[local::at0082]
				}
			}
			ELEMENT[at0051] occurrences matches {0..1} matches {    -- In the past 7 days: How would you rate your fatigue on average?
				value matches {
					1|[local::at0083],
					2|[local::at0084],
					3|[local::at0085],
					4|[local::at0086],
					5|[local::at0087]
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- In the past 7 days: How would you rate your pain on average?
				value matches {
					0|[local::at0010],
					1|[local::at0016],
					2|[local::at0017],
					3|[local::at0018],
					4|[local::at0019],
					5|[local::at0020],
					6|[local::at0021],
					7|[local::at0022],
					8|[local::at0023],
					9|[local::at0024],
					10|[local::at0025]
				}
			}
		}
	}


