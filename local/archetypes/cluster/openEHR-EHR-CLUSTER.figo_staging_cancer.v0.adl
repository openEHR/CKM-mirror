ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"FIGO staging system for gynecological cancer">
					description = <"A staging system for cancers of the female reproductive organs, published by The International Federation of Gynecology and Obstetrics (FIGO).">
				>
				["at0002"] = <
					text = <"FIGO stage">
					description = <"Assesment of the size and extent of the tumor, the involvement of nearby lymph nodes, and the presence or absence of distant metastases.">
				>
				["at0003"] = <
					text = <"Molecular subtype">
					description = <"Presence of molecular features affecting the FIGO stage.">
					comment = <"For example: POLEmut, MMRd, NSMP, and p53abn.">
				>
				["at0004"] = <
					text = <"FIGO version">
					description = <"The version of the FIGO staging system used for the assessment.">
					comment = <"For example: 2009.">
				>
				["at0005"] = <
					text = <"Cancer type">
					description = <"The tumour type of the FIGO staging.">
					comment = <"For example: ovarian, endometrial, cervical, vaginal or vulval cancer, gestational trophoblastic neoplasia, or sarcoma.">
				>
				["at0006"] = <
					text = <"FIGO staging with molecular classification">
					description = <"Concatenation of FIGO stage and molecular subtype with 'm' as delimiter.">
					comment = <"For example: IIImPOLEmut or IVmPOLEmut.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"FIGO stadieinndeling for gynekologisk kreft">
					description = <"Et stadieinndelingssystem for kreft i de kvinnelige kjønnsorganene utgitt av The International Federation of Gynecology and Obstetrics (FIGO).">
				>
				["at0002"] = <
					text = <"FIGO stadium">
					description = <"Vurdering av størrelse og utbredelse av tumor, involvering av nærliggende lymfeknuter, og tilstedeværelse eller fravær av fjernmetastaser.">
				>
				["at0003"] = <
					text = <"Molekylær subtype">
					description = <"Tilstedeværelse av molekylære egenskaper som påvirker FIGO stadiet.">
				>
				["at0004"] = <
					text = <"FIGO-versjon">
					description = <"Versjonen av FIGO stadieinndeling som er brukt for å bestemme FIGO stadiet.">
					comment = <"For eksempel: 2009.">
				>
				["at0005"] = <
					text = <"Krefttype">
					description = <"Krefttypen som klassifiseres etter FIGO stadieinndeling.">
					comment = <"For eksempel: ovariekreft, endometriekreft, livmorhalskreft, vaginakreft, vulvakreft, trofoblastisk kreft i svangerskapet eller sarkom.">
				>
				["at0006"] = <
					text = <"FIGO stadium med molekyær klassifisering">
					description = <"Sammentrekning av FIGO stadium og molekylær subtype med 'm' som skilletegn.">
					comment = <"For eksempel: IIImPOLEmut eller IVmPOLEmut.">
				>
			>
		>
	>
