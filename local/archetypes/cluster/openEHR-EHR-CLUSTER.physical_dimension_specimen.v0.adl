							>
						>
					>
				}
			}
			ELEMENT[at0044] occurrences matches {0..1} matches {    -- Thickness
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
							>
						>
					>
				}
			}
			ELEMENT[at0048] occurrences matches {0..2} matches {    -- Diameter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
							>
						>
					>
				}
			}
			ELEMENT[at0045] occurrences matches {0..1} matches {    -- Circumference
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
							>
						>
					>
				}
			}
			ELEMENT[at0047] occurrences matches {0..1} matches {    -- Area
				value matches {
					C_DV_QUANTITY <
						
						list = <
							["1"] = <
								units = <"cm2">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"mm2">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m2">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[sin_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[sft_i]">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0020] occurrences matches {0..1} matches {    -- Weight
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::124]>
						list = <
							["1"] = <
								units = <"g">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"kg">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"mg">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[oz_av]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[lb_av]">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
		}
	}


