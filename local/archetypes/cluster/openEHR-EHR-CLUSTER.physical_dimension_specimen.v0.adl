	term_definitions = <
		["ar-sy"] = <
			items = <
				["at0000"] = <
					text = <"*Physical dimension of a specimen (en)">
					description = <"*The size or magnitude of a specimen. (en)">
				>
				["at0020"] = <
					text = <"الوزن">
					description = <"*The measured value of the mass of the specimen. (en)">
				>
				["at0023"] = <
