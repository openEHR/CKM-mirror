definition
	CLUSTER[at0000] matches {    -- Physical dimension of a specimen
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0036] occurrences matches {0..1} matches {    -- Specimen name
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0023] occurrences matches {0..1} matches {    -- Description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0046] occurrences matches {0..1} matches {    -- Volume
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::129]>
						list = <
							["1"] = <
								units = <"ml">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"L">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"cl">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[foz_us]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[gal_us]">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0042] occurrences matches {0..3} matches {    -- Length
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
							>
						>
					>
				}
			}
			ELEMENT[at0043] occurrences matches {0..1} matches {    -- Width
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
