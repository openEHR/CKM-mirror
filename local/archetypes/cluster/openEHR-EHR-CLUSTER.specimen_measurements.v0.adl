				["at0042"] = <
					text = <"*Dimension (en)">
					description = <"*A measured dimension of the specimen. (en)">
					comment = <"*This data element has unlimited occurrences to allow for  measurements in 3 or more axes.
If there is no clear orientation of for example length vs width vs height, use multiple occurrences to allow recording of the length of 3 or more axes without any specific orientation, for example 3 x 4 x 5 mm. (en)">
				>
				["at0046"] = <
					text = <"*Volume(en)">
					description = <"*Measured or calculated space enclosed by a boundary (liquid) or occupied by an object (solid tissue).
 (en)">
				>
				["at0049"] = <
					text = <"*Length (en)">
					description = <"*Measurement of the longest aspect of a specimen (eg longest edge of 3 edges) or the measured length of a specimen along its longitudinal axis (eg 2cm of bowel, measured along the axis of the bowel, and even if the measured length is shorter than the diameter of the bowel itself). (en)">
				>
				["at0050"] = <
					text = <"*Width (en)">
					description = <"*Measurement of widest aspect of a specimen, usually perpendicular to the length or the measured length of the second side of two sides. (en)">
				>
				["at0052"] = <
					text = <"*Diameter (en)">
					description = <"*Measurement of the specimen from edge to edge, usually of an approximately round or oval specimen, passing through its centre. (en)">
				>
				["at0053"] = <
					text = <"*Depth (en)">
					description = <"*Measurement of the third aspect of a specimen, usually perpendicular to the length and width or measurement of the vertical aspect of a specimen from the surface to the base. Also known as thickness. (en)">
				>
				["at0054"] = <
					text = <"*Height (en)">
