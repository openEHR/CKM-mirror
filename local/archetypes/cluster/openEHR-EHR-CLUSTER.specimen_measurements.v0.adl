definition
	CLUSTER[at0000] matches {    -- Specimen measurements
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0046] occurrences matches {0..1} matches {    -- Volume
				value matches {
					C_DV_QUANTITY <
						
						list = <
							["1"] = <
								units = <"ml">
								magnitude = <|>=0.0|>
							>
							["2"] = <
