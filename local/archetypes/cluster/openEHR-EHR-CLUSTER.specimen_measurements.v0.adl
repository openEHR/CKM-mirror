definition
	CLUSTER[at0000] matches {    -- Specimen measurements
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0046] occurrences matches {0..1} matches {    -- Volume
				value matches {
					C_DV_QUANTITY <
						
						list = <
							["1"] = <
								units = <"ml">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"L">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"cl">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[foz_us]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[gal_us]">
								magnitude = <|>=0.0|>
							>
							["6"] = <
								units = <"uL">
								magnitude = <|>=0.0|>
							>
							["7"] = <
								units = <"dm3">
								magnitude = <|>=0.0|>
							>
							["8"] = <
								units = <"cm3">
								magnitude = <|>=0.0|>
							>
							["9"] = <
								units = <"mm3">
								magnitude = <|>=0.0|>
							>
							["10"] = <
								units = <"um3">
								magnitude = <|>=0.0|>
							>
						>
					>
					DV_INTERVAL<DV_QUANTITY> matches {*}
				}
			}
			ELEMENT[at0042] occurrences matches {0..*} matches {    -- Dimension
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
								magnitude = <|>=0.0|>
							>
							["6"] = <
								units = <"um">
								magnitude = <|>=0.0|>
							>
							["7"] = <
								units = <"nm">
								magnitude = <|>=0.0|>
							>
						>
					>
					DV_INTERVAL<DV_QUANTITY> matches {*}
				}
				name matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0049,    -- Length
							at0050,    -- Width
							at0054,    -- Height
							at0053,    -- Depth
							at0052]    -- Diameter
						}
					}
				}
			}
			ELEMENT[at0055] occurrences matches {0..*} matches {    -- Perimeter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"m">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[in_i]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[ft_i]">
							>
							["6"] = <
								units = <"um">
								magnitude = <|>=0.0|>
							>
							["7"] = <
								units = <"nm">
								magnitude = <|>=0.0|>
							>
							["8"] = <
								units = <"Ao">
								magnitude = <|>=0.0|>
							>
						>
					>
					DV_INTERVAL<DV_QUANTITY> matches {*}
				}
			}
			ELEMENT[at0020] occurrences matches {0..1} matches {    -- Weight
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::124]>
						list = <
							["1"] = <
								units = <"g">
								magnitude = <|>=0.0|>
							>
							["2"] = <
								units = <"kg">
								magnitude = <|>=0.0|>
							>
							["3"] = <
								units = <"mg">
								magnitude = <|>=0.0|>
							>
							["4"] = <
								units = <"[oz_av]">
								magnitude = <|>=0.0|>
							>
							["5"] = <
								units = <"[lb_av]">
								magnitude = <|>=0.0|>
							>
							["6"] = <
								units = <"ug">
								magnitude = <|>=0.0|>
							>
							["7"] = <
								units = <"ng">
								magnitude = <|>=0.0|>
							>
						>
					>
					DV_INTERVAL<DV_QUANTITY> matches {*}
				}
			}
		}
	}


