				}
			}
			ELEMENT[at0.42] occurrences matches {0..1} matches {    -- Perforation size
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.43,    -- Pinhole
							at0.44,    -- Intermediate
							at0.45,    -- Subtotal
							at0.46]    -- Total
						}
					}
				}
			}
			ELEMENT[at0.47] occurrences matches {0..1} matches {    -- Intermediate estimation
				value matches {
					DV_CODED_TEXT matches {
