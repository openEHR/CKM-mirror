ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"WHO histological grade of urothelial neoplasms (1973)">
					description = <"A histological grading of urothelial tumors according to the WHO Histological Typing of Urinary Bladder Tumours (1973).">
				>
				["at0001"] = <
					text = <"Grade">
					description = <"">
				>
				["at0006"] = <
					text = <"Grade 1">
					description = <"Tumours that have the least degree of cellular anaplasia compatible with a diagnosis of malignancy.">
				>
				["at0007"] = <
					text = <"Grade 2">
					description = <"Tumors with a degree of cellular anaplasia in between grade 1 and grade 3.">
				>
				["at0008"] = <
					text = <"Grade 3">
					description = <"Tumours with the most severe degrees of cellular anaplasia.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"	 WHO histologisk grad av uroteliale neoplasmer (1973)">
					description = <"Histologisk grad av uroteliale neoplasmer i henhold til WHO Histological Typing of Urinary Bladder Tumours (1973).">
				>
				["at0001"] = <
					text = <"Grad">
					description = <"">
				>
				["at0006"] = <
					text = <"Grad 1">
					description = <"Tumorer som har lavest grad av cellulær anaplasi som stemmer overens med malign diagnose.">
				>
				["at0007"] = <
					text = <"Grad 2">
					description = <"Tumorer som har en grad av cellulær anaplasi mellom grad 1 og grad 3.">
				>
				["at0008"] = <
					text = <"Grad 3">
					description = <"Tumorer som har den mest alvorlige graden av cellulær anaplasi.">
				>
			>
		>
	>
