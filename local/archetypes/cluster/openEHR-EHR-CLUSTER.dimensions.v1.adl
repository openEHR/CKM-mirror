definition
	CLUSTER[at0000] matches {	-- Dimensions
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Circumference
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"µm">
							>
							["2"] = <
								units = <"mm">
							>
