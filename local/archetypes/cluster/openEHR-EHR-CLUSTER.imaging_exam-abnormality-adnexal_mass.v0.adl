definition
	CLUSTER[at0000.1.1] matches {    -- Imaging examination of an adnexal mass
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1.1] matches {    -- Body structure
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.1,    -- Morphological abnormality
							at0.0.1]    -- Adnexal mass
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Body site
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0003] occurrences matches {0..*} matches {    -- Structured body site
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v2|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {    -- Imaging findings
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_exam-lymph_node(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_exam-lymph_node_group(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_exam-abnormality(-[a-zA-Z0-9_]+)*\.v0/}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Impression
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Label
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.3.1] occurrences matches {0..1} matches {    -- Type
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.0.2,    -- Unilocular cyst
							at0.0.3,    -- Unilocular-solid cyst
							at0.0.4,    -- Multilocular cyst
							at0.0.5,    -- Multilocular-solid cyst
							at0.0.6,    -- Solid tumour
							at0.0.7]    -- Indeterminate
						}
					}
				}
			}
			ELEMENT[at0.4] occurrences matches {0..3} matches {    -- Diameter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
								precision = <|0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
						>
					>
				}
			}
			ELEMENT[at0.9] occurrences matches {0..1} matches {    -- Shape
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.10] occurrences matches {0..1} matches {    -- Texture
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.11] occurrences matches {0..1} matches {    -- Definition
				value matches {
					DV_TEXT matches {*}
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.12,    -- Well-defined
							at0.13]    -- Poorly-defined
						}
					}
				}
			}
			ELEMENT[at0.14] occurrences matches {0..1} matches {    -- Contour
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.15] occurrences matches {0..1} matches {    -- Margin
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.16] occurrences matches {0..1} matches {    -- Calcification
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.17,    -- Present
							at0.18]    -- Absent
						}
					}
				}
			}
			ELEMENT[at0.19] occurrences matches {0..1} matches {    -- Effect
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.20] occurrences matches {0..1} matches {    -- Vascularisation
				value matches {
					DV_TEXT matches {*}
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.21,    -- None
							at0.22,    -- Mild
							at0.23,    -- Moderate
							at0.24]    -- Marked
						}
					}
				}
			}
			ELEMENT[at0.0.15] occurrences matches {0..3} matches {    -- Solid diameter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
								precision = <|0|>
							>
							["2"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
						>
					>
				}
			}
			ELEMENT[at0.0.8] occurrences matches {0..3} matches {    -- Number of cyst locules
				value matches {
					DV_COUNT matches {
						magnitude matches {|>=0|}
					}
				}
			}
			ELEMENT[at0.0.13] occurrences matches {0..1} matches {    -- Number of papillary projections
				value matches {
					DV_COUNT matches {
						magnitude matches {|>=0|}
					}
				}
			}
			ELEMENT[at0.0.14] occurrences matches {0..1} matches {    -- Maximal septum width
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
								precision = <|0|>
							>
						>
					>
				}
			}
		}
	}


