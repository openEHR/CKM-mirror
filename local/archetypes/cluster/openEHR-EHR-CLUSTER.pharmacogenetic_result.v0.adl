				["at0095"] = <
					text = <"Uncertain risk of aminoglycoside-induced hearing loss">
					description = <"Uncertain risk of aminoglycoside-induced hearing loss.">
				>
				["at0096"] = <
					text = <"Variant detail">
					description = <"Details of variant findings.">
				>
				["at0097"] = <
