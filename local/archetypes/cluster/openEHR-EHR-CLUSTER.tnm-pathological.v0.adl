				["at0021"] = <
					text = <"Perineural infiltrasjon (Pn)">
					description = <"Vurdering av infiltrasjon inn i vev i tilslutning til nerver.">
					comment = <"For eksempel \"Pn0 (Ingen perineural infiltrasjon)\".">
