				["at0015"] = <
					text = <"L1">
					description = <"Invasjon i lymfekar.">
				>
				["at0016"] = <
					text = <"Veneinfiltrasjon (V)">
					description = <"Vurdering av veneinfiltrasjon.">
					comment = <"For eksempel \"V1 (Mikroskopisk veneinfiltrasjon)\".">
