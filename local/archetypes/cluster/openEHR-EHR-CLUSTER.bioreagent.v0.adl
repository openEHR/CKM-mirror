				>
				["at0001"] = <
					text = <"Navn">
					description = <"Identifisering av reagens eller forbruksvare, fortrinnsvis ved vanlig navn, ved formelt, fullt beskrivende navn, eller hvis påkrevet, ved klasse eller kategori.">
					comment = <"For eksempel: \"Løsning for immobilisering av spermier\".">
