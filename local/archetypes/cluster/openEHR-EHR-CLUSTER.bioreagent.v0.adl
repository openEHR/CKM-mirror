				>
				["at0006"] = <
					text = <"Batch/lot-nummer">
					description = <"Nummeret tildelt av produsenten som identifiserer en gruppe enheter som er produsert samtidig, som regel funnet på etiketten eller pakningsvedlegget.">
				>
				["at0007"] = <
					text = <"Utløpsdato">
					description = <"Datoen produktet ikke lenger er egnet for bruk. Datoen finnes vanligvis på produktet eller på pakningsvedlegget.">
					comment = <"Denne datoen gjelder vanligvis for engangsutstyr eller utstyr som ikke skal gjenbrukes.">
				>
				["at0008"] = <
					text = <"Kommentar">
					description = <"Fritekstopplysninger om bioreagens som ikke er fanget opp i andre felter.">
