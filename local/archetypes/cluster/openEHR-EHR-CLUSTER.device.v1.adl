					description = <"Détails supplémentaires sur les propriétés spécifiques du dispositif médical.">
				>
				["at0018"] = <
					text = <"Composants">
					description = <"Informations structurées supplémentaires sur les composants identifiés de l'appareil.">
				>
				["at0019"] = <
					text = <"Gestion des actifs">
					description = <"Plus de détails sur la gestion et l'entretien de l'appareil.">
					comment = <"Par exemple : Propriétaire, coordonnées, emplacement, adresse réseau, date de remplacement, détails d'étalonnage, etc.">
				>
				["at0020"] = <
					text = <"Numéro de série">
					description = <"Numéro attribué par le fabricant qui se trouve sur le dispositif et qui doit être spécifique à chaque dispositif, à son étiquette ou à l'emballage qui l'accompagne.">
