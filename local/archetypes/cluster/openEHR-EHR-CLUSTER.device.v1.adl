					text = <"Utgångsdatum">
					description = <"Datum från då den medicintekniska produkten inte längre är lämplig att användas, vanligtvis finns datumet på den medicintekniska produkten eller tryckt på den medföljande förpackningen.">
					comment = <"Detta datum gäller vanligtvis engångsprodukter.">
				>
				["at0008"] = <
					text = <"Kommentar">
					description = <"Kommentarer avseende den medicintekniska produkten som inte beskrivs i övriga fält.">
				>
				["at0009"] = <
					text = <"Egenskaper">
					description = <"Ytterligare information om specifika egenskaper för den medicintekniska produkten.">
				>
				["at0018"] = <
					text = <"Komponenter">
					description = <"Ytterligare strukturerad information om identifierade komponenter i den medicintekniska produkten.">
				>
				["at0019"] = <
					text = <"Tillgångshantering">
					description = <"Mer information om underhåll och hantering av den medicintekniska produkten.">
					comment = <"Till exempel: Ägare, kontaktuppgifter, plats, nätverksadress, datum för utbyte, kalibreringsinformation etcetera.">
				>
				["at0020"] = <
					text = <"Serienummer">
					description = <"Tilldelat nummer av tillverkaren av den medicintekniska produkten, bör vara specifik för varje medicinteknisk produkt, dess etikett eller medföljande förpackning.">
				>
				["at0021"] = <
					text = <"Unik produktidentifierare (UDI)">
					description = <"En numerisk eller alfanumerisk sträng som är associerad med den medicintekniska produkten i ett givet system.">
					comment = <"Ofta fäst på den medicintekniska produkten som en streckkod.">
				>
				["at0022"] = <
					text = <"Katalognummer">
					description = <"Det exakta nummer som tilldelats av tillverkaren som visas i tillverkarens katalog, produktens märkning eller medföljande förpackning.">
				>
				["at0023"] = <
					text = <"Modellnummer">
					description = <"Det exakta modellnummer som tilldelats av tillverkaren och som finns på den medicintekniska produktens etikett eller medföljande förpackning.">
				>
				["at0024"] = <
					text = <"Annan identifierare">
					description = <"Ospecificerad identifierare, som kan specificeras ytterligare i en template eller kan genereras automatiskt av vissa IT-system.">
					comment = <"Kodning av namnet på identifieraren med ett kodverk är önskvärt, om det finns för området.">
