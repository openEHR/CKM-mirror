ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Physical properties">
					description = <"The measurements of an anatomical structure or object that is part of, found in or originates from an individual.">
				>
				["at0020"] = <
					text = <"Weight">
					description = <"The measured value of the mass of a structure/object.">
				>
				["at0046"] = <
					text = <"Volume">
					description = <"The measured or calculated space enclosed by a boundary (liquid) or occupied by a structure/object (solid tissue).">
				>
				["at0049"] = <
					text = <"Length">
					description = <"The measured length of the longest aspect of a structure/object (e.g. longest edge of a rectangular specimen) or the measured length of a structure/object along its longitudinal axis (e.g. 2cm of bowel, measured along the axis of the bowel, and even if the measured length is shorter than the diameter of the bowel itself).">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements.

If there is no clear orientation of for example length vs width vs height, use multiple occurrences to allow recording of the length of two or more axes without any specific orientation, for example 3 x 4 x 5 mm.">
				>
				["at0050"] = <
					text = <"Width">
					description = <"The measured length of widest aspect of a structure/object, usually perpendicular to the length or the measured length of the second side of two sides.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the widest and narrowest part of an abnormal mass.">
				>
				["at0052"] = <
					text = <"Diameter">
					description = <"The measured length of a structure/object from edge to edge, usually of an approximately round or oval structure/object, passing through its centre.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements.

If there are no clear orientations, use multiple occurrences to record the diameter along two or more axes without a specific orientation, for example, 3 x 4 mm.">
				>
				["at0053"] = <
					text = <"Depth">
					description = <"The measured distance of the horizontal aspect of a structure/object, from the top to the bottom.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the deepest and shallowest part of a skin lesion, or the maximum and minimum depth of an internal tumor in soft tissue.">
				>
				["at0054"] = <
					text = <"Height">
					description = <"The measured distance of the vertical aspect of a structure/object, from the base to the top.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the maximum and minimum height of a tumor above the skin surface.">
				>
				["at0055"] = <
					text = <"Perimeter">
					description = <"The total length of all external surfaces of a square, rectangular or irregularly shaped structure/object.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the inner and outer perimeter of a scar or uneven skin change.">
				>
				["at0080"] = <
					text = <"Radius">
					description = <"The measured length of a structure/object in a straight line from the centre to the circumference.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the maximum and minimum radius of a cyst or nodule, measured from the center to the edge.">
				>
				["at0081"] = <
					text = <"Area">
					description = <"The calculated space of a structure/object in two dimensions.">
				>
				["at0082"] = <
					text = <"Thickness">
					description = <"The measured distance from one surface to the opposite surface of a structure/object.">
					comment = <"This data element has 0..* occurrences to allow recording of multiple measurements. For example, the maximum and minimum thickness of a cyst wall.">
				>
				["at0083"] = <
					text = <"Circumference">
					description = <"The total length of the boundary around a round or elliptical structure/object.">
					comment = <"For example: the circumterence of a gallbladder or colon.

This data element has 0..* occurrences to allow recording of multiple measurements. For example, the inner and outer circumference of a cyst or nodule measured at different locations.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Fysiske egenskaper">
					description = <"Målinger av en anatomisk struktur eller et objekt som er en del av, funnet i eller stammer fra et individ.">
				>
				["at0020"] = <
					text = <"Vekt">
					description = <"Den målte verdien av massen til strukturen/objektet.">
				>
				["at0046"] = <
					text = <"Volum">
					description = <"Den målte eller det beregnede rom innenfor en grense (for væske) eller hvor stort rom en struktur/objekt tar (for fast materiale).">
				>
				["at0049"] = <
					text = <"Lengde">
					description = <"Den målte lengden på den lengste aspekten av strukturen/objektet (f.eks. lengste kant på et rektangulært prøve) eller den målte lengden på en strukturen/objektet langs dens longitudinale akse (f.eks. 2 cm tarm, målt langs tarmens akse, selv om den målte lengden er kortere enn diameteren på tarmen selv).">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger.

Hvis det ikke er en tydelig orientering, for eksempel lengde vs bredde vs høyde, bruk flere forekomster for å tillate registrering av lengden på to eller flere akser uten noen spesifikk orientering, for eksempel 3 x 4 x 5 mm.">
				>
				["at0050"] = <
					text = <"Bredde">
					description = <"Den målte lengden på den bredeste aspekten av strukturen/objektet, vanligvis vinkelrett på lengden, eller den målte lengden på den andre siden av de to sidene.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den bredeste og smaleste delen av en unormal masse.">
				>
				["at0052"] = <
					text = <"Diameter">
					description = <"Den målte lengden av en struktur/objekt fra kant til kant, vanligvis av en omtrent rund eller oval struktur/objekt, som går gjennom sentrum.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger.

Hvis det ikke er noen klare orienteringer, bruk flere forekomster for å registrere diameteren langs to eller flere akser uten spesifikk orientering, for eksempel 3 x 4 mm.">
				>
				["at0053"] = <
					text = <"Dybde">
					description = <"Den målte avstanden av den horisontale aspekten av en struktur/objekt, fra topp til bunn.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den dypeste og grunneste delen av en hudlesjon, eller den maksimale og minimale dybden av en indre svulst i bløtvev-">
				>
				["at0054"] = <
					text = <"Høyde">
					description = <"Den målte avstanden av den vertikale aspekten av en struktur/objekt, fra bunnen til toppen.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den maksimale og minimale høyden på en svulst over hudoverflaten.">
				>
				["at0055"] = <
					text = <"Omkrets (geometrisk)">
					description = <"Den totale lengden av alle eksterne overflater av en firkantet, rektangulær eller uregelmessig formet struktur/objekt.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den indre og ytre omkretsen av et arr eller en ujevn hudforandring.">
				>
				["at0080"] = <
					text = <"Radius">
					description = <"Den målte lengden på strukturen/objektet i en rett linje fra sentrum til omkretsen.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den minste og største radiusen til en cyste eller knute, målt fra sentrum til kanten.">
				>
				["at0081"] = <
					text = <"Areal">
					description = <"Det beregnede arealet av strukturen/objektet i to dimensjoner.">
				>
				["at0082"] = <
					text = <"Tykkelse">
					description = <"Den målte avstanden fra en overflate til den motsatte overflaten av en struktur/objekt.">
					comment = <"Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den maksimale og minimale tykkelsen på en cystevegg.">
				>
				["at0083"] = <
					text = <"Omkrets (sirkulær)">
					description = <"Den totale lengden av grensen rundt en rund eller elliptisk struktur/objekt.">
					comment = <"For eksempel: omkretsen av en galleblære eller tykktarm.

Dette dataelementet har 0..* forekomster for å tillate registrering av flere målinger. For eksempel den indre og ytre omkretsen av en cyste eller knute målt på forskjellige steder.">
				>
			>
		>
	>
