										magnitude = <|>=0.0|>
									>
									["3"] = <
										units = <"mg">
										magnitude = <|>=0.0|>
									>
									["4"] = <
										units = <"g">
										magnitude = <|>=0.0|>
									>
									["5"] = <
										units = <"kg">
										magnitude = <|>=0.0|>
									>
