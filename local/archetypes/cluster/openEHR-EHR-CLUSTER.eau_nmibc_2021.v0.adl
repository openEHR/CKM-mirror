	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Å stratifisere pasienter med ikke-muskelinvasiv blærekreft (NMIBC) i prognostiske risikogrupper ved bruk av EAU 2021-modellen basert på WHO 2004/2016- eller WHO 1973-graderingssystemer, for å støtte klinisk beslutningstaking angående behandling etter transuretral reseksjon av blæresvulst (TURBT).">
			use = <"*Use to record the prognostic risk group classification of patients with non–muscle-invasive bladder cancer (NMIBC) according to the European Association of Urology (EAU) 2021 model, based on the WHO 2004/2016 or WHO 1973 grading systems. This supports structured documentation and enables clinical decision-making following transurethral resection of bladder tumour (TURBT). (en)">
			keywords = <"EAU", "NMIBC", "blærekreft", "risikostratifisering", "TURBT", "WHO", "2004/2016", "WHO", "1973", "ikke-muskelinvasiv", "urotelialt", "karsinom", "prognostisk", "gruppe", "behandlingsplanlegging.">
			misuse = <"">
			copyright = <"© openEHR Foundation">
		>
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To stratify patients with non–muscle-invasive bladder cancer (NMIBC) into prognostic risk groups using the EAU 2021 model based on WHO 2004/2016 or WHO 1973 grading systems, to support clinical decision-making regarding treatment following transurethral resection of bladder tumor (TURBT).">
			use = <"Use to record the prognostic risk group classification of patients with non–muscle-invasive bladder cancer (NMIBC) according to the European Association of Urology (EAU) 2021 model, based on the WHO 2004/2016 or WHO 1973 grading systems. This supports structured documentation and enables clinical decision-making following transurethral resection of bladder tumour (TURBT).">
			keywords = <"EAU", "NMIBC", "bladder", "cancer", "risk", "stratification", "TURBT", "WHO", "2004/2016", "WHO", "1973", "non–muscle-invasive", "urothelial", "carcinoma", "prognostic", "group", "treatment", "planning.">
