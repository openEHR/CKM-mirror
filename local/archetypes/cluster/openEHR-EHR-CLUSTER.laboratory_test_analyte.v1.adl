					hl7v2_mapping = <"NTE.3">
					fhir_mapping = <"Observation.note">
				>
				["at0004"] = <
					text = <"Rangs de referència">
					description = <"Consells addicionals sobre l'aplicabilitat del rang de referència a aquest resultat, o bé es pot incloure un text o una guia textual codificada sobre si el resultat és dins del rang normal.">
