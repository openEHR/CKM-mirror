					text = <"Registrat per error">
					description = <"El resultat de la prova ha estat retirat després d'haver-se publicat.">
				>
				["at0023"] = <
					text = <"Cancel·lat">
					description = <"El resultat no està disponible perquè la prova no es va iniciar o no es va completar (de vegades també s'anomena “anul·lada”).">
				>
