definition
	COMPOSITION[at0000.1] matches {    -- Clinical investigation report
		category matches {
			DV_CODED_TEXT matches {
				defining_code matches {
					[openehr::433]
				}
			}
		}
		context matches {
			EVENT_CONTEXT matches {
				other_context matches {
					ITEM_TREE[at0001] matches {    -- Tree
						items cardinality matches {0..*; unordered} matches {
							ELEMENT[at0002] occurrences matches {0..1} matches {    -- Report ID
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Status
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Extension
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0.1] occurrences matches {0..1} matches {    -- Index item
								value matches {
									DV_TEXT matches {*}
								}
								name matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0.10,    -- Index infectious disease
											at0.11]    -- Index health event
										}
									}
								}
							}
							ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Report type
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Date of notification
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0.4] occurrences matches {0..1} matches {    -- Date investigation initiated
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							allow_archetype CLUSTER[at0.5] occurrences matches {0..*} matches {    -- Investigator
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
							}
							allow_archetype CLUSTER[at0.6] occurrences matches {0..*} matches {    -- Investigation location
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.address(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0.21] occurrences matches {0..1} matches {    -- Information source
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.18] occurrences matches {0..1} matches {    -- Health region
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.7] occurrences matches {0..1} matches {    -- Date report completed
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							CLUSTER[at0.12] occurrences matches {0..*} matches {    -- Reviewed by
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0.8] occurrences matches {0..*} matches {    -- Review type
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0.14] occurrences matches {0..*} matches {    -- Reviewer
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0.13] occurrences matches {0..1} matches {    -- Reviewer role
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0.15] occurrences matches {0..1} matches {    -- Report comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0.16] occurrences matches {0..1} matches {    -- Date review completed
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0.20] occurrences matches {0..1} matches {    -- Date review submitted
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
								}
							}
							ELEMENT[at0.9] occurrences matches {0..1} matches {    -- Date report submitted
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["ar-sy"] = <
			items = <
				["at0000.1"] = <
					text = <"*Clinical investigation report (en)">
					description = <"*Container archetype to carry information about a clinical investigation. (en)">
				>
				["at0.1"] = <
					text = <"*Index item (en)">
					description = <"*The name or type of the health item under investigation. (en)">
				>
				["at0.2"] = <
					text = <"*Report type (en)">
					description = <"*The type of report. (en)">
					comment = <"*For example: hospital investigation; or community investigation. (en)">
				>
				["at0.3"] = <
					text = <"*Date of notification (en)">
					description = <"*The date when the public health authority received the infectious disease notification. (en)">
				>
				["at0.4"] = <
					text = <"*Date investigation initiated (en)">
					description = <"*The date when the investigation was assigned to an investigator. (en)">
				>
				["at0.5"] = <
					text = <"*Investigator (en)">
					description = <"*Details about the individual(s) assigned to investigate the disease notification. (en)">
				>
				["at0.7"] = <
					text = <"*Date report completed (en)">
					description = <"*The date when the investigation report was completed. (en)">
				>
				["at0.6"] = <
					text = <"*Investigation location (en)">
					description = <"*The place where the investigation was carried out. (en)">
					comment = <"*For example: the name of a facility or institution; municipality; or a physical address. (en)">
				>
				["at0.10"] = <
					text = <"*Index infectious disease (en)">
					description = <"*Name of the infectious disease under investigation. (en)">
				>
				["at0.11"] = <
					text = <"*Index health event (en)">
					description = <"*Type of health event under investigation. (en)">
				>
				["at0.8"] = <
					text = <"*Review type (en)">
					description = <"*Identification of the category or level of review. (en)">
					comment = <"*For example: review by the facility or institution where the investigation was carried out; the local regional authority; or the national authority. (en)">
				>
				["at0.9"] = <
					text = <"*Date report submitted (en)">
					description = <"*Date when the final report was submitted to the final health authority. (en)">
				>
				["at0.12"] = <
					text = <"*Reviewed by (en)">
					description = <"*Details about the review process and reviewers of the report. (en)">
				>
				["at0.13"] = <
					text = <"*Reviewer role (en)">
					description = <"*The formal role or position of the reviewer. (en)">
					comment = <"*For example: regional epidemiologist; or Ministry of Health. (en)">
				>
				["at0.14"] = <
					text = <"*Reviewer (en)">
					description = <"*Structured details about the individual or organisation who reviewed the report. (en)">
				>
				["at0.15"] = <
					text = <"*Report comment (en)">
					description = <"*Narrative response to the content of the report by the reviewer. (en)">
				>
				["at0.16"] = <
					text = <"*Date review completed (en)">
					description = <"*Date when the review of the report was completed. (en)">
				>
				["at0.18"] = <
					text = <"*Health region (en)">
					description = <"*Identification of the local health region where the index case was identified or the index event happened. (en)">
					comment = <"*This data element supports reporting by a jurisdictional grouping rather than a specific facility or institution, place or physical address. This data element may be able to be derived from the precise recording of 'Investigation site'. (en)">
				>
				["at0.20"] = <
					text = <"*Date review submitted (en)">
					description = <"*Date when the review was submitted to the next level of review. (en)">
				>
				["at0.21"] = <
					text = <"*Information source (en)">
					description = <"*Description about one or more source types for the information contained in the report. (en)">
					comment = <"*For example: 'hospital record'; or 'interview with family'. (en)">
				>
				["at0000"] = <
					text = <"تقرير">
					description = <"وثيقة لتوصيل المعلومات للآخرين, عادة كاستجابة لطلب من طرف آخر.">
				>
				["at0001"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0002"] = <
					text = <"العنصر التعريفي الفريد للتقرير">
					description = <"معلومات التعريف حول التقرير">
				>
				["at0005"] = <
					text = <"الحالة">
					description = <"حالة التقرير بشكل كلي. و لا تمثل هذه الحالة جزءا من التقرير و إنما جميعه ككل.">
				>
				["at0006"] = <
					text = <"*Extension(en)">
					description = <"*Additional information required to capture local context or to align with other reference models/formalisms.(en)">
					comment = <"* e.g. Local hospital departmental infomation or additional metadata to align with FHIR or CIMI equivalents.(en)">
				>
			>
		>
