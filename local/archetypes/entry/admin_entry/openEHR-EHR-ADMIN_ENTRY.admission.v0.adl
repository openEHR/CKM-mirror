definition
	ADMIN_ENTRY[at0000] matches {	-- Admissão do paciente
		data matches {
			ITEM_TREE[at0001] matches {	-- *Tree(en)
				items cardinality matches {2..*; unordered} matches {
					ELEMENT[at0002] matches {	-- Classificação do paciente
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003, 	-- Internação/ paciente com pernoite
									at0004, 	-- Day hospital
									at0005, 	-- Ambulatorial
									at0006, 	-- Urgência
									at0007, 	-- Público
									at0008, 	-- Pré-admissão
									at0009, 	-- Conta comercial
									at0010, 	-- Não aplicável
									at0011]	-- Desconhecido
								}
							}
						}
					}
					CLUSTER[at0073] occurrences matches {0..1} matches {	-- Localização na internação (leito)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0074] occurrences matches {0..1} matches {	-- Unidade de cuidado
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0077] occurrences matches {0..1} matches {	-- Ala
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0078] occurrences matches {0..1} matches {	-- Quarto
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0079] occurrences matches {0..1} matches {	-- Leito
								value matches {
									DV_TEXT matches {*}
								}
							}
							CLUSTER[at0104] occurrences matches {0..1} matches {	-- Instalação
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0105] occurrences matches {0..1} matches {	-- ID do local
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0106] occurrences matches {0..1} matches {	-- ID Universal
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0107] occurrences matches {0..1} matches {	-- Tipo do ID Universal
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0108, 	-- DNS
													at0109, 	-- GUID
													at0110, 	-- HCD
													at0111, 	-- HL7
													at0112, 	-- ISO
													at0113, 	-- L, M, N
													at0114, 	-- Random
													at0115, 	-- UUID
													at0116, 	-- x400
													at0118]	-- x500
												}
											}
										}
									}
								}
							}
							ELEMENT[at0101] occurrences matches {0..1} matches {	-- Prédio
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0102] occurrences matches {0..1} matches {	-- Andar
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0103] occurrences matches {0..1} matches {	-- Descrição da localização
								value matches {
									DV_TEXT matches {*}
								}
							}
							CLUSTER[at0084] occurrences matches {0..1} matches {	-- Endereço
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0085] occurrences matches {0..1} matches {	-- Logradouro
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0086] occurrences matches {0..1} matches {	-- Cidade
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0087] occurrences matches {0..1} matches {	-- Estado
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0088] occurrences matches {0..1} matches {	-- CEP
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0089] occurrences matches {0..1} matches {	-- País
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {	-- Tipo de localização
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0082, 	-- Clínica
											at0090, 	-- Casa
											at0091, 	-- Departamento
											at0092, 	-- Unidade de enfermagem
											at0093]	-- *Provider's office(en)
										}
									}
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Tipo de Admissão
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0014, 	-- Acidente
									at0015, 	-- Emergência
									at0016, 	-- Trabalho de parto
									at0017, 	-- Eletiva
									at0018, 	-- Recém nascido
									at0019, 	-- Urgência
									at0020, 	-- Eletivo
									at0021, 	-- Admissão em repouso geriátrico
									at0022]	-- Admissão estatística
								}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- Número da pré-admissão
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0094] occurrences matches {0..1} matches {	-- Localização prévia do paciente
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0119] occurrences matches {0..1} matches {	-- Instalação
								items cardinality matches {1..*; unordered} matches {
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0104]/items[at0105]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Instalação]/items[ID do local]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0104]/items[at0106]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Instalação]/items[ID Universal]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0104]/items[at0107]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Instalação]/items[Tipo do ID Universal]
								}
							}
							CLUSTER[at0095] occurrences matches {0..1} matches {	-- Endereço
								items cardinality matches {1..*; unordered} matches {
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0084]/items[at0085]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Endereço]/items[Logradouro]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0084]/items[at0086]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Endereço]/items[Cidade]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0084]/items[at0087]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Endereço]/items[Estado]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0084]/items[at0088]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Endereço]/items[CEP]
									use_node ELEMENT /data[at0001]/items[at0073]/items[at0084]/items[at0089]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Endereço]/items[País]
								}
							}
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0074]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Unidade de cuidado]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0077]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Ala]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0078]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Quarto]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0079]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Leito]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0101]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Prédio]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0102]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Andar]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0103]	-- /data[*Tree(en)]/items[Localização na internação (leito)]/items[Descrição da localização]
						}
					}
					CLUSTER[at0098] occurrences matches {0..*} matches {	-- Médico do atendimento
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0099] occurrences matches {0..1} matches {	-- ID
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0100] occurrences matches {0..1} matches {	-- Nome
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0120] occurrences matches {0..1} matches {	-- Sobrenome
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0025] occurrences matches {0..1} matches {	-- Médico solicitante
						items cardinality matches {1..*; unordered} matches {
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0099]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[ID]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0100]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Nome]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0120]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Sobrenome]
						}
					}
					CLUSTER[at0121] occurrences matches {0..*} matches {	-- Médico especialista
						items cardinality matches {1..*; unordered} matches {
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0099]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[ID]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0100]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Nome]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0120]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Sobrenome]
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Unidade hospitalar
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {	-- Fonte da admissão
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0051] occurrences matches {0..1} matches {	-- Médico da admissão
						items cardinality matches {1..*; unordered} matches {
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0099]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[ID]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0100]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Nome]
							use_node ELEMENT /data[at0001]/items[at0098]/items[at0120]	-- /data[*Tree(en)]/items[Médico do atendimento]/items[Sobrenome]
						}
					}
					ELEMENT[at0061] occurrences matches {0..1} matches {	-- Tipo de financiamento
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0062, 	-- *Australian Health Care Agreements(en)
									at0063, 	-- Seguros de saúde privados
									at0064, 	-- Auto financiado
									at0065, 	-- Auxilio doença
									at0122, 	-- Reclamação de terceiro sobre veículo motorizado
									at0123, 	-- Outra compensação
									at0124, 	-- *Department of Veterans' Affairs(en)
									at0125, 	-- *Department of Defence(en)
									at0126, 	-- *Correctional facility(en)
									at0127, 	-- Outro hospital ou instituição pública (cuidados contratados)
									at0128, 	-- Acordos de saúde recíprocos (com outros países)
									at0129, 	-- Outro
									at0130]	-- Desconhecido
								}
							}
						}
					}
					ELEMENT[at0066] occurrences matches {0..1} matches {	-- Indicação de alteração de preço
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}		-- Qualquer termo que é Indicador de alteração de preço.
							}
						}
					}
					ELEMENT[at0071] matches {	-- Data/hora admissão
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


