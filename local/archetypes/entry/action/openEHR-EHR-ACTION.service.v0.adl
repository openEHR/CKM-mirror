					text = <"Details over dienst">
					description = <"Gestructureerde informatie over de dienst.">
					comment = <"Voor het vastleggen van gedetailleerde, gestructureerde informatie over specifieke aspecten van de dienst.">
				>
				["at0028"] = <
					text = <"Opmerking">
					description = <"Aanvullende informatie over de activiteit of zorgtrajectstap die niet in andere velden is opgenomen.">
				>
				["at0029"] = <
					text = <"*Multimedia representation (en)">
					description = <"*Digital image, video, diagram or other media representing the service. (en)">
				>
				["at0031"] = <
					text = <"*Extension (en)">
