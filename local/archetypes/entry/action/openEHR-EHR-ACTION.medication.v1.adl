				["at0154"] = <
					text = <"重新开始日期/时间">
					description = <"根据“已暂停的施用/给药”（Administrations suspended）路径步骤，设置重新开始药物治疗过程的日期/时间。">
					comment = <"例如：2017-10-29">
				>
