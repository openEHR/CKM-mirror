					description = <"*Structured information about the procedure. Use to capture detailed, structured information about anatomical location, method & technique, equipment used, devices implanted, results, findings etc.(en)">
				>
				["at0004"] = <
					text = <"*Procedure planned (en)">
					description = <"*">
