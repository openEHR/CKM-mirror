					text = <"Antragsteller">
					description = <"Angaben über den Gesundheitsdienstleister oder die Organisation, die die Dienstleistung anfordert.">
				>
				["at0018"] = <
					text = <"Service Provider Identifier">
					description = <"Die ID, die dem Auftrag vom Dienstleister zugewiesen wird, der die Dienstleistung erfüllt.">
					comment = <"Dies entspricht dem \"Filler Identifier\" in FHIR.">
				>
				["at0019"] = <
					text = <"Empfänger">
