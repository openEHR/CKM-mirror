			ITEM_LIST[at0009] matches {	-- List
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- Product
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

