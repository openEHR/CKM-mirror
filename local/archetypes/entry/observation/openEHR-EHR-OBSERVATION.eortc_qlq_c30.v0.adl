		protocol matches {
			ITEM_TREE[at0161] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0162] occurrences matches {0..1} matches {    -- Scoring method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0163,    -- Raw score
									at0164]    -- Linear transformation
								}
							}
						}
					}
					allow_archetype CLUSTER[at0165] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}




