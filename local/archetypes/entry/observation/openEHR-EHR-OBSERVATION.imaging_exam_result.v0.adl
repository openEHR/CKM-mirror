		protocol matches {
			ITEM_TREE[at0025] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Technique
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Structured technique
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0057] occurrences matches {0..1} matches {    -- Imaging quality
						value matches {
							DV_TEXT matches {*}
