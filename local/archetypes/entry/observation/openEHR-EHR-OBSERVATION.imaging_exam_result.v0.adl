								}
							}
							allow_archetype CLUSTER[at0067] occurrences matches {0..*} matches {    -- Comparison series details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {    -- Examination request details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Receiver order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_CODED_TEXT matches {*}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Requester order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0029] occurrences matches {0..*} matches {    -- Examination requested name
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0030] occurrences matches {0..*} matches {    -- Requester
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					allow_archetype CLUSTER[at0083] occurrences matches {0..1} matches {    -- Distribution list
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Imaging examination result">
					description = <"The result of an imaging examination performed on an individual, using radiological techniques.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Study name">
					description = <"The name of the imaging examination performed.">
					comment = <"For example: 'CT abdomen'; 'Doppler ultrasound of renal artery' or 'Plain chest xray'. Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Examples of candidate terminologies are the LOINC Imaging Document Codes, RadLex Playbook or SNOMED-CT. Equivalent to DiagnosticReport.code in FHIR.">
				>
				["at0006"] = <
					text = <"Structured target body site">
					description = <"Structured detail about the body site or region targetted for imaging.">
					comment = <"If the body site is specified within the text or code for 'Study name', then this field may be redundant.">
				>
				["at0008"] = <
					text = <"Imaging findings">
					description = <"Narrative description or overview of all clinical findings.">
				>
				["at0014"] = <
					text = <"Clinical summary">
					description = <"Narrative description of relevant clinical history that provides context for the examination and interpretation of results.">
					comment = <"Alternatively this data element can include an RM link to clinical information provided in the original examination request or located elsewhere within the health record.">
				>
				["at0020"] = <
					text = <"Imaging diagnosis">
					description = <"Single word, phrase or brief description representing the likely condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging differential diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" Equivalent to DiagnosticReport.conclusionCode in FHIR.">
				>
				["at0021"] = <
					text = <"Overall impression">
					description = <"Narrative concise, clinically relevant interpretation of all imaging findings, and include a comparison with previous studies where appropriate.">
					comment = <"Also referred to as 'Opinion' or 'Conclusion'. Equivalent to DiagnosticReport.conclusion in FHIR.">
				>
				["at0023"] = <
					text = <"Comment">
					description = <"Additional narrative about the examination not captured in other fields.">
					comment = <"For example: a note that the film was given to the patient. Equivalent to DiagnosticReport.note in FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Imaging service">
					description = <"Details about the service, organisation or individual carrying out the imaging examination.">
				>
				["at0027"] = <
					text = <"Examination request details">
					description = <"Details about a single imaging examination requested.">
				>
				["at0028"] = <
					text = <"Requester order identifier">
					description = <"Unique identifier for the imaging examination order assigned by the requester.">
					comment = <"Equivalent to the HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Examination requested name">
					description = <"Identification of imaging examination requested.">
					comment = <"This can be useful in situations where the examination requested differs from the examination performed.">
				>
				["at0030"] = <
					text = <"Requester">
					description = <"Details about the clinician and/or organisation requesting the imaging examination.">
					comment = <"Equivalent to DICOM Referring Physician's Name (0008,0090)">
				>
				["at0031"] = <
					text = <"Receiver order identifier">
					description = <"Unique identifier for the imaging examination order assigned by the radiology service.">
					comment = <"Equivalent to the HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050).">
				>
				["at0041"] = <
					text = <"Structured technique/procedure">
					description = <"Additional structured details of technical and clinical aspects of capturing the image/s.">
				>
				["at0044"] = <
					text = <"Structured imaging findings">
					description = <"Structured details about the imaging examination findings targeting a specific structure or region.">
				>
				["at0045"] = <
					text = <"Report representation">
					description = <"Digital representation of the examination result.">
					comment = <"Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report. Equivalent to DiagnosticReport.presentedForm in FHIR.">
				>
				["at0046"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Confounding factors">
					description = <"Narrative description of factors, not recorded elsewhere, that may influence the examination findings and/or result.">
				>
				["at0049"] = <
					text = <"Technique summary">
					description = <"Narrative description about the radiological procedure used to capture the study.">
					comment = <"Use this data element to describe a summary of technique, including technical limitations or deviation from the normal technical procedures. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.'">
				>
				["at0055"] = <
					text = <"Target body site">
					description = <"Description of the simple body site or region targetted for imaging.">
					comment = <"If the body site is specified within the text or code for 'Study name', then this field may be redundant. Coding of 'Target body site' with a terminology, such as SNOMED CT, is desirable. Equivalent to ImagingStudy.series.bodySite in FHIR.">
				>
				["at0056"] = <
					text = <"Comparison findings">
					description = <"Narrative description about the comparison of this examination with previous similar studies.">
					comment = <"Also use this data element to indicate if no comparison images and/or reports are available. Details about the images used for comparison can be recorded using the 'Comparison study details' CLUSTER, within Protocol.">
				>
				["at0057"] = <
					text = <"Imaging quality description">
					description = <"Narrative description about the quality of the examination.">
					comment = <"For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate.">
				>
				["at0058"] = <
					text = <"Imaging differential diagnosis">
					description = <"Single word, phrase or brief description representing a possible condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\"">
				>
				["at0059"] = <
					text = <"Recommendation">
					description = <"Suggestion for further imaging, investigations and/or referral, and associated rationale.">
					comment = <"This data element has 0..* occurrences to allow for more than one recommendation. Formal orders for additional imaging examination, investigation should be recorded using an INSTRUCTION archetype, such as INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Clinical indication">
					description = <"Narrative description about the reason the examination was originally requested.">
					comment = <"Also known as 'Clinical question' or 'Clinical query'.">
				>
				["at0062"] = <
					text = <"Procedure summary">
					description = <"Narrative description about the clinical procedure or other clinical considerations used to capture the study.">
					comment = <"Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered. ">
				>
				["at0063"] = <
					text = <"Imaging quality">
					description = <"Assessment about the quality of the examination.">
					comment = <"Coding of the imaging quality with a value set is recommended. For example: 'excellent, good, fair, poor, and non-diagnostic'; or 'adequate, suboptimal and inadequate'.
">
				>
				["at0065"] = <
					text = <"Series details">
					description = <"Details about a series included in this report.">
				>
				["at0067"] = <
					text = <"Comparison series details">
					description = <"Details about an imaging series being compared to the reported study.">
				>
				["at0069"] = <
					text = <"Device">
					description = <"Details about imaging device/s used to capture the study.">
				>
				["at0070"] = <
					text = <"Study date">
					description = <"Date/time when the imaging started.">
					comment = <"Equivalent to ImagingStudy.started in FHIR.">
				>
				["at0071"] = <
					text = <"Status timestamp">
					description = <"The date and/or time that ‘Overall result status’ was assigned.">
				>
				["at0072"] = <
					text = <"Overall result status">
					description = <"The status of the imaging examination result as a whole.">
					comment = <"The values have been aligned with the FHIR Diagnostic report status, with the exception of 'Entered in error' which is relevant only to a messaging-oriented paradigm. Other local codes/terms can be used via the Text 'choice'. In most use cases, only one status is applicable at any time, however this data element allows multiple occurrences for the use cases where there are a variety of types of status that need to be specified, usually within a template. Equivalent to DiagnosticReport.status in FHIR. ">
				>
				["at0073"] = <
					text = <"Registered">
					description = <"The existence of the study is registered, but no result is yet available.">
				>
				["at0074"] = <
					text = <"Partial">
					description = <"This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified.">
				>
				["at0075"] = <
					text = <"Preliminary">
					description = <"Verified early results are available, but not all results are final. This is a sub-category of 'Partial'.">
				>
				["at0076"] = <
					text = <"Final">
					description = <"The result is complete and verified by an authorised person.">
				>
				["at0077"] = <
					text = <"Amended">
					description = <"Subsequent to being final, the result has been modified.">
				>
				["at0078"] = <
					text = <"Corrected">
					description = <"Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'.">
				>
				["at0079"] = <
					text = <"Appended">
					description = <"Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'.">
				>
				["at0080"] = <
					text = <"Cancelled">
					description = <"The result is unavailable because the test was not started or not completed (also sometimes called 'aborted').">
				>
				["at0083"] = <
					text = <"Distribution list">
					description = <"Details of additional clinicians or organisations who require a copy of the examination result.">
					comment = <"The 'Distribution list' is for information-only, and that the primary recipient of the report is the person intended to act on the information.">
				>
				["at0087"] = <
					text = <"Technique">
					description = <"Name of the radiological procedure used to capture the study.">
					comment = <"Coding of the 'Technique' with a terminology is permitted. Multiple occurrences allow for identification of one or more techniques.">
				>
				["at0088"] = <
					text = <"Procedure">
					description = <"Identification of the clinical procedure or management used to capture the study.">
				>
				["at0090"] = <
					text = <"Unknown">
					description = <"The status of the result is not known.">
				>
				["at0091"] = <
					text = <"Modality">
					description = <"The type of imaging device, process or method that originally acquired or produced the data used to create the images in the study.">
					comment = <"For example: 'XRay', 'CT scan', 'ultrasound', 'MRI', 'bone densitometry', 'tomography', 'mammography', 'nuclear medicine', 'PET', 'angiography', and 'fluoroscopy'. This data element may be redundant if the modality is clearly represented in the 'Study name'. Alternatively it may also be possible to autopopulate the 'Modality' from a coded 'Study name'. Coding of the 'Modality' with a value set is recommended, such as the DICOM CID 29 Acquisition Modality value set. Equivalent to ImagingStudy.modality in FHIR and Modality (0008, 0060) in DICOM.">
				>
				["at0092"] = <
					text = <"Study instance identifier">
					description = <"Unique identifier for the imaging study assigned by the imaging device.">
					comment = <"Equivalent to Study Instance UID (0020,000D) in DICOM and ImagingStudy.identifier in FHIR.">
				>
				["at0093"] = <
					text = <"Comparison study details">
					description = <"Details about images from a prior study used for comparison to the reported study.">
				>
				["at0094"] = <
					text = <"Study name">
					description = <"The name of the comparison imaging examination performed.">
				>
				["at0095"] = <
					text = <"Study identifier">
					description = <"Unique identifier for the comparison imaging study.">
				>
				["at0096"] = <
					text = <"Study date">
					description = <"Date and time the comparison examination started.">
				>
				["at0097"] = <
					text = <"Study end point">
					description = <"Digital location of the comparison imaging content and metadata.">
					comment = <"This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR.">
				>
				["at0098"] = <
					text = <"Study end point">
					description = <"Digital location of the imaging content and metadata.">
					comment = <"This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR.">
				>
				["at0099"] = <
					text = <"Study status">
					description = <"The current state of the imaging study.">
					comment = <"Equivalent to ImagingStudy.status in FHIR.">
				>
				["at0100"] = <
					text = <"Registered">
					description = <"The existence of the imaging study is registered, but there is nothing yet available.">
				>
				["at0101"] = <
					text = <"Available">
					description = <"At least one instance has been associated with this imaging study.">
				>
				["at0102"] = <
					text = <"Cancelled">
					description = <"The imaging study is unavailable because the study was not started or not completed.">
				>
				["at0103"] = <
					text = <"Unknown">
					description = <"The status is not known by the radiology source system.">
				>
				["at0104"] = <
					text = <"Report identifier">
					description = <"Unique identifier for the imaging report assigned by the radiology service.">
					comment = <"As one instance of this archetype is used per study, this identifier can be used to connect more than one imaging study within a single combined report. Equivalent to DiagnosticReport.identifier in FHIR. ">
				>
				["at0105"] = <
					text = <"Study description">
					description = <"Radiology service-generated description or classification of the study.">
					comment = <"Equivalent to ImagingStudy.description in FHIR and Study description (0008, 1030) in DICOM. Possible DICOM examples: MR BREAS, UNIT; NEURO^HEAD; or MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Image details">
					description = <"Link to full details of any imaging carried out during the study.">
					comment = <"Typically referencing images used the DICOM enabled modalities, but this data field provides an alternative method to access the images. Equivalent to DiagnosticReport.imagingStudy in FHIR.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Bildediagnostisk svar">
					description = <"Resultatet av en bildediagnostisk undersøkelse av et individ, og som er gjort ved radiologiske metoder.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Uspesifisert hendelse">
					description = <"Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i et templat eller i en applikasjon.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Undersøkelsesnavn">
					description = <"Navnet på den utførte bildediagnostiske undersøkelsen.">
					comment = <"For eksempel: \"CT abdomen\", \"Doppler ultralyd av nyrearterier\", \"Røntgen thorax\". Det anbefales sterkt å bruke en terminologi der det er mulig, fortrinnsvis en kode som både spesifiserer modalitet og anatomisk lokalisering. Mulige terminologier er NCRP, LOINC, SNOMED CT eller RadLex. Vanligvis tilsvarende FIHR DiagnosticReport.code.">
				>
				["at0006"] = <
					text = <"Strukturert kroppsdel">
					description = <"Strukturert detaljer om kroppsdelen som var målet for undersøkelsen.">
					comment = <"Hvis kroppsstedet er spesifisert av en kode i elementet \"Undersøkelsesnavn\", er dette feltet ikke nødvendig.">
				>
				["at0008"] = <
					text = <"Funn">
					description = <"Tekstlig beskrivelse eller oversikt over alle funnene.">
				>
				["at0014"] = <
					text = <"Kliniske opplysninger">
					description = <"Beskrivelse av relevant klinisk informasjon som er tilgjengelig på tolkningstidspunktet. Også kalt klinisk sammendrag.">
					comment = <"Alternativt kan dette elementet inkludere en lenke til klinisk informasjon som er gitt i den originale rekvisisjonen/henvisningen eller annet sted i den elektroniske pasientjournalen.">
				>
				["at0020"] = <
					text = <"Bildediagnostisk diagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en den sannsynlige tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn én diagnose. Det er anbefalt å bruke en terminologi dersom mulig, for eksempel SNOMED CT. Dette dataelementet bør oppfattes som gjensidig utelukkende til \"Bildediagnostisk differensialdiagnose\", altså bare enten \"Bildediagnostisk diagnose\" eller \"Bildediagnostisk differensialdiagnose\" i hver instans av arketypen \"Bildediagnostisk svar\". Tilsvarer DiagnosticReport.conclusionCode in FHIR.">
				>
				["at0021"] = <
					text = <"Resymé">
					description = <"Tekstlig presis og klinisk relevant tolkning av alle funn ved bildeundersøkelsen og inkluderer en sammenligning med tidligere granskninger dersom de er passende.">
					comment = <"Også referert til som \"Sammendrag\" eller \"Funn og undersøkelsesresultat\". Vanligvis tilsvarerende FHIR DiagnosticReport.conclusion.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligere kommentarer om undersøkelsen som ikke passer i andre felter.">
					comment = <"For eksempel et notat om at kopi av bilde eller svarrapport er gitt til pasienten. Tilsvarer DiagnosticReport.note i FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Tjenesteleverandør">
					description = <"Demografiske opplysninger om organisasjon eller person som utfører den bildediagnostiske undersøkelsen.">
				>
				["at0027"] = <
					text = <"Henvisningsdetaljer">
					description = <"Detaljer om en enkelt henvisning/rekvisisjon.">
				>
				["at0028"] = <
					text = <"Rekvirentens rekvisisjonsID">
					description = <"Rekvirentens/bestillers lokale ID for rekvisisjonen/henvisningen.">
					comment = <"Tilsvarende HL7 \"Placer Order Identifier\".">
				>
				["at0029"] = <
					text = <"Bestilt undersøkelsesnavn">
					description = <"Navn på bestilt undersøkelse.">
					comment = <"Denne kan være nyttig hvis den rekvirerte undersøkelse avviker fra undersøkelse som faktisk er utført.">
				>
				["at0030"] = <
					text = <"Rekvirent">
					description = <"Detaljer om den kliniker og/eller organisasjon som rekvirerer undersøkelsen.">
					comment = <"Tilsvarer DICOM Referring Physician's Name (0008,0090).">
				>
				["at0031"] = <
					text = <"Rekvisisjonsmottakers rekvisisjonsID">
					description = <"Henvisningens lokale identifikator/referansenummer i tjenesteyter/mottakers system, som regel tildelt av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050).">
				>
				["at0041"] = <
					text = <"Strukturert teknikk/prosedyre">
					description = <"Ytterligere strukturerte detaljer med tekniske eller kliniske aspekter knyttet til bildetakingen.">
				>
				["at0044"] = <
					text = <"Strukturerte funn">
					description = <"Strukturerte detaljer om funn ved bruk av bildeundersøkelse i spesifikke regioner eller strukturer i kroppen.">
				>
				["at0045"] = <
					text = <"Presentasjon av rapporten">
					description = <"Digital versjon av svarrapporten.">
					comment = <"Flere formater tillates, men innholdet i de forskjellige formatene må representere det samme innholdet. For eksempel en pdf versjon av rapporten. Tilsvarer DiagnosticReport.presentedForm i FHIR.">
				>
				["at0046"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0047"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0048"] = <
					text = <"Konfunderende faktorer">
					description = <"Tekstlig beskrivelse av faktorer som kan påvirke undersøkelsesfunn og/eller tolkning, og som ikke er registrert annet sted.">
				>
				["at0049"] = <
					text = <"Sammendrag av teknikk">
					description = <"Tekstlig beskrivelse av den radiologiske prosedyren som er brukt for å gjennomføre undersøkelsen.">
					comment = <"Bruk dette dataelementet for å gi en oversikt over teknikk, inkludert tekniske begrensninger eller avvik fra normal teknikk. For eksempel: \"5 mm aksiale bilder fra lungebasene gjennom skambensymfysen ble tatt etter administrering av intravenøs og oral kontrast. Koronale og Sagittale omformaterte bilder ble konstruert fra kildedataene.\"">
				>
				["at0055"] = <
					text = <"Kroppsdel">
					description = <"En beskrivelse av den anatomisk regionen, organet, eller organsystemet som var målet for undersøkelsen.">
					comment = <"Hvis målområde er spesifisert av en kode i undersøkelsesnavnet, er dette feltet ikke nødvendig. Det er anbefalt å kode \"Kroppsdel\" med en terminologi, som SNOMED CT. Kroppsdelen er vanligvis ekvivalent til FHIR ImagingStudy.series.bodySite.">
				>
				["at0056"] = <
					text = <"Funn ved sammenligning">
					description = <"Tekstlig beskrivelse med sammenligning mellom denne undersøkelsen og tidligere lignende undersøkelser.">
					comment = <"Bruk også dette dataelementet for å indikere dersom ingen sammenligningsbilder og/eller rapporter er tilgjengelige. Detaljer om bildene som er brukt for sammenligning kan lagres ved å bruke det interne Clusteret \"Detaljer om sammenlignende bilder\" i protokolldelen av denne arketypen.">
				>
				["at0057"] = <
					text = <"Bildediagnostisk kvalitetsbeskrivelse">
					description = <"Tekstlig beskrivelse av kvaliteten på undersøkelsen.">
					comment = <"For eksempel om det er begrensninger ved undersøkelsen og deres konsekvens for tolkning, anbefaling om å gjenta undersøkelsen eller om alternative undersøkelsesmetoder som kan være mer formålstjenlig.">
				>
				["at0058"] = <
					text = <"Bildediagnostisk differensialdiagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en mulig tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn én differensialdiagnose. Det er anbefalt å bruke en terminologi dersom mulig, for eksempel SNOMED CT. Dette dataelementet bør oppfattes som gjensidig utelukkende til \"Bildediagnostisk diagnose\", altså bare enten \"Bildediagnostisk diagnose\" eller \"Bildediagnostisk differensialdiagnose\" i hver instans av arketypen \"Bildediagnostisk svar\".">
				>
				["at0059"] = <
					text = <"Anbefaling">
					description = <"Forslag til videre bildediagnostiske undersøkelser, granskninger og/eller henvisninger, inkludert begrunnelser knyttet til dette.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn en anbefaling. Bruk en INSTRUCTION-arketype for formelle henvisninger for ytterligere bildeundersøkelser, for eksempel INSTRUCTION.service_request (Helsetjenesteforespørsel).">
				>
				["at0061"] = <
					text = <"Problemstilling">
					description = <"Tekstlig beskrivelse av begrunnelsen for henvisningen/rekvisisjonen.">
					comment = <"Også kjent som \"Begrunnelse for rekvisisjon/henvisning\".">
				>
				["at0062"] = <
					text = <"Sammendrag av prosedyre">
					description = <"Tekstlig sammendrag om den kliniske prosedyren eller andre kliniske vurderinger som er knyttet til den radiologiske undersøkelsen.">
					comment = <"Bruk dette dataelementet for å oppsummere den kliniske prosedyren, pasientens plassering (pasientposisjon), intervensjoner (som for eksempel Valsalva manøver) og administrering av kontrast, radiofarmakum og/eller behandlinger som del av bildeundersøkelsen. For eksempel: \"80 ml Isovue-370 ble administrert intravenøst ved bruk av en bolus-sporingsteknikk sentrert på hovedlungearterien\". Dataelementet kan også bli brukt til å registrere en oversikt over andre problemer, komplikasjoner eller overfølsomhetsreaksjoner som har oppstått.">
				>
				["at0063"] = <
					text = <"Bildediagnostisk kvalitet">
					description = <"Vurdering om kvaliteten på undersøkelsen.">
					comment = <"Det anbefales å kode undersøkelseskvalitet med et verdi- eller kodesett, for eksempel \"Utmerket\", \"God\", \"Bra\", \"Dårlig\" og \"Ikke diagnostiserbar\", eller \"Adekvat\", \"Suboptimal\" og \"Inadekvat\".">
				>
				["at0065"] = <
					text = <"Detaljer om bildeserie/r">
					description = <"Detaljer om en eller flere bildediagnostisk serier som er inkludert i denne resultatrapporten.">
				>
				["at0067"] = <
					text = <"Detaljer om sammenlignende serier">
					description = <"Detaljer om en serie av bildeundersøkelser som det sammenliknes med.">
				>
				["at0069"] = <
					text = <"Utstyr">
					description = <"Detaljer om utstyr eller apparat/-er som er brukt ved den bildediagnostiske undersøkelsen.">
				>
				["at0070"] = <
					text = <"Undersøkelsesdato">
					description = <"Dato/Tid da den bildedannende undersøkelsen startet.">
					comment = <"Tilsvarer FIHR 'ImagingStudy.started.">
				>
				["at0071"] = <
					text = <"Tidsangivelse for overordnet resultatstatus">
					description = <"Dato og/eller tid \"Overordnet resultatstatus\" ble gitt. (Tidspunkt for siste statusendring).">
				>
				["at0072"] = <
					text = <"Status på undersøkelsesresultatet">
					description = <"Den overordnede statusen for hele undersøkelsesresultatet.">
					comment = <"Verdisetettet her er samordnet med FHIR Diagnostic report status, med unntak av koden \"Entered in error\" som bare er relevant i et meldingsorientert paradigme. Andre lokale koder eller termer kan brukes ved å bruke datatypen \"Fri eller kodet tekst\". I de fleste tilfellene er det bare passende å bruke en statuskode. Dataelementet er imidlertid åpent for flere forekomster for å understøtte bruksområdene der statuser for forskjellige aspekter av resultatet er delt opp i flere elementer. Et eksempel på dette er de norske standard svarrapport av medisinske tjenester v1.4 (HIS 80822:2014), der status for undersøkelsesresultat (Tjenestetype) og status/steg i arbeidsflyt håndteres separat.

Dette elementet tilsvarer FHIR Diagnostic report status.">
				>
				["at0073"] = <
					text = <"Registrert">
					description = <"Undersøkelsen er registrert i radiologisystemet (RIS), men resultatet er ikke tilgjengelig ennå.">
				>
				["at0074"] = <
					text = <"Ufullstendig">
					description = <"Undersøkelsen er delvis (dvs initalt, foreløpig eller preliminært) besvart. Resultatet kan være ukomplett eller ubekreftet.">
				>
				["at0075"] = <
					text = <"Foreløpig">
					description = <"Verifiserte tidlige svar er tilgjengelige, men ikke alle svar er endelige. Dette er en underkategori av \"Ufullstendig\".">
				>
				["at0076"] = <
					text = <"Endelig">
					description = <"Undersøkelsesresultatene er komplett og bekreftet av ansvarlig(e) person(er).">
				>
				["at0077"] = <
					text = <"Revidert">
					description = <"Undersøkelsesresultatet har blitt modifisert etter å ha vært i status \"Endelig\".">
				>
				["at0078"] = <
					text = <"Korrigert">
					description = <"Undersøkelsesresultatet har blitt modifisert, for å rette en feil, etter å ha vært i status \"Endelig\". Dette er en underkategori til \"Revidert\".">
				>
				["at0079"] = <
					text = <"Tillegg">
					description = <"Nytt innhold har blitt lagt til i undersøkelsesresultatet etter at resultatet har hatt status \"Endelig\". Det eksisterende innholdet er uendret. Dette er en underkategori av \"Revidert\".">
				>
				["at0080"] = <
					text = <"Kansellert">
					description = <"Svaret er utilgjengelig fordi analysen ikke ble påbegynt eller ferdigstilt (også kalt \"avbrutt\").">
				>
				["at0083"] = <
					text = <"Kopimottakere">
					description = <"Detaljert informasjon knyttet til kliniker eller organisatorisk enhet som skal motta en kopi av den bildediagnostiske resultatet.">
					comment = <"Personene eller organisasjonene på distribusjonslisten mottar svaret kun til informasjon, og det er hovedmottakeren av rapporten som forventes å agere på den.">
				>
				["at0087"] = <
					text = <"Teknikk">
					description = <"Navn på den radiologiske prosedyren som er brukt for å gjennomføre undersøkelsen.">
					comment = <"Koding av 'Teknikk' med en terminologi er tillatt. Flere forekomster av dataelementet gjør det mulig å identifisere flere enn en teknikk.">
				>
				["at0088"] = <
					text = <"Prosedyre">
					description = <"Identifisering av den kliniske prosedyren eller håndteringen som er brukt som en del av den bildediagnostiske undersøkelsen.">
				>
				["at0090"] = <
					text = <"Ukjent">
					description = <"Resultatstatus er ukjent.">
				>
				["at0091"] = <
					text = <"Modalitet">
					description = <"Typen bildedannende utstyr, prosess eller metode som opprinnelig innhentet eller produserte dataene som ble brukt til å lage bildene i studien.">
					comment = <"For eksempel: \"Rtg\", \"CT\", \"Ultralyd\", \"UL\", \"MR\", \"Bentetthetsmåling\", \"Tomografi\", \"Mammografi\", \"Nukleærmedisin\", \"PET\", \"Angiografi\" og \"Fluoroskopi\". 
Dette dataelementet kan være overflødig hvis modaliteten er tydelig representert i 'Undersøkelsesnavnet'. Alternativt kan det også være mulig å autofylle 'Modalitet' fra et kodet 'Undersøkelsesnavn'. Koding av 'Modalitet' med et verdisett anbefales. For eksempel er verdien i kodeverket \"DICOM CID 29 Acquisition Modality\". Vanligvis tilsvarer dette ImagingStudy.modality i FHIR og Modality (0008, 0060) i DICOM.">
				>
				["at0092"] = <
					text = <"Undersøkelses ID">
					description = <"Unik identifikator for bildeundersøkelsen som tildeles av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer DICOM Study Instance UID (0020,000D) og ImagingStudy.identifier i FHIR.">
				>
				["at0093"] = <
					text = <"Detaljer om sammenlignende bilder">
					description = <"Detaljer om bilder fra en tidligere bildediagnostisk undersøkelse som er bruk til sammenlikning.">
				>
				["at0094"] = <
					text = <"Undersøkelsesnavn">
					description = <"Navnet på den bildediagnostiske bildeundersøkelsen som det sammenlignes med.">
				>
				["at0095"] = <
					text = <"Undersøkelses ID">
					description = <"Den unike identifikatoren til den bildediagnostisk undersøkelsen det sammenlignes med.">
				>
				["at0096"] = <
					text = <"Undersøkelsesdato">
					description = <"Dato og tid den bildediagnostiske undersøkelsen det sammenlignes med.">
				>
				["at0097"] = <
					text = <"Lagringssted">
					description = <"Det digitale lagringsstedet for bildeinnhold og metadata.">
					comment = <"Dette lagringsstedet identifiserer nettverkstjenesten som gir tilgang til undersøkelsen for lagring, visning, spørring eller henting. Tilsvarer ImagingStudy.endpoint i FHIR.">
				>
				["at0098"] = <
					text = <"Lagringssted">
					description = <"Det digitale lagringsstedet for bildeinnhold og metadata.">
					comment = <"Dette lagringsstedet identifiserer nettverkstjenesten som gir tilgang til undersøkelsen for lagring, visning, spørring eller henting. Tilsvarer ImagingStudy.endpoint i FHIR.">
				>
				["at0099"] = <
					text = <"Undersøkelsesstatus">
					description = <"Den nåværende statusen av den bildediagnostiske undersøkelsen i RIS/PACS.">
					comment = <"Tilsvarer ImagingStudy.status i FHIR.">
				>
				["at0100"] = <
					text = <"Registrert">
					description = <"Den bildediagnostiske undersøkelsen er registrert i radiologisystemet (RIS), men resultatet er ikke tilgjengelig ennå.">
				>
				["at0101"] = <
					text = <"Tilgjengelig">
					description = <"Minst én forekomst er ferdig i denne bildediagnostiske undersøkelsen.">
				>
				["at0102"] = <
					text = <"Kansellert">
					description = <"Den bildediagnostiske undersøkelsen er utilgjengelig fordi undersøkelsen ikke ble startet eller fullført.">
				>
				["at0103"] = <
					text = <"Ukjent">
					description = <"Statusen er ikke kjent av radiologi informasjonssystemet (RIS) .">
				>
				["at0104"] = <
					text = <"RapportID">
					description = <"Den lokale, unike identifikator som tildeles en bildediagnostisk svarrapport av radiologisystemet (RIS/PACS).">
					comment = <"Siden en instans av denne arketypen brukes per undersøkelse, kan denne ID'en knyttes til flere enn en undersøkelse, og brukes for å koble sammen svar på flere undersøkelser i en sammenstilt svarrapport. Tilsvarer DiagnosticReport.identifier i FHIR.">
				>
				["at0105"] = <
					text = <"Klassifikasjon av undersøkelsen">
					description = <"Klassifikasjon eller beskrivelse av undersøkelsen som tildeles av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer ImagingStudy.description i FHIR og Study description (0008, 1030) i DICOM. Eksempel på mulige verdier i DICOM er \"MR BREAS\", \"UNIT\"; \"NEURO^HEAD\"; or \"MR BREASTUNI UE\".">
				>
				["at0106"] = <
					text = <"Bildedetaljer">
					description = <"Link til alle detaljer om eventuell bilder utført under den bildediagnostiske undersøkelsen.">
					comment = <"Vanligvis brukes DICOM modaliteter for å referere til bilder, men dette datafeltet gir en alternativ metode for å få tilgang til bildene. Tilsvarer FHIRs DiagnosticReport.imagingStudy.">
				>
			>
		>
		["de"] = <
			items = <
				["at0000"] = <
					text = <"Bildgebendes Untersuchungsergebnis">
					description = <"*The result of an imaging examination performed on an individual, using radiological techniques. (en)">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Beliebiges Ereignis">
					description = <"Standardwert, ein undefinierter/s Zeitpunkt oder Intervallereignis, das explizit im Template oder zur Laufzeit der Anwendung definiert werden kann.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"*Study name (en)">
					description = <"*The name of the imaging examination performed. (en)">
					comment = <"*For example: 'CT abdomen'; 'Doppler ultrasound of renal artery' or 'Plain chest xray'. Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Examples of candidate terminologies are the LOINC Imaging Document Codes, RadLex Playbook or SNOMED-CT. Equivalent to DiagnosticReport.code in FHIR. (en)">
				>
				["at0006"] = <
					text = <"*Structured target body site (en)">
					description = <"*Structured detail about the body site or region targetted for imaging. (en)">
					comment = <"*If the body site is specified within the text or code for 'Study name', then this field may be redundant. (en)">
				>
				["at0008"] = <
					text = <"Bildgebende Befunde">
					description = <"*Narrative description or overview of all clinical findings. (en)">
				>
				["at0014"] = <
					text = <"*Clinical summary (en)">
					description = <"*Narrative description of relevant clinical history that provides context for the examination and interpretation of results. (en)">
					comment = <"*Alternatively this data element can include an RM link to clinical information provided in the original examination request or located elsewhere within the health record. (en)">
				>
				["at0020"] = <
					text = <"Bildgebende Diagnose">
					description = <"Eine einzelne Wortangabe, eine Phrase oder eine kurze Beschreibung, die einen wahrscheinlichen Zustand oder eine wahrscheinliche Diagnose darstellt.">
					comment = <"*This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging differential diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" Equivalent to DiagnosticReport.conclusionCode in FHIR. (en)">
				>
				["at0021"] = <
					text = <"Schlussfolgerung">
					description = <"Kurz beschriebene, klinisch relevante Interpretation aller bildgebenden Befunde und gegebenenfalls ein Vergleich zu früheren Studien.">
					comment = <"*Also referred to as 'Opinion' or 'Conclusion'. Equivalent to DiagnosticReport.conclusion in FHIR. (en)">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Zusätzliche Informationen über die Untersuchung, welche noch nicht an anderer Stelle dargestellt wurden.">
					comment = <"*For example: a note that the film was given to the patient. Equivalent to DiagnosticReport.note in FHIR. (en)">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Bildgebungsdienst">
					description = <"*Details about the service, organisation or individual carrying out the imaging examination. (en)">
				>
				["at0027"] = <
					text = <"Details zum Untersuchungsauftrag">
					description = <"*Details about a single imaging examination requested. (en)">
				>
				["at0028"] = <
					text = <"Auftragskennung des Auftragsgebers">
					description = <"*Unique identifier for the imaging examination order assigned by the requester. (en)">
					comment = <"*Equivalent to the HL7 Placer Order Identifier. (en)">
				>
				["at0029"] = <
					text = <"Name der angeforderter Untersuchung">
					description = <"*Identification of imaging examination requested. (en)">
					comment = <"*This can be usefule in situations where the examination requested differs from the examination performed. (en)">
				>
				["at0030"] = <
					text = <"Auftragsgeber">
					description = <"*Details about the clinician and/or organisation requesting the imaging examination. (en)">
					comment = <"*Equivalent to DICOM Referring Physician's Name (0008,0090) (en)">
				>
				["at0031"] = <
					text = <"*Receiver order identifier (en)">
					description = <"*Unique identifier for the imaging examination order assigned by the radiology service. (en)">
					comment = <"*Equivalent to the HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050). (en)">
				>
				["at0041"] = <
					text = <"Technik / Prozedur strukturiert">
					description = <"Zusätzliche strukturierte Informationen zu technischen und klinischen Aspekten der Aufnahme des Bildes/der Bilder.">
				>
				["at0044"] = <
					text = <"*Structured imaging findings (en)">
					description = <"*Structured details about the imaging examination findings targeting a specific structure or region. (en)">
				>
				["at0045"] = <
					text = <"Berichtsrepräsentation">
					description = <"*Digital representation of the examination result. (en)">
					comment = <"*Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report. Equivalent to DiagnosticReport.presentedForm in FHIR. (en)">
				>
				["at0046"] = <
					text = <"Erweiterung">
					description = <"Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.">
					comment = <"Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Störfaktoren">
					description = <"Die Beschreibung von Faktoren, welche noch nicht an anderer Stelle erfasst werden, die Untersuchungsbefunde und / oder -ergebnisse beeinflussen können.">
				>
				["at0049"] = <
					text = <"*Technique summary (en)">
					description = <"*Narrative description about the radiological procedure used to capture the study. (en)">
					comment = <"*Use this data element to describe a summary of technique, including technical limitations or deviation from the normal technical procedures. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.' (en)">
				>
				["at0055"] = <
					text = <"*Target body site (en)">
					description = <"*Description of the simple body site or region targetted for imaging. (en)">
					comment = <"*If the body site is specified within the text or code for 'Study name', then this field may be redundant. Coding of 'Target body site' with a terminology, such as SNOMED CT, is desirable. Equivalent to ImagingStudy.series.bodySite in FHIR. (en)">
				>
				["at0056"] = <
					text = <"*Comparison findings (en)">
					description = <"Die Beschreibung zum Vergleich dieser Untersuchung mit früheren ähnlichen Studien.">
					comment = <"*Also use this data element to indicate if no comparison images and/or reports are available. Details about the images used for comparison can be recorded using the 'Comparison study details' CLUSTER, within Protocol. (en)">
				>
				["at0057"] = <
					text = <"*Imaging quality description (en)">
					description = <"*Narrative description about the quality of the examination. (en)">
					comment = <"Zum Beispiel: die Art jeglicher Einschränkungen und deren Auswirkung auf die Interpretation, Empfehlung zur Wiederholung der Studie oder ob eine alternative Beurteilungsmethode geeigneter sein könnte.">
				>
				["at0058"] = <
					text = <"Bildgebende Differentialdiagnose">
					description = <"Eine einzelne Wortangabe, eine Phrase oder eine kurze Beschreibung, die einen möglichen Zustand oder eine mögliche Diagnose darstellt.">
					comment = <"*This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" (en)">
				>
				["at0059"] = <
					text = <"Empfehlung">
					description = <"Vorschlag für weitere Bildgebung, Untersuchungen und/oder Überweisung und dazugehörige Begründung.">
					comment = <"Dieses Datenelement hat die Kardinalität 0..*, um mehr als eine Empfehlung zu ermöglichen. Formelle Anordnungen für zusätzliche bildgebende Untersuchungen, Prüfungen sollten unter Verwendung eines INSTRUCTION-Archetyps wie INSTRUCTION.service_request dargestellt werden.">
				>
				["at0061"] = <
					text = <"*Clinical indication (en)">
					description = <"*Narrative description about the reason the examination was originally requested. (en)">
					comment = <"*Also known as 'Clinical question' or 'Clinical query'. (en)">
				>
				["at0062"] = <
					text = <"*Procedure summary (en)">
					description = <"*Narrative description about the clinical procedure or other clinical considerations used to capture the study. (en)">
					comment = <"*Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered.  (en)">
				>
				["at0063"] = <
					text = <"*Imaging quality (en)">
					description = <"Die Beurteilung der Qualität der Untersuchung.">
					comment = <"Es wird empfohlen, die Qualität der Bildgebung mit einer Wertemenge zu codieren. Zum Beispiel: „ausgezeichnet, gut, mittelmäßig, schlecht und nicht diagnostisch“ oder „angemessen, suboptimal und unzureichend“.">
				>
				["at0065"] = <
					text = <"*Series details (en)">
					description = <"*Details about a series included in this report. (en)">
				>
				["at0067"] = <
					text = <"*Comparison series details (en)">
					description = <"*Details about an imaging series being compared to the reported study. (en)">
				>
				["at0069"] = <
					text = <"Gerät">
					description = <"*Details about imaging device/s used to capture the study. (en)">
				>
				["at0070"] = <
					text = <"*Study date (en)">
					description = <"*Date/time when the imaging started. (en)">
					comment = <"*Equivalent to ImagingStudy.started in FHIR. (en)">
				>
				["at0071"] = <
					text = <"*Status timestamp (en)">
					description = <"*The date and/or time that ‘Overall result status’ was assigned. (en)">
				>
				["at0072"] = <
					text = <"*Overall result status (en)">
					description = <"*The status of the imaging examination result as a whole. (en)">
					comment = <"*The values have been aligned with the FHIR Diagnostic report status, with the exception of 'Entered in error' which is relevant only to a messaging-oriented paradigm. Other local codes/terms can be used via the Text 'choice'. In most use cases, only one status is applicable at any time, however this data element allows multiple occurrences for the use cases where there are a variety of types of status that need to be specified, usually within a template. Equivalent to DiagnosticReport.status in FHIR.  (en)">
				>
				["at0073"] = <
					text = <"Registriert">
					description = <"*The existence of the study is registered, but no result is yet available. (en)">
				>
				["at0074"] = <
					text = <"Teilweise">
					description = <"*This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified. (en)">
				>
				["at0075"] = <
					text = <"Vorläufig">
					description = <"Es sind erste, bestätigte Ergebnisse verfügbar, aber nicht alle Ergebnisse sind final. Dies ist eine Unterkategorie von \"Teilweise\".">
				>
				["at0076"] = <
					text = <"Final">
					description = <"*The result is complete and verified by an authorised person. (en)">
				>
				["at0077"] = <
					text = <"Abgeändert">
					description = <"*Subsequent to being final, the result has been modified. (en)">
				>
				["at0078"] = <
					text = <"Korrigiert">
					description = <"*Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'. (en)">
				>
				["at0079"] = <
					text = <"Hinzugefügt">
					description = <"*Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'. (en)">
				>
				["at0080"] = <
					text = <"Abgebrochen">
					description = <"Das Ergebnis ist nicht verfügbar, weil der Test nicht gestartet oder nicht abgeschlossen wurde (manchmal auch als \"gescheitert\" bezeichnet).">
				>
				["at0083"] = <
					text = <"Verteilerliste">
					description = <"*Details of additional clinicians or organisations who require a copy of the examination result. (en)">
					comment = <"Die \"Verteilerliste\" dient nur zu Informationszwecken. Der Hauptempfänger des Berichts ist die Person, die dazu bestimmt ist, auf die Information zu reagieren.">
				>
				["at0087"] = <
					text = <"*Technique (en)">
					description = <"*Name of the radiological procedure used to capture the study. (en)">
					comment = <"*Coding of the 'Technique' with a terminology is permitted. Multiple occurrences allow for identification of one or more techniques. (en)">
				>
				["at0088"] = <
					text = <"*Procedure (en)">
					description = <"*Identification of the clinical procedure or management used to capture the study. (en)">
				>
				["at0090"] = <
					text = <"*Unknown (en)">
					description = <"*The status of the result is not known. (en)">
				>
				["at0091"] = <
					text = <"*Modality (en)">
					description = <"*The type of imaging device, process or method that originally acquired or produced the data used to create the images in the study. (en)">
					comment = <"*For example: 'XRay', 'CT scan', 'ultrasound', 'MRI', 'bone densitometry', 'tomography', 'mammography', 'nuclear medicine', 'PET', 'angiography', and 'fluoroscopy'. This data element may be redundant if the modality is clearly represented in the 'Study name'. Alternatively it may also be possible to autopopulate the 'Modality' from a coded 'Study name'. Coding of the 'Modality' with a value set is recommended, such as the DICOM CID 29 Acquisition Modality value set. Equivalent to ImagingStudy.modality in FHIR and Modality (0008, 0060) in DICOM. (en)">
				>
				["at0092"] = <
					text = <"*Study instance identifier (en)">
					description = <"*Unique identifier for the imaging study assigned by the imaging device. (en)">
					comment = <"*Equivalent to Study Instance UID (0020,000D) in DICOM and ImagingStudy.identifier in FHIR. (en)">
				>
				["at0093"] = <
					text = <"*Comparison study details (en)">
					description = <"*Details about images from a prior study used for comparison to the reported study. (en)">
				>
				["at0094"] = <
					text = <"*Study name (en)">
					description = <"*The name of the comparison imaging examination performed. (en)">
				>
				["at0095"] = <
					text = <"*Study identifier (en)">
					description = <"*Unique identifier for the comparison imaging study. (en)">
				>
				["at0096"] = <
					text = <"*Study date (en)">
					description = <"*Date and time the comparison examination started. (en)">
				>
				["at0097"] = <
					text = <"*Study end point (en)">
					description = <"*Digital location of the comparison imaging content and metadata. (en)">
					comment = <"*This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR. (en)">
				>
				["at0098"] = <
					text = <"*Study end point (en)">
					description = <"*Digital location of the imaging content and metadata. (en)">
					comment = <"*This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR. (en)">
				>
				["at0099"] = <
					text = <"*Study status (en)">
					description = <"*The current state of the imaging study. (en)">
					comment = <"*Equivalent to ImagingStudy.status in FHIR. (en)">
				>
				["at0100"] = <
					text = <"*Registered (en)">
					description = <"*The existence of the imaging study is registered, but there is nothing yet available. (en)">
				>
				["at0101"] = <
					text = <"*Available (en)">
					description = <"*At least one instance has been associated with this imaging study. (en)">
				>
				["at0102"] = <
					text = <"*Cancelled (en)">
					description = <"*The imaging study is unavailable because the study was not started or not completed. (en)">
				>
				["at0103"] = <
					text = <"*Unknown (en)">
					description = <"*The status is not known by the radiology source system. (en)">
				>
				["at0104"] = <
					text = <"*Report identifier (en)">
					description = <"*Unique identifier for the imaging report assigned by the radiology service. (en)">
					comment = <"*As one instance of this archetype is used per study, this identifier can be used to connect more than one imaging study within a single combined report. Equivalent to DiagnosticReport.identifier in FHIR.  (en)">
				>
				["at0105"] = <
					text = <"*Study description (en)">
					description = <"*Radiology service-generated description or classification of the study. (en)">
					comment = <"*Equivalent to ImagingStudy.description in FHIR and Study description (0008, 1030) in DICOM. Possible DICOM examples: MR BREAS, UNIT; NEURO^HEAD; or MR BREASTUNI UE. (en)">
				>
				["at0106"] = <
					text = <"*Image details (en)">
					description = <"*Link to full details of any imaging carried out during the study. (en)">
					comment = <"*Typically referencing images used the DICOM enabled modalities, but this data field provides an alternative method to access the images. Equivalent to FHIR's DiagnosticReport.imagingStudy. (en)">
				>
			>
		>
		["sv"] = <
			items = <
				["at0000"] = <
					text = <"Bilddiagnostiskt resultat">
					description = <"*The result of an imaging examination performed on an individual, using radiological techniques. (en)">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Ospecificerad händelse">
					description = <"Förinställd, ospecificerad tidpunkt eller tidsintervall som kan definieras mer uttryckligen i en mall (template) eller i en applikation.
">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"*Study name (en)">
					description = <"*The name of the imaging examination performed. (en)">
					comment = <"*For example: 'CT abdomen'; 'Doppler ultrasound of renal artery' or 'Plain chest xray'. Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Examples of candidate terminologies are the LOINC Imaging Document Codes, RadLex Playbook or SNOMED-CT. Equivalent to DiagnosticReport.code in FHIR. (en)">
				>
				["at0006"] = <
					text = <"*Structured target body site (en)">
					description = <"*Structured detail about the body site or region targetted for imaging. (en)">
					comment = <"*If the body site is specified within the text or code for 'Study name', then this field may be redundant. (en)">
				>
				["at0008"] = <
					text = <"*Imaging findings (en)">
					description = <"*Narrative description or overview of all clinical findings. (en)">
				>
				["at0014"] = <
					text = <"*Clinical summary (en)">
					description = <"*Narrative description of relevant clinical history that provides context for the examination and interpretation of results. (en)">
					comment = <"*Alternatively this data element can include an RM link to clinical information provided in the original examination request or located elsewhere within the health record. (en)">
				>
				["at0020"] = <
					text = <"Diagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar det troliga tillståndet eller diagnosen.">
					comment = <"*This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging differential diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" Equivalent to DiagnosticReport.conclusionCode in FHIR. (en)">
				>
				["at0021"] = <
					text = <"Slutsats">
					description = <"Kortfattad och kliniskt relevant tolkning av alla bilddiagnostiska fynd, inklusive en jämförelse med tidigare studier där så är lämpligt.
">
					comment = <"*Also referred to as 'Opinion' or 'Conclusion'. Equivalent to DiagnosticReport.conclusion in FHIR. (en)">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligare beskrivning av undersökningen som inte fångats i andra fält.">
					comment = <"*For example: a note that the film was given to the patient. Equivalent to DiagnosticReport.note in FHIR. (en)">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"*Imaging service (en)">
					description = <"*Details about the service, organisation or individual carrying out the imaging examination. (en)">
				>
				["at0027"] = <
					text = <"Beställningsdetaljer">
					description = <"*Details about a single imaging examination requested. (en)">
				>
				["at0028"] = <
					text = <"Beställnings-ID">
					description = <"*Unique identifier for the imaging examination order assigned by the requester. (en)">
					comment = <"*Equivalent to the HL7 Placer Order Identifier. (en)">
				>
				["at0029"] = <
					text = <"Namn på beställd undersökning">
					description = <"*Identification of imaging examination requested. (en)">
					comment = <"*This can be usefule in situations where the examination requested differs from the examination performed. (en)">
				>
				["at0030"] = <
					text = <"Beställare">
					description = <"*Details about the clinician and/or organisation requesting the imaging examination. (en)">
					comment = <"*Equivalent to DICOM Referring Physician's Name (0008,0090) (en)">
				>
				["at0031"] = <
					text = <"*Receiver order identifier (en)">
					description = <"*Unique identifier for the imaging examination order assigned by the radiology service. (en)">
					comment = <"*Equivalent to the HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050). (en)">
				>
				["at0041"] = <
					text = <"*Structured technique/procedure (en)">
					description = <"*Additional structured details of technical and clinical aspects of capturing the image/s. (en)">
				>
				["at0044"] = <
					text = <"*Structured imaging findings (en)">
					description = <"*Structured details about the imaging examination findings targeting a specific structure or region. (en)">
				>
				["at0045"] = <
					text = <"*Report representation (en)">
					description = <"*Digital representation of the examination result. (en)">
					comment = <"*Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report. Equivalent to DiagnosticReport.presentedForm in FHIR. (en)">
				>
				["at0046"] = <
					text = <"Tilläggsinformation">
					description = <"Ytterligare information som krävs för att utvidga modellen med lokalt innehåll eller för anpassning till andra referensmodeller.
">
					comment = <"Till exempel: lokala informationskrav eller ytterligare metadata för anpassning till FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Möjliga felkällor">
					description = <"Beskrivning av faktorer som inte registrerats någon annanstans, men som kan påverka fynden och/eller undersökningsresultatet.">
				>
				["at0049"] = <
					text = <"*Technique summary (en)">
					description = <"*Narrative description about the radiological procedure used to capture the study. (en)">
					comment = <"*Use this data element to describe a summary of technique, including technical limitations or deviation from the normal technical procedures. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.' (en)">
				>
				["at0055"] = <
					text = <"*Target body site (en)">
					description = <"*Description of the simple body site or region targetted for imaging. (en)">
					comment = <"*If the body site is specified within the text or code for 'Study name', then this field may be redundant. Coding of 'Target body site' with a terminology, such as SNOMED CT, is desirable. Equivalent to ImagingStudy.series.bodySite in FHIR. (en)">
				>
				["at0056"] = <
					text = <"*Comparison findings (en)">
					description = <"*Narrative description about the comparison of this examination with previous similar studies. (en)">
					comment = <"*Also use this data element to indicate if no comparison images and/or reports are available. Details about the images used for comparison can be recorded using the 'Comparison study details' CLUSTER, within Protocol. (en)">
				>
				["at0057"] = <
					text = <"*Imaging quality description (en)">
					description = <"*Narrative description about the quality of the examination. (en)">
					comment = <"*For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate. (en)">
				>
				["at0058"] = <
					text = <"Differentialdiagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar ett möjligt tillstånd eller en diagnos.">
					comment = <"*This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" (en)">
				>
				["at0059"] = <
					text = <"Rekommendation">
					description = <"Förslag för ytterligare avbildning, undersökningar och/eller hänvisning och tillhörande motiv.

">
					comment = <"*This data element has 0..* occurrences to allow for more than one recommendation. Formal orders for additional imaging examination, investigation should be recorded using an INSTRUCTION archetype, such as INSTRUCTION.service_request. (en)">
				>
				["at0061"] = <
					text = <"*Clinical indication (en)">
					description = <"*Narrative description about the reason the examination was originally requested. (en)">
					comment = <"*Also known as 'Clinical question' or 'Clinical query'. (en)">
				>
				["at0062"] = <
					text = <"*Procedure summary (en)">
					description = <"*Narrative description about the clinical procedure or other clinical considerations used to capture the study. (en)">
					comment = <"*Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered.  (en)">
				>
				["at0063"] = <
					text = <"*Imaging quality (en)">
					description = <"*Assessment about the quality of the examination. (en)">
					comment = <"*Coding of the imaging quality with a value set is recommended. For example: 'excellent, good, fair, poor, and non-diagnostic'; or 'adequate, suboptimal and inadequate'.
 (en)">
				>
