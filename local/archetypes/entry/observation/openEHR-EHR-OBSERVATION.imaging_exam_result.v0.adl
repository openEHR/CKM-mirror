				["at0056"] = <
					text = <"Vergleich zu Vorbefunden">
					description = <"Die Beschreibung des Vergleichs dieses Bildes oder einer Bildserie zu früheren ähnlichen Untersuchungen.">
					comment = <"Wenn keine früheren Bilder und / oder Berichte verfügbar sind, sollte dies mittels dieses Datenelements angegeben werden.">
