definition
	OBSERVATION[at0000] matches {    -- Imaging examination result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0004] matches {    -- Test name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Modality
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0055] occurrences matches {0..1} matches {    -- Target site
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Structured target site
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Overall result status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0009,    -- Registered
													at0010,    -- Interim
													at0011,    -- Final
													at0012,    -- Amended
													at0013]    -- Cancelled / Aborted
												}
											}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- DateTime result issued
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {    -- Clinical query
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Clinical summary
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Imaging findings
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Structured imaging findings
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_finding\.v0/}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Comparison
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Study quality
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {    -- Study quality description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Conclusion
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0058] occurrences matches {0..*} matches {    -- Imaging differential diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0020] occurrences matches {0..*} matches {    -- Imaging diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0059] occurrences matches {0..*} matches {    -- Recommendation
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {    -- Report representation
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0023] occurrences matches {0..*} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0047] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0048] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0025] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0026] occurrences matches {0..*} matches {    -- Imaging service
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {    -- Examination request details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Requester order identifier
								value matches {
									DV_IDENTIFIER matches {*}
								}
							}
							ELEMENT[at0029] occurrences matches {0..*} matches {    -- Examination requested name
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0030] occurrences matches {0..*} matches {    -- Requester
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {    -- Order identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0036] occurrences matches {0..*} matches {    -- DICOM study identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Report identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Technique
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0062] occurrences matches {0..1} matches {    -- Procedure
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Structured technique/procedure
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0065] occurrences matches {0..*} matches {    -- Reported image/s
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[at0064] occurrences matches {0..*} matches {    -- Comparison images
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0068] occurrences matches {0..1} matches {    -- DICOM series identifier
								value matches {
									DV_IDENTIFIER matches {*}
								}
							}
							allow_archetype CLUSTER[at0067] occurrences matches {0..*} matches {    -- Comparison image/s
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					allow_archetype CLUSTER[at0069] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Imaging examination result">
					description = <"Record the findings and interpretation of an imaging examination performed.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Test name">
					description = <"The name of the imaging examination or procedure performed.">
					comment = <"Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Possible candidate terminologies are LOINC, SNOMED CT or RadLex.">
				>
				["at0005"] = <
					text = <"Modality">
					description = <"Type of equipment that originally acquired the image or series of images.">
					comment = <"Also known as 'Examination type'. For example: Ultrasound; Computed tomography; or X-ray. Coding with a terminology is desirable, where possible. If the modality is specified by a code in the 'Test name', then this field may be redundant.">
				>
				["at0006"] = <
					text = <"Structured target site">
					description = <"Structured detail about the anatomical site related to the entire result.">
					comment = <"If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Identification of an anatomical site related to a specific finding will be recorded within each instance of CLUSTER.imaging_finding.">
				>
				["at0007"] = <
					text = <"Overall result status">
					description = <"The status of the examination result as a whole.">
				>
				["at0008"] = <
					text = <"Imaging findings">
					description = <"Narrative description of the clinical findings.">
				>
				["at0009"] = <
					text = <"Registered">
					description = <"No result yet available.">
				>
				["at0010"] = <
					text = <"Interim">
					description = <"This is an initial or interim result: data may be missing or verification not been performed.">
				>
				["at0011"] = <
					text = <"Final">
					description = <"The result is complete and verified by the responsible radiologist.">
				>
				["at0012"] = <
					text = <"Amended">
					description = <"The result has been modified subsequent to being Final, and is complete and verified by the radiologist.">
				>
				["at0013"] = <
					text = <"Cancelled / Aborted">
					description = <"The result is not available because the examination was not started or completed.">
				>
				["at0014"] = <
					text = <"Clinical summary">
					description = <"Narrative description of relevant clinical history that provides context for the examination and interpretation of results.">
					comment = <"This may include a link to the clinical information provided in the original examination request or elsewhere in the health record.">
				>
				["at0020"] = <
					text = <"Imaging diagnosis">
					description = <"Single word, phrase or brief description representing the likely condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one diagnoses. Coding with a terminology is preferred, where possible. This data element should be regarded as mutually exclusive to 'Differential diagnoses' - only one of 'Differential diagnoses' OR 'Imaging diagnosis' should be present in the each Imaging examination result.">
				>
				["at0021"] = <
					text = <"Conclusion">
					description = <"Narrative concise, clinically relevant interpretation of all imaging findings, and include a comparison with previous studies where appropriate.">
					comment = <"Also referred to as 'Opinion' or 'Impression'.">
				>
				["at0023"] = <
					text = <"Comment">
					description = <"Additional narrative about the examination not captured in other fields.">
					comment = <"For example: a note that the film was given to the patient.">
				>
				["at0024"] = <
					text = <"DateTime result issued">
					description = <"The date and/or time that the result was issued for the recorded 'Examination result status'.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Imaging service">
					description = <"Demographic details about the service, organisation or individual carrying out the imaging examination.">
				>
				["at0027"] = <
					text = <"Examination request details">
					description = <"Details concerning a single examination requested.">
					comment = <"Usually there is one examination request for each result, however in some circumstances multiple examination requests may be represented using a single Imaging examination result archetype.">
				>
				["at0028"] = <
					text = <"Requester order identifier">
					description = <"The local identifier assigned to the order by the order requester. Equivalent to the HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Examination requested name">
					description = <"Identification of imaging examination or procedure requested, where the examination requested differs from the examination actually performed.">
				>
				["at0030"] = <
					text = <"Requester">
					description = <"Details about the clinician or organisation requesting the imaging examination.">
				>
				["at0031"] = <
					text = <"Order identifier">
					description = <"The local identifier assigned to the examination order by the order filler, usually by the receiving Radiology Information System (RIS). Usually equivalent to the HL7 Filler Order Number.">
				>
				["at0033"] = <
					text = <"Report identifier">
					description = <"The local identifier given to the imaging examination report.">
				>
				["at0036"] = <
					text = <"DICOM study identifier">
					description = <"Unique identifier of the reported study allocated by the imaging service.">
				>
				["at0041"] = <
					text = <"Structured technique/procedure">
					description = <"Additional structured details of technical and clinical aspects of capturing the image/s.">
				>
				["at0044"] = <
					text = <"Structured imaging findings">
					description = <"Structured details about the imaging  findings.">
				>
				["at0045"] = <
					text = <"Report representation">
					description = <"Digital image or video representing the examination result.">
				>
				["at0046"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Confounding factors">
					description = <"Narrative description of factors, not recorded elsewhere, that may influence the examination findings and/or result.">
				>
				["at0049"] = <
					text = <"Technique">
					description = <"Narrative description about the technical aspects of capturing the image/s.">
					comment = <"Use this data element to provide an outline of technique. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.' It is particularly useful to record technical limitations or any deviation from normal, such as capturing a non-routine alternative or additional imaging.">
				>
				["at0055"] = <
					text = <"Target site">
					description = <"Simple description about the body site or region targetted for imaging.">
					comment = <"If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant.">
				>
				["at0056"] = <
					text = <"Comparison">
					description = <"Narrative description about the comparison of this examination, with previous similar studies.">
					comment = <"Also use this data element to indicate if no comparison images and/or reports are available.">
				>
				["at0057"] = <
					text = <"Study quality description">
					description = <"Narrative description about the quality of the examination study.">
					comment = <"For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate.">
				>
				["at0058"] = <
					text = <"Imaging differential diagnosis">
					description = <"Single word, phrase or brief description representing a possible condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnoses. Coding with a terminology is preferred, where possible. This data element should be regarded as mutually exclusive to 'Imaging diagnosis' - only one of 'Differential diagnoses' OR 'Imaging diagnosis' should be present in each Imaging examination result.">
				>
				["at0059"] = <
					text = <"Recommendation">
