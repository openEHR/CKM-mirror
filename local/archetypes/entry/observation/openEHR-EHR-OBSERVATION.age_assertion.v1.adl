					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Extensió">
					description = <"Informació addicional necessària per a ampliar el model amb contingut local o per alinear-se amb altres models/formalismes de referència.">
					comment = <"Per exemple: requisits locals d'informació; o metadades addicionals per alinear-se amb equivalents de FHIR.">
				>
			>
		>
