definition
	OBSERVATION[at0000] matches {    -- Imaging examination result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0004] matches {    -- Study name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0091] occurrences matches {0..*} matches {    -- Modality
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0055] occurrences matches {0..1} matches {    -- Target body site
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Structured target body site
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0070] occurrences matches {0..1} matches {    -- Study date
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0072] occurrences matches {0..*} matches {    -- Overall result status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0073,    -- Registered
													at0074,    -- Partial
													at0075,    -- Preliminary
													at0076,    -- Final
													at0077,    -- Amended
													at0078,    -- Corrected
													at0079,    -- Appended
													at0080,    -- Cancelled
													at0090]    -- Unknown
												}
											}
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0071] occurrences matches {0..1} matches {    -- Status timestamp
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {    -- Clinical indication
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Clinical summary
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Imaging findings
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Structured imaging findings
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Comparison findings
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Imaging quality
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {    -- Imaging quality description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Overall impression
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0058] occurrences matches {0..*} matches {    -- Imaging differential diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0020] occurrences matches {0..*} matches {    -- Imaging diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0059] occurrences matches {0..*} matches {    -- Recommendation
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {    -- Report representation
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0023] occurrences matches {0..*} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0047] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0048] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0108] occurrences matches {0..1} matches {    -- Position
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0107] occurrences matches {0..*} matches {    -- Stabilising appliance
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0025] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0092] occurrences matches {0..1} matches {    -- Study instance identifier
						value matches {
							DV_IDENTIFIER matches {*}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0105] occurrences matches {0..1} matches {    -- Study description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0104] occurrences matches {0..1} matches {    -- Report identifier
						value matches {
							DV_IDENTIFIER matches {*}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0099] occurrences matches {0..1} matches {    -- Study status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0100,    -- Registered
									at0101,    -- Available
									at0102,    -- Cancelled
									at0103]    -- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0098] occurrences matches {0..1} matches {    -- Study end point
						value matches {
							DV_TEXT matches {*}
							DV_URI matches {*}
						}
					}
					ELEMENT[at0106] occurrences matches {0..*} matches {    -- Image details
						value matches {
							DV_URI matches {*}
						}
					}
					allow_archetype CLUSTER[at0026] occurrences matches {0..*} matches {    -- Imaging service
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0087] occurrences matches {0..*} matches {    -- Technique
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Technique summary
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0088] occurrences matches {0..*} matches {    -- Procedure
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0062] occurrences matches {0..1} matches {    -- Procedure summary
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Structured technique/procedure
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0065] occurrences matches {0..*} matches {    -- Series details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0069] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[at0093] occurrences matches {0..*} matches {    -- Comparison study details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0094] occurrences matches {0..1} matches {    -- Study name
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0095] occurrences matches {0..1} matches {    -- Study identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0096] occurrences matches {0..1} matches {    -- Study date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0097] occurrences matches {0..*} matches {    -- Study end point
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							allow_archetype CLUSTER[at0067] occurrences matches {0..*} matches {    -- Comparison series details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_series(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {    -- Examination request details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Receiver order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_CODED_TEXT matches {*}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Requester order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0029] occurrences matches {0..*} matches {    -- Examination requested name
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0030] occurrences matches {0..*} matches {    -- Requester
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					allow_archetype CLUSTER[at0083] occurrences matches {0..1} matches {    -- Distribution list
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Imaging examination result">
					description = <"The result of an imaging examination performed on an individual, using radiological techniques.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Study name">
					description = <"The name of the imaging examination performed.">
					comment = <"For example: 'CT abdomen'; 'Doppler ultrasound of renal artery' or 'Plain chest xray'. Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Examples of candidate terminologies are the LOINC Imaging Document Codes, RadLex Playbook or SNOMED-CT. Equivalent to DiagnosticReport.code in FHIR.">
				>
				["at0006"] = <
					text = <"Structured target body site">
					description = <"Structured detail about the body site or region targetted for imaging.">
					comment = <"If the body site is specified within the text or code for 'Study name', then this field may be redundant.">
				>
				["at0008"] = <
					text = <"Imaging findings">
					description = <"Narrative description or overview of all clinical findings.">
				>
				["at0014"] = <
					text = <"Clinical summary">
					description = <"Narrative description of relevant clinical history that provides context for the examination and interpretation of results.">
					comment = <"Alternatively this data element can include an RM link to clinical information provided in the original examination request or located elsewhere within the health record.">
				>
				["at0020"] = <
					text = <"Imaging diagnosis">
					description = <"Single word, phrase or brief description representing the likely condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging differential diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\" Equivalent to DiagnosticReport.conclusionCode in FHIR.">
				>
				["at0021"] = <
					text = <"Overall impression">
					description = <"Narrative concise, clinically relevant interpretation of all imaging findings, and include a comparison with previous studies where appropriate.">
					comment = <"Also referred to as 'Opinion' or 'Conclusion'. Equivalent to DiagnosticReport.conclusion in FHIR.">
				>
				["at0023"] = <
					text = <"Comment">
					description = <"Additional narrative about the examination not captured in other fields.">
					comment = <"For example: a note that the film was given to the patient. Equivalent to DiagnosticReport.note in FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Imaging service">
					description = <"Details about the service, organisation or individual carrying out the imaging examination.">
				>
				["at0027"] = <
					text = <"Examination request details">
					description = <"Details about a single imaging examination requested.">
				>
				["at0028"] = <
					text = <"Requester order identifier">
					description = <"Unique identifier for the imaging examination order assigned by the requester.">
					comment = <"Equivalent to the HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Examination requested name">
					description = <"Identification of imaging examination requested.">
					comment = <"This can be useful in situations where the examination requested differs from the examination performed.">
				>
				["at0030"] = <
					text = <"Requester">
					description = <"Details about the clinician and/or organisation requesting the imaging examination.">
					comment = <"Equivalent to DICOM Referring Physician's Name (0008,0090)">
				>
				["at0031"] = <
					text = <"Receiver order identifier">
					description = <"Unique identifier for the imaging examination order assigned by the radiology service.">
					comment = <"Equivalent to the HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050).">
				>
				["at0041"] = <
					text = <"Structured technique/procedure">
					description = <"Additional structured details of technical and clinical aspects of capturing the image/s.">
				>
				["at0044"] = <
					text = <"Structured imaging findings">
					description = <"Structured details about the imaging examination findings targeting a specific structure or region.">
				>
				["at0045"] = <
					text = <"Report representation">
					description = <"Digital representation of the examination result.">
					comment = <"Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report. Equivalent to DiagnosticReport.presentedForm in FHIR.">
				>
				["at0046"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Confounding factors">
					description = <"Narrative description of factors, not recorded elsewhere, that may influence the examination findings and/or result.">
				>
				["at0049"] = <
					text = <"Technique summary">
					description = <"Narrative description about the radiological procedure used to capture the study.">
					comment = <"Use this data element to describe a summary of technique, including technical limitations or deviation from the normal technical procedures. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.'">
				>
				["at0055"] = <
					text = <"Target body site">
					description = <"Description of the simple body site or region targetted for imaging.">
					comment = <"If the body site is specified within the text or code for 'Study name', then this field may be redundant. Coding of 'Target body site' with a terminology, such as SNOMED CT, is desirable. Equivalent to ImagingStudy.series.bodySite in FHIR.">
				>
				["at0056"] = <
					text = <"Comparison findings">
					description = <"Narrative description about the comparison of this examination with previous similar studies.">
					comment = <"Also use this data element to indicate if no comparison images and/or reports are available. Details about the images used for comparison can be recorded using the 'Comparison study details' CLUSTER, within Protocol.">
				>
				["at0057"] = <
					text = <"Imaging quality description">
					description = <"Narrative description about the quality of the examination.">
					comment = <"For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate.">
				>
				["at0058"] = <
					text = <"Imaging differential diagnosis">
					description = <"Single word, phrase or brief description representing a possible condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnosis. Coding with a terminology such as SNOMED CT is preferred, where possible. This data element should be considered as mutually exclusive to \"Imaging diagnosis\", ie only either \"Imaging diagnosis\" or \"Imaging differential diagnosis\" in each instance of the archetype \"Imaging examination result.\"">
				>
				["at0059"] = <
					text = <"Recommendation">
					description = <"Suggestion for further imaging, investigations and/or referral, and associated rationale.">
					comment = <"This data element has 0..* occurrences to allow for more than one recommendation. Formal orders for additional imaging examination, investigation should be recorded using an INSTRUCTION archetype, such as INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Clinical indication">
					description = <"Narrative description about the reason the examination was originally requested.">
					comment = <"Also known as 'Clinical question' or 'Clinical query'.">
				>
				["at0062"] = <
					text = <"Procedure summary">
					description = <"Narrative description about the clinical procedure or other clinical considerations used to capture the study.">
					comment = <"Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered. ">
				>
				["at0063"] = <
					text = <"Imaging quality">
					description = <"Assessment about the quality of the examination.">
					comment = <"Coding of the imaging quality with a value set is recommended. For example: 'excellent, good, fair, poor, and non-diagnostic'; or 'adequate, suboptimal and inadequate'.
">
				>
				["at0065"] = <
					text = <"Series details">
					description = <"Details about a series included in this report.">
				>
				["at0067"] = <
					text = <"Comparison series details">
					description = <"Details about an imaging series being compared to the reported study.">
				>
				["at0069"] = <
					text = <"Device">
					description = <"Details about imaging device/s used to capture the study.">
				>
				["at0070"] = <
					text = <"Study date">
					description = <"Date/time when the imaging started.">
					comment = <"Equivalent to ImagingStudy.started in FHIR.">
				>
				["at0071"] = <
					text = <"Status timestamp">
					description = <"The date and/or time that ‘Overall result status’ was assigned.">
				>
				["at0072"] = <
					text = <"Overall result status">
					description = <"The status of the imaging examination result as a whole.">
					comment = <"The values have been aligned with the FHIR Diagnostic report status, with the exception of 'Entered in error' which is relevant only to a messaging-oriented paradigm. Other local codes/terms can be used via the Text 'choice'. In most use cases, only one status is applicable at any time, however this data element allows multiple occurrences for the use cases where there are a variety of types of status that need to be specified, usually within a template. Equivalent to DiagnosticReport.status in FHIR. ">
				>
				["at0073"] = <
					text = <"Registered">
					description = <"The existence of the study is registered, but no result is yet available.">
				>
				["at0074"] = <
					text = <"Partial">
					description = <"This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified.">
				>
				["at0075"] = <
					text = <"Preliminary">
					description = <"Verified early results are available, but not all results are final. This is a sub-category of 'Partial'.">
				>
				["at0076"] = <
					text = <"Final">
					description = <"The result is complete and verified by an authorised person.">
				>
				["at0077"] = <
					text = <"Amended">
					description = <"Subsequent to being final, the result has been modified.">
				>
				["at0078"] = <
					text = <"Corrected">
					description = <"Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'.">
				>
				["at0079"] = <
					text = <"Appended">
					description = <"Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'.">
				>
				["at0080"] = <
					text = <"Cancelled">
					description = <"The result is unavailable because the test was not started or not completed (also sometimes called 'aborted').">
				>
				["at0083"] = <
					text = <"Distribution list">
					description = <"Details of additional clinicians or organisations who require a copy of the examination result.">
					comment = <"The 'Distribution list' is for information-only, and that the primary recipient of the report is the person intended to act on the information.">
				>
				["at0087"] = <
					text = <"Technique">
					description = <"Name of the radiological procedure used to capture the study.">
					comment = <"Coding of the 'Technique' with a terminology is permitted. Multiple occurrences allow for identification of one or more techniques.">
				>
				["at0088"] = <
					text = <"Procedure">
					description = <"Identification of the clinical procedure or management used to capture the study.">
				>
				["at0090"] = <
					text = <"Unknown">
					description = <"The status of the result is not known.">
				>
				["at0091"] = <
					text = <"Modality">
					description = <"The type of imaging device, process or method that originally acquired or produced the data used to create the images in the study.">
					comment = <"For example: 'XRay', 'CT scan', 'ultrasound', 'MRI', 'bone densitometry', 'tomography', 'mammography', 'nuclear medicine', 'PET', 'angiography', and 'fluoroscopy'. This data element may be redundant if the modality is clearly represented in the 'Study name'. Alternatively it may also be possible to autopopulate the 'Modality' from a coded 'Study name'. Coding of the 'Modality' with a value set is recommended, such as the DICOM CID 29 Acquisition Modality value set. Equivalent to ImagingStudy.modality in FHIR and Modality (0008, 0060) in DICOM.">
				>
				["at0092"] = <
					text = <"Study instance identifier">
					description = <"Unique identifier for the imaging study assigned by the imaging device.">
					comment = <"Equivalent to Study Instance UID (0020,000D) in DICOM and ImagingStudy.identifier in FHIR.">
				>
				["at0093"] = <
					text = <"Comparison study details">
					description = <"Details about images from a prior study used for comparison to the reported study.">
				>
				["at0094"] = <
					text = <"Study name">
					description = <"The name of the comparison imaging examination performed.">
				>
				["at0095"] = <
					text = <"Study identifier">
					description = <"Unique identifier for the comparison imaging study.">
				>
				["at0096"] = <
					text = <"Study date">
					description = <"Date and time the comparison examination started.">
				>
				["at0097"] = <
					text = <"Study end point">
					description = <"Digital location of the comparison imaging content and metadata.">
					comment = <"This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR.">
				>
				["at0098"] = <
					text = <"Study end point">
					description = <"Digital location of the imaging content and metadata.">
					comment = <"This location identifies the network service providing access (for storage, viewing, querying, or retrieval) for the study. Equivalent to ImagingStudy.endpoint in FHIR.">
				>
				["at0099"] = <
					text = <"Study status">
					description = <"The current state of the imaging study.">
					comment = <"Equivalent to ImagingStudy.status in FHIR.">
				>
				["at0100"] = <
					text = <"Registered">
					description = <"The existence of the imaging study is registered, but there is nothing yet available.">
				>
				["at0101"] = <
					text = <"Available">
					description = <"At least one instance has been associated with this imaging study.">
				>
				["at0102"] = <
					text = <"Cancelled">
					description = <"The imaging study is unavailable because the study was not started or not completed.">
				>
				["at0103"] = <
					text = <"Unknown">
					description = <"The status is not known by the radiology source system.">
				>
				["at0104"] = <
					text = <"Report identifier">
					description = <"Unique identifier for the imaging report assigned by the radiology service.">
					comment = <"As one instance of this archetype is used per study, this identifier can be used to connect more than one imaging study within a single combined report. Equivalent to DiagnosticReport.identifier in FHIR. ">
				>
				["at0105"] = <
					text = <"Study description">
					description = <"Radiology service-generated description or classification of the study.">
					comment = <"Equivalent to ImagingStudy.description in FHIR and Study description (0008, 1030) in DICOM. Possible DICOM examples: MR BREAS, UNIT; NEURO^HEAD; or MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Image details">
					description = <"Link to full details of any imaging carried out during the study.">
					comment = <"Typically referencing images used the DICOM enabled modalities, but this data field provides an alternative method to access the images. Equivalent to DiagnosticReport.imagingStudy in FHIR.">
				>
				["at0107"] = <
					text = <"Stabilising appliance">
					description = <"Identification of a stabilising appliance in use.">
					comment = <"For example: braces, casts, external fixation.">
				>
				["at0108"] = <
					text = <"Position">
					description = <"Position of the individual during the imaging examination.">
					comment = <"For example: standing or lying.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Bildediagnostisk svar">
					description = <"Resultatet av en bildediagnostisk undersøkelse av et individ, og som er gjort ved radiologiske metoder.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Uspesifisert hendelse">
					description = <"Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i et templat eller i en applikasjon.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Undersøkelsesnavn">
					description = <"Navnet på den utførte bildediagnostiske undersøkelsen.">
					comment = <"For eksempel: \"CT abdomen\", \"Doppler ultralyd av nyrearterier\", \"Røntgen thorax\". Det anbefales sterkt å bruke en terminologi der det er mulig, fortrinnsvis en kode som både spesifiserer modalitet og anatomisk lokalisering. Mulige terminologier er NCRP, LOINC, SNOMED CT eller RadLex. Vanligvis tilsvarende FIHR DiagnosticReport.code.">
				>
				["at0006"] = <
					text = <"Strukturert kroppsdel">
					description = <"Strukturert detaljer om kroppsdelen som var målet for undersøkelsen.">
					comment = <"Hvis kroppsstedet er spesifisert av en kode i elementet \"Undersøkelsesnavn\", er dette feltet ikke nødvendig.">
				>
				["at0008"] = <
					text = <"Funn">
					description = <"Tekstlig beskrivelse eller oversikt over alle funnene.">
				>
				["at0014"] = <
					text = <"Kliniske opplysninger">
					description = <"Beskrivelse av relevant klinisk informasjon som er tilgjengelig på tolkningstidspunktet. Også kalt klinisk sammendrag.">
					comment = <"Alternativt kan dette elementet inkludere en lenke til klinisk informasjon som er gitt i den originale rekvisisjonen/henvisningen eller annet sted i den elektroniske pasientjournalen.">
				>
				["at0020"] = <
					text = <"Bildediagnostisk diagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en den sannsynlige tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn én diagnose. Det er anbefalt å bruke en terminologi dersom mulig, for eksempel SNOMED CT. Dette dataelementet bør oppfattes som gjensidig utelukkende til \"Bildediagnostisk differensialdiagnose\", altså bare enten \"Bildediagnostisk diagnose\" eller \"Bildediagnostisk differensialdiagnose\" i hver instans av arketypen \"Bildediagnostisk svar\". Tilsvarer DiagnosticReport.conclusionCode in FHIR.">
				>
				["at0021"] = <
					text = <"Resymé">
					description = <"Tekstlig presis og klinisk relevant tolkning av alle funn ved bildeundersøkelsen og inkluderer en sammenligning med tidligere granskninger dersom de er passende.">
					comment = <"Også referert til som \"Sammendrag\" eller \"Funn og undersøkelsesresultat\". Vanligvis tilsvarerende FHIR DiagnosticReport.conclusion.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligere kommentarer om undersøkelsen som ikke passer i andre felter.">
					comment = <"For eksempel et notat om at kopi av bilde eller svarrapport er gitt til pasienten. Tilsvarer DiagnosticReport.note i FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Tjenesteleverandør">
					description = <"Demografiske opplysninger om organisasjon eller person som utfører den bildediagnostiske undersøkelsen.">
				>
				["at0027"] = <
					text = <"Henvisningsdetaljer">
					description = <"Detaljer om en enkelt henvisning/rekvisisjon.">
				>
				["at0028"] = <
					text = <"Rekvirentens rekvisisjonsID">
					description = <"Rekvirentens/bestillers lokale ID for rekvisisjonen/henvisningen.">
					comment = <"Tilsvarende HL7 \"Placer Order Identifier\".">
				>
				["at0029"] = <
					text = <"Bestilt undersøkelsesnavn">
					description = <"Navn på bestilt undersøkelse.">
					comment = <"Denne kan være nyttig hvis den rekvirerte undersøkelse avviker fra undersøkelse som faktisk er utført.">
				>
				["at0030"] = <
					text = <"Rekvirent">
					description = <"Detaljer om den kliniker og/eller organisasjon som rekvirerer undersøkelsen.">
					comment = <"Tilsvarer DICOM Referring Physician's Name (0008,0090).">
				>
				["at0031"] = <
					text = <"Rekvisisjonsmottakers rekvisisjonsID">
					description = <"Henvisningens lokale identifikator/referansenummer i tjenesteyter/mottakers system, som regel tildelt av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer HL7 Filler Order Identifier or DICOM Accession number attribute (0008,0050).">
				>
				["at0041"] = <
					text = <"Strukturert teknikk/prosedyre">
					description = <"Ytterligere strukturerte detaljer med tekniske eller kliniske aspekter knyttet til bildetakingen.">
				>
				["at0044"] = <
					text = <"Strukturerte funn">
					description = <"Strukturerte detaljer om funn ved bruk av bildeundersøkelse i spesifikke regioner eller strukturer i kroppen.">
				>
				["at0045"] = <
					text = <"Presentasjon av rapporten">
					description = <"Digital versjon av svarrapporten.">
					comment = <"Flere formater tillates, men innholdet i de forskjellige formatene må representere det samme innholdet. For eksempel en pdf versjon av rapporten. Tilsvarer DiagnosticReport.presentedForm i FHIR.">
				>
				["at0046"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0047"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0048"] = <
					text = <"Konfunderende faktorer">
					description = <"Tekstlig beskrivelse av faktorer som kan påvirke undersøkelsesfunn og/eller tolkning, og som ikke er registrert annet sted.">
				>
				["at0049"] = <
					text = <"Sammendrag av teknikk">
					description = <"Tekstlig beskrivelse av den radiologiske prosedyren som er brukt for å gjennomføre undersøkelsen.">
					comment = <"Bruk dette dataelementet for å gi en oversikt over teknikk, inkludert tekniske begrensninger eller avvik fra normal teknikk. For eksempel: \"5 mm aksiale bilder fra lungebasene gjennom skambensymfysen ble tatt etter administrering av intravenøs og oral kontrast. Koronale og Sagittale omformaterte bilder ble konstruert fra kildedataene.\"">
				>
				["at0055"] = <
					text = <"Kroppsdel">
					description = <"En beskrivelse av den anatomisk regionen, organet, eller organsystemet som var målet for undersøkelsen.">
					comment = <"Hvis målområde er spesifisert av en kode i undersøkelsesnavnet, er dette feltet ikke nødvendig. Det er anbefalt å kode \"Kroppsdel\" med en terminologi, som SNOMED CT. Kroppsdelen er vanligvis ekvivalent til FHIR ImagingStudy.series.bodySite.">
				>
				["at0056"] = <
					text = <"Funn ved sammenligning">
					description = <"Tekstlig beskrivelse med sammenligning mellom denne undersøkelsen og tidligere lignende undersøkelser.">
					comment = <"Bruk også dette dataelementet for å indikere dersom ingen sammenligningsbilder og/eller rapporter er tilgjengelige. Detaljer om bildene som er brukt for sammenligning kan lagres ved å bruke det interne Clusteret \"Detaljer om sammenlignende bilder\" i protokolldelen av denne arketypen.">
				>
				["at0057"] = <
					text = <"Bildediagnostisk kvalitetsbeskrivelse">
					description = <"Tekstlig beskrivelse av kvaliteten på undersøkelsen.">
					comment = <"For eksempel om det er begrensninger ved undersøkelsen og deres konsekvens for tolkning, anbefaling om å gjenta undersøkelsen eller om alternative undersøkelsesmetoder som kan være mer formålstjenlig.">
				>
				["at0058"] = <
					text = <"Bildediagnostisk differensialdiagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en mulig tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn én differensialdiagnose. Det er anbefalt å bruke en terminologi dersom mulig, for eksempel SNOMED CT. Dette dataelementet bør oppfattes som gjensidig utelukkende til \"Bildediagnostisk diagnose\", altså bare enten \"Bildediagnostisk diagnose\" eller \"Bildediagnostisk differensialdiagnose\" i hver instans av arketypen \"Bildediagnostisk svar\".">
				>
				["at0059"] = <
					text = <"Anbefaling">
					description = <"Forslag til videre bildediagnostiske undersøkelser, granskninger og/eller henvisninger, inkludert begrunnelser knyttet til dette.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn en anbefaling. Bruk en INSTRUCTION-arketype for formelle henvisninger for ytterligere bildeundersøkelser, for eksempel INSTRUCTION.service_request (Helsetjenesteforespørsel).">
				>
				["at0061"] = <
					text = <"Problemstilling">
					description = <"Tekstlig beskrivelse av begrunnelsen for henvisningen/rekvisisjonen.">
					comment = <"Også kjent som \"Begrunnelse for rekvisisjon/henvisning\".">
				>
				["at0062"] = <
					text = <"Sammendrag av prosedyre">
					description = <"Tekstlig sammendrag om den kliniske prosedyren eller andre kliniske vurderinger som er knyttet til den radiologiske undersøkelsen.">
					comment = <"Bruk dette dataelementet for å oppsummere den kliniske prosedyren, pasientens plassering (pasientposisjon), intervensjoner (som for eksempel Valsalva manøver) og administrering av kontrast, radiofarmakum og/eller behandlinger som del av bildeundersøkelsen. For eksempel: \"80 ml Isovue-370 ble administrert intravenøst ved bruk av en bolus-sporingsteknikk sentrert på hovedlungearterien\". Dataelementet kan også bli brukt til å registrere en oversikt over andre problemer, komplikasjoner eller overfølsomhetsreaksjoner som har oppstått.">
				>
				["at0063"] = <
					text = <"Bildediagnostisk kvalitet">
					description = <"Vurdering om kvaliteten på undersøkelsen.">
					comment = <"Det anbefales å kode undersøkelseskvalitet med et verdi- eller kodesett, for eksempel \"Utmerket\", \"God\", \"Bra\", \"Dårlig\" og \"Ikke diagnostiserbar\", eller \"Adekvat\", \"Suboptimal\" og \"Inadekvat\".">
				>
				["at0065"] = <
					text = <"Detaljer om bildeserie/r">
					description = <"Detaljer om en eller flere bildediagnostisk serier som er inkludert i denne resultatrapporten.">
				>
				["at0067"] = <
					text = <"Detaljer om sammenlignende serier">
					description = <"Detaljer om en serie av bildeundersøkelser som det sammenliknes med.">
				>
				["at0069"] = <
					text = <"Utstyr">
					description = <"Detaljer om utstyr eller apparat/-er som er brukt ved den bildediagnostiske undersøkelsen.">
				>
				["at0070"] = <
					text = <"Undersøkelsesdato">
					description = <"Dato/Tid da den bildedannende undersøkelsen startet.">
					comment = <"Tilsvarer FIHR 'ImagingStudy.started.">
				>
				["at0071"] = <
					text = <"Tidsangivelse for overordnet resultatstatus">
					description = <"Dato og/eller tid \"Overordnet resultatstatus\" ble gitt. (Tidspunkt for siste statusendring).">
				>
				["at0072"] = <
					text = <"Status på undersøkelsesresultatet">
					description = <"Den overordnede statusen for hele undersøkelsesresultatet.">
					comment = <"Verdisetettet her er samordnet med FHIR Diagnostic report status, med unntak av koden \"Entered in error\" som bare er relevant i et meldingsorientert paradigme. Andre lokale koder eller termer kan brukes ved å bruke datatypen \"Fri eller kodet tekst\". I de fleste tilfellene er det bare passende å bruke en statuskode. Dataelementet er imidlertid åpent for flere forekomster for å understøtte bruksområdene der statuser for forskjellige aspekter av resultatet er delt opp i flere elementer. Et eksempel på dette er de norske standard svarrapport av medisinske tjenester v1.4 (HIS 80822:2014), der status for undersøkelsesresultat (Tjenestetype) og status/steg i arbeidsflyt håndteres separat.

Dette elementet tilsvarer FHIR Diagnostic report status.">
				>
				["at0073"] = <
					text = <"Registrert">
					description = <"Undersøkelsen er registrert i radiologisystemet (RIS), men resultatet er ikke tilgjengelig ennå.">
				>
				["at0074"] = <
					text = <"Ufullstendig">
					description = <"Undersøkelsen er delvis (dvs initalt, foreløpig eller preliminært) besvart. Resultatet kan være ukomplett eller ubekreftet.">
				>
				["at0075"] = <
					text = <"Foreløpig">
					description = <"Verifiserte tidlige svar er tilgjengelige, men ikke alle svar er endelige. Dette er en underkategori av \"Ufullstendig\".">
				>
				["at0076"] = <
					text = <"Endelig">
					description = <"Undersøkelsesresultatene er komplett og bekreftet av ansvarlig(e) person(er).">
				>
				["at0077"] = <
					text = <"Revidert">
					description = <"Undersøkelsesresultatet har blitt modifisert etter å ha vært i status \"Endelig\".">
				>
				["at0078"] = <
					text = <"Korrigert">
					description = <"Undersøkelsesresultatet har blitt modifisert, for å rette en feil, etter å ha vært i status \"Endelig\". Dette er en underkategori til \"Revidert\".">
				>
				["at0079"] = <
					text = <"Tillegg">
					description = <"Nytt innhold har blitt lagt til i undersøkelsesresultatet etter at resultatet har hatt status \"Endelig\". Det eksisterende innholdet er uendret. Dette er en underkategori av \"Revidert\".">
				>
				["at0080"] = <
					text = <"Kansellert">
					description = <"Svaret er utilgjengelig fordi analysen ikke ble påbegynt eller ferdigstilt (også kalt \"avbrutt\").">
				>
				["at0083"] = <
					text = <"Kopimottakere">
					description = <"Detaljert informasjon knyttet til kliniker eller organisatorisk enhet som skal motta en kopi av den bildediagnostiske resultatet.">
					comment = <"Personene eller organisasjonene på distribusjonslisten mottar svaret kun til informasjon, og det er hovedmottakeren av rapporten som forventes å agere på den.">
				>
				["at0087"] = <
					text = <"Teknikk">
					description = <"Navn på den radiologiske prosedyren som er brukt for å gjennomføre undersøkelsen.">
					comment = <"Koding av 'Teknikk' med en terminologi er tillatt. Flere forekomster av dataelementet gjør det mulig å identifisere flere enn en teknikk.">
				>
				["at0088"] = <
					text = <"Prosedyre">
					description = <"Identifisering av den kliniske prosedyren eller håndteringen som er brukt som en del av den bildediagnostiske undersøkelsen.">
				>
				["at0090"] = <
					text = <"Ukjent">
					description = <"Resultatstatus er ukjent.">
				>
				["at0091"] = <
					text = <"Modalitet">
					description = <"Typen bildedannende utstyr, prosess eller metode som opprinnelig innhentet eller produserte dataene som ble brukt til å lage bildene i studien.">
					comment = <"For eksempel: \"Rtg\", \"CT\", \"Ultralyd\", \"UL\", \"MR\", \"Bentetthetsmåling\", \"Tomografi\", \"Mammografi\", \"Nukleærmedisin\", \"PET\", \"Angiografi\" og \"Fluoroskopi\". 
Dette dataelementet kan være overflødig hvis modaliteten er tydelig representert i 'Undersøkelsesnavnet'. Alternativt kan det også være mulig å autofylle 'Modalitet' fra et kodet 'Undersøkelsesnavn'. Koding av 'Modalitet' med et verdisett anbefales. For eksempel er verdien i kodeverket \"DICOM CID 29 Acquisition Modality\". Vanligvis tilsvarer dette ImagingStudy.modality i FHIR og Modality (0008, 0060) i DICOM.">
				>
				["at0092"] = <
					text = <"Undersøkelses ID">
					description = <"Unik identifikator for bildeundersøkelsen som tildeles av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer DICOM Study Instance UID (0020,000D) og ImagingStudy.identifier i FHIR.">
				>
				["at0093"] = <
					text = <"Detaljer om sammenlignende bilder">
					description = <"Detaljer om bilder fra en tidligere bildediagnostisk undersøkelse som er bruk til sammenlikning.">
				>
				["at0094"] = <
					text = <"Undersøkelsesnavn">
					description = <"Navnet på den bildediagnostiske bildeundersøkelsen som det sammenlignes med.">
				>
				["at0095"] = <
					text = <"Undersøkelses ID">
					description = <"Den unike identifikatoren til den bildediagnostisk undersøkelsen det sammenlignes med.">
				>
				["at0096"] = <
					text = <"Undersøkelsesdato">
					description = <"Dato og tid den bildediagnostiske undersøkelsen det sammenlignes med.">
				>
				["at0097"] = <
					text = <"Lagringssted">
					description = <"Det digitale lagringsstedet for bildeinnhold og metadata.">
					comment = <"Dette lagringsstedet identifiserer nettverkstjenesten som gir tilgang til undersøkelsen for lagring, visning, spørring eller henting. Tilsvarer ImagingStudy.endpoint i FHIR.">
				>
				["at0098"] = <
					text = <"Lagringssted">
					description = <"Det digitale lagringsstedet for bildeinnhold og metadata.">
					comment = <"Dette lagringsstedet identifiserer nettverkstjenesten som gir tilgang til undersøkelsen for lagring, visning, spørring eller henting. Tilsvarer ImagingStudy.endpoint i FHIR.">
				>
				["at0099"] = <
					text = <"Undersøkelsesstatus">
					description = <"Den nåværende statusen av den bildediagnostiske undersøkelsen i RIS/PACS.">
					comment = <"Tilsvarer ImagingStudy.status i FHIR.">
				>
				["at0100"] = <
					text = <"Registrert">
					description = <"Den bildediagnostiske undersøkelsen er registrert i radiologisystemet (RIS), men resultatet er ikke tilgjengelig ennå.">
				>
				["at0101"] = <
					text = <"Tilgjengelig">
					description = <"Minst én forekomst er ferdig i denne bildediagnostiske undersøkelsen.">
				>
				["at0102"] = <
					text = <"Kansellert">
					description = <"Den bildediagnostiske undersøkelsen er utilgjengelig fordi undersøkelsen ikke ble startet eller fullført.">
				>
				["at0103"] = <
					text = <"Ukjent">
					description = <"Statusen er ikke kjent av radiologi informasjonssystemet (RIS) .">
				>
				["at0104"] = <
					text = <"RapportID">
					description = <"Den lokale, unike identifikator som tildeles en bildediagnostisk svarrapport av radiologisystemet (RIS/PACS).">
					comment = <"Siden en instans av denne arketypen brukes per undersøkelse, kan denne ID'en knyttes til flere enn en undersøkelse, og brukes for å koble sammen svar på flere undersøkelser i en sammenstilt svarrapport. Tilsvarer DiagnosticReport.identifier i FHIR.">
				>
				["at0105"] = <
					text = <"Klassifikasjon av undersøkelsen">
					description = <"Klassifikasjon eller beskrivelse av undersøkelsen som tildeles av radiologisystemet (RIS/PACS).">
					comment = <"Tilsvarer ImagingStudy.description i FHIR og Study description (0008, 1030) i DICOM. Eksempel på mulige verdier i DICOM er \"MR BREAS\", \"UNIT\"; \"NEURO^HEAD\"; or \"MR BREASTUNI UE\".">
				>
				["at0106"] = <
					text = <"Bildedetaljer">
					description = <"Link til alle detaljer om eventuell bilder utført under den bildediagnostiske undersøkelsen.">
					comment = <"Vanligvis brukes DICOM modaliteter for å referere til bilder, men dette datafeltet gir en alternativ metode for å få tilgang til bildene. Tilsvarer FHIRs DiagnosticReport.imagingStudy.">
				>
				["at0107"] = <
					text = <"Stabiliserende utstyr">
					description = <"Identifisering av stabiliserende utstyr som er brukt.">
					comment = <"For eksempel korsett, gips eller ekstern fiksasjon.">
				>
				["at0108"] = <
					text = <"Stilling">
					description = <"Individets posisjon ved tidspunktet for den radiologiske undersøkelsen.">
					comment = <"For eksempel stående eller sittende.">
				>
			>
		>
		["de"] = <
			items = <
				["at0000"] = <
					text = <"Bildgebendes Untersuchungsergebnis">
					description = <"Ergebnis einer bildgebenden Untersuchung, die an einer Person mit radiologischen Verfahren durchgeführt wird.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Beliebiges Ereignis">
					description = <"Standardwert, ein undefinierter/s Zeitpunkt oder Intervallereignis, das explizit im Template oder zur Laufzeit der Anwendung definiert werden kann.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Name der Studie">
					description = <"Der Name der durchgeführten bildgebenden Untersuchung.">
					comment = <"Zum Beispiel: „CT Abdomen“, „Doppler-Ultraschall der Nierenarterie“ oder „Röntgenaufnahme des Brustkorbs“. Es wird dringend empfohlen, eine Terminologie zu verwenden, vorzugsweise einen Begriff, der sowohl die Modalität als auch die anatomische Lokalisation angibt, sofern möglich. Beispiele für mögliche Terminologien sind die LOINC Imaging Document Codes, RadLex Playbook oder SNOMED-CT. Entspricht DiagnosticReport.code in FHIR.">
				>
				["at0006"] = <
					text = <"Zielgebiet strukturiert">
					description = <"Strukturierte Angaben über die Körperstelle oder -region, die für die Bildgebung bestimmt ist.">
					comment = <"Wenn die Körperstelle durch einen Code im \"Name der Studie\" angegeben wird, kann dieses Datenelement überflüssig sein.">
				>
				["at0008"] = <
					text = <"Bildgebende Befunde">
					description = <"Die Beschreibung oder Zusammenfassung aller klinischen Ergebnisse.">
				>
				["at0014"] = <
					text = <"Klinische Zusammenfassung">
					description = <"Die Beschreibung der relevanten klinischen Anamnese, die einen Kontext für die Untersuchung und Interpretation der Ergebnisse liefert.">
					comment = <"Alternativ kann dieses Datenelement einen RM-Link zu klinischen Informationen enthalten, die in der ursprünglichen Untersuchungsanfrage bereitgestellt wurden oder sich an anderer Stelle in der Gesundheitsakte befinden.">
				>
				["at0020"] = <
					text = <"Bildgebende Diagnose">
					description = <"Ein einzelnes Wort, eine Phrase oder eine kurze Beschreibung, die einen wahrscheinlichen Zustand oder eine wahrscheinliche Diagnose darstellt.">
					comment = <"Dieses Datenelement kommt mehrfach vor, um mehr als eine Diagnose zu ermöglichen. Wenn möglich, wird eine Codierung mit einer Terminologie wie SNOMED CT bevorzugt. Dieses Datenelement ist als sich gegenseitig ausschließend zur „Bildgebenden Differenzialdiagnostik“ zu betrachten, dh jeweils nur entweder „Bildgebende Diagnostik“ oder „Bildgebende Differenzialdiagnostik“ des Archetyps „Bildgebendes Untersuchungsergebnis“. Entspricht DiagnosticReport.conclusionCode in FHIR.">
				>
				["at0021"] = <
					text = <"Schlussfolgerung">
					description = <"Kurz beschriebene, klinisch relevante Interpretation aller bildgebenden Befunde und gegebenenfalls ein Vergleich zu früheren Studien.">
					comment = <"Auch als „Beurteilung“ oder „Gesamteindruck“ bezeichnet. Entspricht DiagnosticReport.conclusion in FHIR.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Zusätzliche Informationen über die Untersuchung, welche noch nicht an anderer Stelle dargestellt wurden.">
					comment = <"Zum Beispiel: eine Anmerkung, dass der Film dem Patienten gegeben wurde. Entspricht DiagnosticReport.note in FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Bildgebungsdienst">
					description = <"Angaben über den Dienst, die Organisation oder die Person, die die bildgebende Untersuchung durchführt.">
				>
				["at0027"] = <
					text = <"Details zum Untersuchungsauftrag">
					description = <"Angaben zur Anforderung einer einzelnen bildgebenden Untersuchung.">
				>
				["at0028"] = <
					text = <"Auftragskennung des Anforderers">
					description = <"Eindeutige Kennung für den Auftrag der bildgebenden Untersuchung, vom Anforderer vergeben.">
					comment = <"Entspricht der HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Name der angeforderter Untersuchung">
					description = <"Identifizierung der angeforderten bildgebenden Untersuchung.">
					comment = <"Dies kann in Situationen nützlich sein, in denen die angeforderte Untersuchung von der durchgeführten Untersuchung abweicht.">
				>
				["at0030"] = <
					text = <"Anforderer">
					description = <"Angaben zur ärzlichen Person und/oder der Organisation, die die bildgebende Untersuchung anfordert.">
					comment = <"Entspricht dem DICOM Referring Physician's Name (0008,0090).">
				>
				["at0031"] = <
					text = <"Auftragskennung des Empfängers">
					description = <"Eindeutige Kennung für den Auftrag der bildgebenden Untersuchung, vergeben durch den radiologischen Dienst.">
					comment = <"Entspricht dem HL7 Filler Order Identifier oder dem DICOM Accession number attribute (0008,0050).">
				>
				["at0041"] = <
					text = <"Technische / klinische Verfahren strukturiert">
					description = <"Zusätzliche strukturierte Informationen zu technischen und klinischen Aspekten der Aufnahme des Bildes/der Bilder.">
				>
				["at0044"] = <
					text = <"Strukturierte bildgebende Befunde">
					description = <"Strukturierte Informationen zu den bildgebenden Befunden, die auf eine bestimmte Körperstruktur oder -region abzielen.">
				>
				["at0045"] = <
					text = <"Berichtsrepräsentation">
					description = <"Digitale Darstellung des Ergebnisses der Untersuchung.">
					comment = <"Mehrere Formate sind zulässig, sie sollten jedoch gleichwertige klinische Inhalte darstellen. Zum Beispiel eine PDF-Version des Berichts. Entspricht DiagnosticReport.presentedForm in FHIR.">
				>
				["at0046"] = <
					text = <"Erweiterung">
					description = <"Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.">
					comment = <"Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Störfaktoren">
					description = <"Die Beschreibung von Faktoren, welche noch nicht an anderer Stelle erfasst werden, die Untersuchungsbefunde und / oder -ergebnisse beeinflussen können.">
				>
				["at0049"] = <
					text = <"Zusammenfassung des technischen Verfahrens">
					description = <"Beschreibung des radiologischen Verfahrens, das zur Erfassung der Studie verwendet wurde.">
					comment = <"Verwenden Sie dieses Datenelement, um eine Zusammenfassung der technischen Prozedur zu beschreiben, einschließlich technischer Einschränkungen oder Abweichungen von den normalen technischen Verfahren. Zum Beispiel: \"5-mm-Axialbilder von den Lungenbasen durch die Schambeinfuge wurden nach Verabreichung von intravenösem und oralem Kontrastmittel aufgenommen. Aus den Quelldaten wurden koronale und sagittale neu formatierte Bilder erstellt.\"">
				>
				["at0055"] = <
					text = <"Zielgebiet">
					description = <"Beschreibung der einfachen Körperstelle oder -region, die für die Bildgebung bestimmt ist.">
					comment = <"Wenn die Körperstelle im Text oder Code für „Name der Studie“ angegeben ist, kann dieses Feld überflüssig sein. Eine Codierung von „Zielgebiet“ mit einer Terminologie, wie z. B. SNOMED CT, ist wünschenswert. Entspricht ImagingStudy.series.bodySite in FHIR.">
				>
				["at0056"] = <
					text = <"Vergleichsbefunde">
					description = <"Die Beschreibung zum Vergleich dieser Untersuchung mit früheren ähnlichen Studien.">
					comment = <"Verwenden Sie dieses Datenelement auch, um anzugeben, ob keine Vergleichsbilder und/oder Berichte verfügbar sind. Details zu den zum Vergleich verwendeten Bildern können im CLUSTER „Vergleichstudie Details“ im Archetyp-Teil \"Protocol\" erfasst werden.">
				>
				["at0057"] = <
					text = <"Beschreibung der Qualität der Bildgebung">
					description = <"Die Beschreibung der Qualität der Untersuchung.">
					comment = <"Zum Beispiel: die Art jeglicher Einschränkungen und deren Auswirkung auf die Interpretation, Empfehlung zur Wiederholung der Untersuchung oder ob eine alternative Beurteilungsmethode geeigneter sein könnte.">
				>
				["at0058"] = <
					text = <"Bildgebende Differentialdiagnose">
					description = <"Ein einzelnes Wort, eine Phrase oder eine kurze Beschreibung, die einen möglichen Zustand oder eine mögliche Diagnose darstellt.">
					comment = <"Dieses Datenelement kommt mehrfach vor, um mehr als eine Angabe von Differentialdiagnose zu ermöglichen. Wenn möglich, wird eine Codierung mit einer Terminologie wie SNOMED-CT bevorzugt. Dieses Datenelement ist als sich gegenseitig ausschließend zu „Bildgebende Diagnose“ zu betrachten, also jeweils nur entweder „Bildgebende Diagnose“ oder „Bildgebende Differentialdiagnose“ in einer Instanz des Archetyps „Bildgebendes Untersuchungsergebnis“.">
				>
				["at0059"] = <
					text = <"Empfehlung">
					description = <"Vorschlag für weitere Bildgebung, Untersuchungen und/oder Überweisung und dazugehörige Begründung.">
					comment = <"Dieses Datenelement hat die Kardinalität 0..*, um mehr als eine Angabe von Empfehlung zu ermöglichen. Formelle Anordnungen für zusätzliche bildgebende Untersuchungen, Prüfungen sollten unter Verwendung eines INSTRUCTION-Archetyps wie INSTRUCTION.service_request dargestellt werden.">
				>
				["at0061"] = <
					text = <"Klinische Indikation">
					description = <"Die Beschreibung des Grundes für die ursprüngliche Untersuchungsanfrage.">
					comment = <"Auch bekannt als „Klinische Fragestellung“ oder „Klinische Frage“.">
				>
				["at0062"] = <
					text = <"Zusammenfassung der Prozedur">
					description = <"Beschreibung des klinischen Verfahrens oder andere klinische Aspekte, die zur Erfassung der Studie verwendet wurden.">
					comment = <"Verwenden Sie dieses Datenelement, um eine Zusammenfassung des klinischen Verfahrens, der Patientenpositionierung, Eingriffe wie Valsalva-Manöver und der Verabreichung von Kontrastmitteln, Radiopharmaka und/oder Behandlungen als Teil der Bildgebungsstudie zu beschreiben. Zum Beispiel: „80 ml Isovue-370 wurden intravenös unter Verwendung einer Bolus-Tracking-Technik verabreicht, die auf die Hauptpulmonalarterie zentriert war“. Es kann auch verwendet werden, um einen Überblick über aufgetretene Probleme, Komplikationen oder Nebenwirkungen darzustellen.">
				>
				["at0063"] = <
					text = <"Qualität der Bildgebung">
					description = <"Die Beurteilung der Qualität der Untersuchung.">
					comment = <"Es wird empfohlen, die Qualität der Bildgebung mit einer Wertemenge zu codieren. Zum Beispiel: „ausgezeichnet, gut, mittelmäßig, schlecht und nicht diagnostisch“ oder „angemessen, suboptimal und unzureichend“.">
				>
				["at0065"] = <
					text = <"Seriendetails">
					description = <"Details zu einer in diesem Bericht enthaltenen Serie.">
				>
				["at0067"] = <
					text = <"Details zur Vergleichsserie">
					description = <"Details zu einer Bildgebungsserie, die mit der berichteten Studie verglichen wird.">
				>
				["at0069"] = <
					text = <"Gerät">
					description = <"Details zu Bildgebungsgeräten, die zur Erfassung der Studie verwendet wurden.">
				>
				["at0070"] = <
					text = <"Zeitpunkt der Studie">
					description = <"Zeitpunkt zum Beginn der Bildgebung.">
					comment = <"Entspricht ImagingStudy.started in FHIR.">
				>
				["at0071"] = <
					text = <"Zeitstempel des Status">
					description = <"Der Zeitpunkt, an dem der „Status des Gesamtergebnisses“ bestimm wurde.">
				>
				["at0072"] = <
					text = <"Status des Gesamtergebnisses">
					description = <"Der Status des Ergebnisses der bildgebenden Untersuchung als Ganzes.">
					comment = <"Die Werte wurden mit dem Status von Diagnostic report im FHIR abgeglichen, mit Ausnahme von „Entered in error“, das nur für ein datenaustausch-orientiertes Paradigma relevant ist. Andere lokale Codes/Begriffe können über die \"Choice of\" vom Text-Datentyp verwendet werden. In den meisten Anwendungsfällen ist jeweils nur ein Status anwendbar, jedoch erlaubt dieses Datenelement mehrere Vorkommen für die Anwendungsfälle, in denen eine Vielzahl von Status-Typen angegeben werden müssen, normalerweise innerhalb eines Templates. Entspricht DiagnosticReport.status in FHIR.">
				>
				["at0073"] = <
					text = <"Registriert">
					description = <"Die Existenz der Studie ist registriert, aber es liegt noch kein Ergebnis vor.">
				>
				["at0074"] = <
					text = <"Teilweise">
					description = <"Dies ist ein Teilergebnis (z. B. Erst-, Zwischen- oder vorläufiges Ergebnis), aber die Daten können unvollständig oder nicht-verifiziert sein.">
				>
				["at0075"] = <
					text = <"Vorläufig">
					description = <"Verifizierte frühe Ergebnisse sind verfügbar, aber nicht alle Ergebnisse sind endgültig. Dies ist eine Unterkategorie von \"Teilweise\".">
				>
				["at0076"] = <
					text = <"Final">
					description = <"Das Ergebnis ist vollständig und von einer autorisierten Person verifiziert.">
				>
				["at0077"] = <
					text = <"Geändert">
					description = <"Nachdem das Ergebnis endgültig war, wurde es geändert.">
				>
				["at0078"] = <
					text = <"Korrigiert">
					description = <"Nachdem das Ergebnis endgültig war, wurde es modifiziert, um einen Fehler zu korrigieren. Dies ist eine Unterkategorie von „Geändert“.">
				>
				["at0079"] = <
					text = <"Nachgetragen">
					description = <"Nachdem das Ergebnis endgültig war, wurde es durch Hinzufügen neuer Inhalte modifiziert. Der vorhandene Inhalt bleibt unverändert. Dies ist eine Unterkategorie von „Geändert“.">
				>
				["at0080"] = <
					text = <"Abgebrochen">
					description = <"Das Ergebnis ist nicht verfügbar, weil die Untersuchung nicht gestartet oder nicht abgeschlossen wurde (manchmal auch als \"gescheitert\" bezeichnet).">
				>
				["at0083"] = <
					text = <"Verteilerliste">
					description = <"Angaben zu weiteren Ärzten oder Organisationen, die eine Kopie des Untersuchungsergebnisses benötigen.">
					comment = <"Die „Verteilerliste“ dient nur zu Informationszwecken, und der Hauptempfänger des Berichts ist die Person, die aufgrund der Informationen handeln soll.">
				>
				["at0087"] = <
					text = <"Technisches Verfahren">
					description = <"Name des radiologischen Verfahrens, das zur Erfassung der Studie verwendet wurde.">
					comment = <"Die Codierung des „Technischen Verfahrens“ mit einer Terminologie ist erlaubt. Mehrere Vorkommen ermöglichen die Identifizierung einer oder mehrerer Techniken.">
				>
				["at0088"] = <
					text = <"Klinische Prozedur">
					description = <"Identifizierung des klinischen Verfahrens oder Managements, das zur Erfassung der Studie verwendet wurde.">
				>
				["at0090"] = <
					text = <"Unbekannt">
					description = <"Der Status des Ergebnisses ist nicht bekannt.">
				>
				["at0091"] = <
					text = <"Modalität">
					description = <"Die Art des Bildgebungsgeräts, -prozesses oder -verfahrens, das ursprünglich die Daten erfasst oder erzeugt hat, die zur Erstellung der Bilder in der Studie verwendet wurden.">
					comment = <"Zum Beispiel: „Röntgen“, „CT-Scan“, „Ultraschall“, „MRT“, „Knochendichtemessung“, „Tomografie“, „Mammografie“, „Nuklearmedizin“, „PET“, „Angiografie“ und „Fluoroskopie“. Dieses Datenelement kann redundant sein, wenn die Modalität eindeutig im „Name der Studie“ dargestellt wird. Alternativ ist es möglicherweise auch möglich, die „Modalität“ automatisch aus einem codierten „Name der Studie“ auszufüllen. Es wird empfohlen, die „Modalität“ mit einem Wertesatz zu codieren, z. B. mit dem Wertesatz DICOM CID 29 Acquisition Modality. Entspricht ImagingStudy.modality in FHIR und Modality (0008, 0060) in DICOM.">
				>
				["at0092"] = <
					text = <"Studieninstanz-ID">
					description = <"Eindeutige Kennung für die vom Bildgebungsgerät zugewiesene Bildgebungsstudie.">
					comment = <"Entspricht der Study Instance UID (0020.000D) in DICOM und ImagingStudy.identifier in FHIR.">
				>
				["at0093"] = <
					text = <"Details zur Vergleichsstudie">
					description = <"Details zu Bildern aus einer früheren Studie, die zum Vergleich mit der berichteten Studie verwendet wurden.">
				>
				["at0094"] = <
					text = <"Name der Studie">
					description = <"Der Name der durchgeführten bildgebenden Vergleichsuntersuchung.">
				>
				["at0095"] = <
					text = <"Studienkennung">
					description = <"Eindeutige Kennung für die Vergleichsbildgebungsstudie.">
				>
				["at0096"] = <
					text = <"Zeitpunkt der Studie">
					description = <"Zeitpunkt zum Beginn der Vergleichsuntersuchung.">
				>
				["at0097"] = <
					text = <"Studienendpunkt">
					description = <"Digitale Position der Vergleichsbildinhalte und Metadaten.">
					comment = <"Diese Position identifiziert den Netzwerkdienst, der den Zugriff (zum Speichern, Anzeigen, Abfragen oder Abrufen) für die Studie bereitstellt. Entspricht ImagingStudy.endpoint in FHIR.">
				>
				["at0098"] = <
					text = <"Studienendpunkt">
					description = <"Digitale Position der Bildinhalte und Metadaten.">
					comment = <"Diese Position identifiziert den Netzwerkdienst, der den Zugriff (zum Speichern, Anzeigen, Abfragen oder Abrufen) für die Studie bereitstellt. Entspricht ImagingStudy.endpoint in FHIR.">
				>
				["at0099"] = <
					text = <"Studienstatus">
					description = <"Der aktuelle Status der Bildgebungsstudie.">
					comment = <"Entspricht ImagingStudy.status in FHIR.">
				>
				["at0100"] = <
					text = <"Registriert">
					description = <"Die Existenz der Bildgebungsstudie ist registriert, aber sie ist noch nichts verfügbar.">
				>
				["at0101"] = <
					text = <"Verfügbar">
					description = <"Mindestens eine Instanz wurde dieser Bildgebungsstudie zugeordnet.">
				>
				["at0102"] = <
					text = <"Abgebrochen">
					description = <"Die Bildgebungsstudie ist nicht verfügbar, weil die Studie nicht gestartet oder nicht abgeschlossen wurde.">
				>
				["at0103"] = <
					text = <"Unbekannt">
					description = <"Der Status ist im radiologischen Quellsystem nicht bekannt.">
				>
				["at0104"] = <
					text = <"Bericht-ID">
					description = <"Eindeutige Kennung für den vom Radiologiedienst vergebenen Bildgebungsbericht.">
					comment = <"Da pro Studie eine Instanz dieses Archetyps verwendet wird, kann diese Identifikation verwendet werden, um mehr als eine Bildgebungsstudie in einem einzigen kompakten Bericht zu verbinden. Entspricht DiagnosticReport.identifier in FHIR.">
				>
				["at0105"] = <
					text = <"Studienbeschreibung">
					description = <"Vom radiologischen Dienst erstellte Beschreibung oder Klassifizierung der Studie.">
					comment = <"Entspricht ImagingStudy.description in FHIR und Study description (0008, 1030) in DICOM. Mögliche DICOM-Beispiele: MR BREAS, UNIT; NEURO^HEAD oder MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Bildinformationen">
					description = <"Link zu vollständigen Details aller während der Studie durchgeführten Bildgebungen.">
					comment = <"Typischerweise wurden beim Referenzieren von Bildern die DICOM-fähigen Modalitäten verwendet, aber dieses Datenfeld bietet eine alternative Methode für den Zugriff auf die Bilder. Entspricht DiagnosticReport.imagingStudy in FHIR.">
				>
				["at0107"] = <
					text = <"Stabilisierende Instrumente">
					description = <"Identifizierung eines verwendeten Stabilisierungsinstrumentes.">
					comment = <"Zum Beispiel: Zahnspangen, Gipsverbände, externe Fixierung.">
				>
				["at0108"] = <
					text = <"Position">
					description = <"Position/Lage der Person während der bildgebenden Untersuchung.">
					comment = <"Beispiel: stehend oder liegend.">
				>
			>
		>
		["sv"] = <
			items = <
				["at0000"] = <
					text = <"Bilddiagnostiskt resultat">
					description = <"Resultatet av en bilddiagnostisk undersökning som utförs på en individ med hjälp av radiologiska tekniker.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Ospecificerad händelse">
					description = <"Ospecificerad standardhändelse vid en tidpunkt eller inom ett tidsintervall som explicit kan definieras i en mall eller genereras automatiskt av vissa IT-system.
">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Undersökningsnamn">
					description = <"Namnet på den bilddiagnostiska undersökning som har utförts.">
					comment = <"Exempel: ”CT buk”, ”Dopplerultraljud av njurartär” eller ”Slätröntgen av torax”. Kodning med en terminologi rekommenderas starkt, helst en term som anger både modalitet och anatomisk plats, där så är möjligt. Exempel på möjliga terminologier är LOINC Imaging Document Codes, RadLex Playbook eller Snomed CT. Motsvarar DiagnosticReport.code i FHIR.">
				>
				["at0006"] = <
					text = <"Strukturerat kroppsställe">
					description = <"Strukturerad detalj om kroppsstället eller regionen som är målet för den bilddiagnostiska undersökningen.">
					comment = <"Om kroppsstället anges i texten eller koden för ”Undersökningsnamn\" kan detta fält vara överflödigt.">
				>
				["at0008"] = <
					text = <"Bilddiagnostiska fynd">
					description = <"Beskrivning eller översikt över alla kliniska fynd.">
				>
				["at0014"] = <
					text = <"Klinisk sammanfattning">
					description = <"Beskrivning av relevant klinisk anamnes som ger ett sammanhang för undersökningen och tolkningen av resultaten.">
					comment = <"Alternativt kan detta fält inkludera en RM-länk till klinisk information som antingen tillhandahålls i den ursprungliga beställningen av undersökningen eller finns någon annanstans i patientjournalen.">
				>
				["at0020"] = <
					text = <"Bilddiagnostisk diagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar det troliga tillståndet eller diagnosen.">
					comment = <"Detta fält har flera förekomster för att möjliggöra mer än en diagnos. Kodning med en terminologi som Snomed CT är att föredra, där så är möjligt. Detta fält bör betraktas som ömsesidigt uteslutande i förhållande till ”Bilddiagnostisk differentialdiagnos”, dvs. endast antingen ”Bilddiagnostisk diagnos” eller ”Bilddiagnostisk differentialdiagnos” får förekomma i varje förekomst av arketypen ”Imaging examination result”. Motsvarar DiagnosticReport.conclusionCode i FHIR.">
				>
				["at0021"] = <
					text = <"Slutsats">
					description = <"Kortfattad och kliniskt relevant bedömning av alla bilddiagnostiska fynd, inklusive en jämförelse med tidigare undersökningar där så är lämpligt.
">
					comment = <"Kallas även \"Bedömning\". Motsvarar DiagnosticReport.conclusion i FHIR.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligare beskrivning av undersökningen som inte fångats i andra fält.">
					comment = <"Till exempel: en anteckning om att filmen har lämnats till patienten. Motsvarar DiagnosticReport.note i FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Bilddiagnostisk tjänst">
					description = <"Detaljer om den tjänst, organisation eller person som utför den bilddiagnostiska undersökningen.">
				>
				["at0027"] = <
					text = <"Detaljer om beställt undersökning">
					description = <"Detaljer om en enskild bilddiagnostisk undersökning som har beställts.">
				>
				["at0028"] = <
					text = <"Beställarens beställningsidentifierare">
					description = <"Unik identifierare för beställningen av en bilddiagnostisk undersökning som har tilldelats av beställarens system, vanligtvis ett kliniskt systemet.">
					comment = <"Motsvarar HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Namn på beställd undersökning">
					description = <"Namn på den beställda bilddiagnostiska undersökningen.">
					comment = <"Detta kan vara användbart i situationer där den beställda undersökningen skiljer sig från den utförda undersökningen.">
				>
				["at0030"] = <
					text = <"Beställare">
					description = <"Detaljer om den vårdpersonal eller organisation som har beställt undersökningen.">
					comment = <"Motsvarar DICOM Referring Physician's Name (0008,0090).">
				>
				["at0031"] = <
					text = <"Mottagarens beställningsidenitifierare">
					description = <"Unik identifierare för beställningen av en bilddiagnostisk undersökning som har tilldelats av mottagarens system, vanligtvis ett radiologiskt informationssystem.">
					comment = <"Motsvarar HL7 Filler Order Identifier eller DICOM Accession number attributet (0008,0050).">
				>
				["at0041"] = <
					text = <"Strukturerad teknik/åtgärd">
					description = <"Ytterligare strukturerade detaljer om tekniska och kliniska aspekter av att ta bilden/bilderna.">
				>
				["at0044"] = <
					text = <"Strukturerade bilddiagnostiska fynd">
					description = <"Strukturerade detaljer om fynd vid en bilddiagnostisk undersökning som avser en specifik struktur eller region.">
				>
				["at0045"] = <
					text = <"Utlåtande representation">
					description = <"Digital representation av undersökningsresultatet.">
					comment = <"Flera format är tillåtna, men de ska representera likvärdigt kliniskt innehåll. Till exempel en PDF-version av utlåtandet. Motsvarar DiagnosticReport.presentedForm i FHIR.">
				>
				["at0046"] = <
					text = <"Ytterligare information">
					description = <"Ytterligare information som krävs för att utvidga modellen med lokalt definierat innehåll eller för anpassning till andra referensmodeller.
">
					comment = <"Till exempel: lokala informationskrav eller ytterligare metadata för anpassning till FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Möjliga felkällor">
					description = <"Beskrivning av faktorer och felkällor, som inte har registrerats någon annanstans, som kan påverka undersökningsfynden eller -resultaten. 

">
				>
				["at0049"] = <
					text = <"Teknik sammanfattning">
					description = <"Beskrivning av den radiologiska åtgärden som har använts vid undersökningen.">
					comment = <"Använd detta fält för att beskriva en sammanfattning av tekniken, inklusive tekniska begränsningar eller avvikelser från normala tekniska åtgärder. Till exempel: \"5 mm axiala bilder från lungbaserna genom pubissymfysen togs efter administrering av intravenös och oral kontrast. Koronala och sagittala omformaterade bilder konstruerades från källdata.">
				>
				["at0055"] = <
					text = <"Kroppsställe">
					description = <"Beskrivning av kroppsstället eller regionen som är målet för den bilddiagnostiska undersökningen.
">
					comment = <"Om kroppsstället anges i texten eller koden för ”Undersökningsnamn\" kan detta fält vara överflödigt. Kodning av ”Kroppsställe” med en terminologi, t.ex. Snomed CT, är önskvärd. Motsvarar ImagingStudy.series.bodySite i FHIR.">
				>
				["at0056"] = <
					text = <"Fynd vid jämförelse">
					description = <"Beskrivning om jämförelsen av den aktuella undersökningen med tidigare liknande undersökningar.">
					comment = <"Använd även detta fält för att ange om inga jämförelsebilder eller -utlåtanden finns tillgängliga. Detaljer om de bilder som använts för jämförelse kan registreras med hjälp av ”Comparison study details” CLUSTER, inom Protocol.">
				>
				["at0057"] = <
					text = <"Beskrivning av bildkvalitet">
					description = <"Beskrivning av kvaliteten på bilderna som har tagits vid samma undersökning.">
					comment = <"Till exempel: eventuella begränsningar och deras inverkan på bedömningen; rekommendation om att undersökningen bör upprepas; eller om en alternativ bedömningsmetod kan vara lämpligare.">
				>
				["at0058"] = <
					text = <"Bilddiagnostisk differentialdiagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar ett möjligt tillstånd eller en diagnos.">
					comment = <"Detta fält har flera förekomster för att möjliggöra mer än en differentialdiagnos. Kodning med en terminologi som Snomed CT är att föredra, där så är möjligt. Detta fält bör betraktas som ömsesidigt uteslutande i förhållande till ”Bilddiagnostisk diagnos”, dvs. endast antingen ”Bilddiagnostisk diagnos” eller ”Bilddiagnostisk differentialdiagnos” får förekomma i varje förekomst av arketypen ”Imaging examination result”.">
				>
				["at0059"] = <
					text = <"Rekommendation">
					description = <"Förslag på ytterligare bildtagning, undersökningar eller remittering samt motivering till detta.

">
					comment = <"Detta fält har 0..* förekomster för att möjliggöra mer än en rekommendation. Formella beställningar på ytterligare bilddiagnostisk undersökning eller bedömning bör registreras med hjälp av en INSTRUCTION-arketype, t.ex. INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Klinisk indikation">
					description = <"Beskrivning av orsaken till att undersökningen ursprungligen beställdes.">
					comment = <"Även känd som ”klinisk fråga” eller ”klinisk frågeställning”.">
				>
				["at0062"] = <
					text = <"Åtgärd sammanfattning">
					description = <"Beskrivning av den kliniska åtgärden eller andra kliniska överväganden som har tillämpats vid undersökningen.">
					comment = <"Använd detta fält för att beskriva en sammanfattning av den kliniska åtgärden, patientpositionering, interventioner som Valsalva-manöver och administrering av kontrastmedel, radioaktiva läkemedel eller behandlingar som har tillämpats vid undersökningen. Till exempel: ”80 ml Isovue-370 administrerades intravenöst med hjälp av en bolusspårningsteknik centrerad på huvudlungartären”. Fältet kan också användas för att registrera en översikt över eventuella problem, komplikationer eller biverkningar som har uppstått.">
				>
				["at0063"] = <
					text = <"Bildkvalitet">
					description = <"Bedömning av kvaliteten på bilderna som har tagits vid samma undersökning.">
					comment = <"Kodning av bildkvaliteten med ett urval rekommenderas. Till exempel: ”utmärkt, bra, rimlig, dålig och icke-diagnostisk”; eller ”adekvat, suboptimal och otillräcklig”.">
				>
				["at0065"] = <
					text = <"Detaljer om bildserier">
					description = <"Detaljer om bildserier som ingår i detta utlåtande.">
				>
				["at0067"] = <
					text = <"Detaljer om den jämförande bildserien">
					description = <"Detaljer om bildserier från en tidigare undersökning som jämförs med bildserier från den aktuella undersökningen.">
				>
				["at0069"] = <
					text = <"Utrustning">
					description = <"Detaljer om den bilddiagnostiska utrustningen som har använts vid undersökningen.">
				>
				["at0070"] = <
					text = <"Undersökningsdatum">
					description = <"Datum och tid då undersökningen började.">
					comment = <"Motsvarar ImagingStudy.started i FHIR.">
				>
				["at0071"] = <
					text = <"Tidpunkt för status undersökningsresultat">
					description = <"Datum och tid för utfärdandet av status för undersökningsresultatet.">
				>
				["at0072"] = <
					text = <"Status undersökningsresultat">
					description = <"Status för resultatet av den bilddiagnostiska undersökningen som helhet.">
					comment = <"Värdena har anpassats till FHIR Diagnostic report status, med undantag för ”Entered in error” som endast är relevant för ett meddelandeorienterat paradigm. Andra lokala koder/termer kan användas via texten ”choice”. I de flesta användningsfall är endast en status tillämplig vid varje tillfälle, men detta fält tillåter flera förekomster för de användningsfall där det finns en mängd olika typer av status som behöver specificeras, vanligtvis inom en template. Motsvarar DiagnosticReport.status i FHIR.">
				>
				["at0073"] = <
					text = <"Registrerad">
					description = <"Undersökningen är registrerad i radiologiinformationssystemet, men resultatet finns för närvarande inte.">
				>
				["at0074"] = <
					text = <"Partiell">
					description = <"Detta är ett partiellt undersökningsresultat, dvs. initialt, interim eller preliminärt. Resultatdata kan vara ofullständiga eller obekräftade.">
				>
				["at0075"] = <
					text = <"Preliminär">
					description = <"Verifierade tidiga resultat är tillgängliga, men inte alla resultat är slutgiltiga. Detta är en underkategori av status \"Partiell\".">
				>
				["at0076"] = <
					text = <"Slutgiltig">
					description = <"Undersökningsresultatet är fullständigt och verifierat av ansvarig person alternativt autoverifierat maskinellt.">
				>
				["at0077"] = <
					text = <"Reviderad">
					description = <"Det slutgiltiga undersökningsresultatet har reviderats. Denna status har underkategorierna \"Rättad\" och \"Utökad\".">
				>
				["at0078"] = <
					text = <"Rättad">
					description = <"Det slutgiltiga undersökningsresultatet har rättats. Detta är en underkategori av \"Reviderad\".">
				>
				["at0079"] = <
					text = <"Utökat">
					description = <"Det slutgiltiga undersökningsresultatet har ändrats genom att nytt innehåll har inkluderats. Det befintliga innehållet är oförändrat. Detta är en underkategori av \"Reviderad\".">
				>
				["at0080"] = <
					text = <"Avbruten">
					description = <"Resultatet är inte tillgängligt eftersom undersökningen inte har startats eller inte slutförts.">
				>
				["at0083"] = <
					text = <"Distributionslista">
					description = <"Detaljer om ytterligare kliniker eller organisationer som behöver en kopia av undersökningsresultatet.
">
					comment = <"Personerna eller organisationerna på distributionslistan får endast utlåtandet för information, och det är huvudmottagaren av utlåtandet som förväntas agera på den.">
				>
				["at0087"] = <
					text = <"Teknik">
					description = <"Namn på den radiologiska åtgärden som har använts vid undersökningen.">
					comment = <"Kodning av ”Teknik” med en terminologi är tillåten. Flera förekomster möjliggör identifiering av en eller flera tekniker.">
				>
				["at0088"] = <
					text = <"Åtgärd">
					description = <"Namn på den kliniska åtgärden eller hantering som har använts vid undersökningen.">
				>
				["at0090"] = <
					text = <"Okänd">
					description = <"Resultatets status är inte känd.">
				>
				["at0091"] = <
					text = <"Modalitet">
					description = <"Den typ av bilddiagnostisk utrustning, process eller metod som ursprungligen samlade in eller producerade de data som användes för att skapa bilderna i undersökningen.">
					comment = <"Exempel: ”röntgen”, ”CT skanning”, ”ultraljud”, ”MR”, ”MRT”, ”bendensitometri”, ”tomografi”, ”mammografi”, ”nukleärmedicin”, ”PET”, ’angiografi’ och ”fluoroskopi”. Detta dataelement kan vara överflödigt om modaliteten är tydligt representerad i ”Undersökningsnamn”. Alternativt kan det också vara möjligt att automatiskt populera ”Modalitet” från ett kodat ”Undersökningsnamn”. Kodning av ”Modalitet” med ett urval rekommenderas, t.ex. DICOM CID 29 Acquisition Modality-urvalet. Motsvarar ImagingStudy.modality i FHIR och Modality (0008, 0060) i DICOM.">
				>
				["at0092"] = <
					text = <"Idenitifierare för undersökningen">
					description = <"Unik identifierare för den bilddiagnostiska undersökningen som har tilldelats av det radiologiska informationssystemet.">
					comment = <"Motsvarar Study Instance UID (0020,000D) i DICOM och ImagingStudy.identifier i FHIR.">
				>
				["at0093"] = <
					text = <"Detaljer om den jämförande undersökningen">
					description = <"Detaljer om bilder från en tidigare undersökning som har använts för att jämföra med bilderna från den aktuella undersökningen.">
				>
				["at0094"] = <
					text = <"Undersökningsnamn">
					description = <"Namnet på den jämförande undersökning som har utförts.">
				>
				["at0095"] = <
					text = <"Idenitifierare för undersökningen">
					description = <"Unik identifierare för den jämförande undersökningen.">
				>
				["at0096"] = <
					text = <"Undersökningsdatum">
					description = <"Datum och tid då den jämförande undersökningen började.">
				>
				["at0097"] = <
					text = <"Lagringsplats">
					description = <"Digital plats för det jämförande bilddiagnostiska innehållet och metadata.">
					comment = <"Denna plats identifierar den nätverkstjänst som tillhandahåller åtkomst (för lagring, visning, frågor eller hämtning) för undersökningen. Motsvarar ImagingStudy.endpoint i FHIR.">
				>
				["at0098"] = <
					text = <"Lagringsplats">
					description = <"Digital plats för det bilddiagnostiska innehållet och metadata.">
					comment = <"Denna plats identifierar den nätverkstjänst som tillhandahåller åtkomst (för lagring, visning, frågor eller hämtning) för undersökningen. Motsvarar ImagingStudy.endpoint i FHIR.">
				>
				["at0099"] = <
					text = <"Undersökningsstatus">
					description = <"Den aktuella statusen för den bilddiagnostiska undersökningen.">
					comment = <"Motsvarar ImagingStudy.status i FHIR.">
				>
				["at0100"] = <
					text = <"Registrerad">
					description = <"Undersökningen är registrerad i radiologiinformationssystemet, men resultatet finns för närvarande inte.">
				>
				["at0101"] = <
					text = <"Tillgänglig">
					description = <"Minst en instans har associerats med denna bilddiagnostiska undersökning.">
				>
				["at0102"] = <
					text = <"Avbruten">
					description = <"Den bilddiagnostiska undersökningen är inte tillgänglig eftersom undersökningen inte har startats eller inte slutförts.">
				>
				["at0103"] = <
					text = <"Okänd">
					description = <"Statusen är inte känd i radiologiinformationssystemet.">
				>
				["at0104"] = <
					text = <"Idenitifierare för utlåtandet">
					description = <"Unik identifierare för det bilddiagnostiska utlåtandet som har tilldelats av det radiologiska informationssystemet.">
					comment = <"Eftersom en instans av denna arketyp används per undersökning kan denna identifierare användas för att koppla samman mer än en bilddiagnostisk undersökning inom ett utlåtande. Motsvarar DiagnosticReport.identifier i FHIR.">
				>
				["at0105"] = <
					text = <"Beskrivning av undersökning">
					description = <"Beskrivning eller klassificering av den bilddiagnostisk undersökningen som har tilldelats av det radiologiska informationssystemet.">
					comment = <"Motsvarar ImagingStudy.description i FHIR och Study description (0008, 1030) i DICOM. Möjliga DICOM-exempel: MR BREAS, UNIT; NEURO^HEAD; eller MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Bilddetaljer">
					description = <"Länk till fullständig information om alla bilder som har tagits vid undersökningen.">
					comment = <"Vanligtvis används de DICOM-aktiverade modaliteterna för att referera till bilder, men detta fält ger en alternativ metod för att komma åt bilderna. Motsvarar FHIR:s DiagnosticReport.imagingStudy.">
				>
				["at0107"] = <
					text = <"Stabiliserande anordning">
					description = <"Identifiering av en stabiliseringsanordning som används.">
					comment = <"Till exempel: tandställning, gips, extern fixering.">
				>
				["at0108"] = <
					text = <"Position">
					description = <"Individens position under den bilddiagnostisk undersökningen.">
					comment = <"Till exempel: stående eller liggande.">
				>
			>
		>
		["ca"] = <
			items = <
				["at0000"] = <
					text = <"Resultat de l'examen d'imatge">
					description = <"El resultat d'un examen d'imatge realitzat en un pacient, utilitzant tècniques radiològiques.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Qualsevol esdeveniment">
					description = <"Per defecte, un punt no especificat en l'esdeveniment de temps o interval que es pot definir explícitament en una plantilla o en temps d'execució.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Nom de l'estudi">
					description = <"El nom de l'examen d'imatge realitzat.">
					comment = <"Per exemple: 'TAC d'abdomen'; 'ecografia Doppler d'artèria renal' o 'radiografia simple de tòrax'. És molt recomanable el codi amb una terminologia, preferiblement un terme que especifiqui tant la modalitat com la ubicació anatòmica, quan sigui possible. Exemples de terminologies són els codis de documents d'imatge LOINC, RadLex Playbook o SNOMED-CT. Equivalent a DiagnosticReport.code a FHIR.">
				>
				["at0006"] = <
					text = <"Part del cos estructurada">
					description = <"Detall estructurat sobre el lloc o regió del cos destinat a la imatge.">
					comment = <"Si el lloc del cos s'especifica dins del text o codi per a «nom de l'estudi», aquest camp pot ser redundant.">
				>
				["at0008"] = <
					text = <"Resultats d'imatge">
					description = <"Descripció o resum narratiu de tots els resultats clínics.">
				>
				["at0014"] = <
					text = <"Resum clínic">
					description = <"Descripció narrativa de la història clínica rellevant que proporciona context per a l'examen i interpretació de resultats.">
					comment = <"Alternativament aquest element pot incloure un enllaç RM a la informació clínica proporcionada en la sol·licitud d'examen original o situada en un altre lloc dins de l'historial de salut.">
				>
				["at0020"] = <
					text = <"Diagnòstic d'imatge">
					description = <"Paraula única, frase o breu descripció que representi una possible condició o diagnòstic.">
					comment = <"Aquest element de dades té múltiples ocurrències per permetre més d'un diagnòstic diferencial. Es prefereix la codificació amb una terminologia com SNOMED CT, sempre que sigui possible. Aquest element de dades s'ha de considerar mútuament excloent per al \"diagnòstic diferencial d'imatges\", és a dir, només \"diagnòstic d'imatges\" o \"diagnòstic diferencial d'imatges\" en cada instància de l'arquetip \"resultat de l'examen d'imatges\". Equivalent a DiagnosticReport.conclusionCode a FHIR.">
				>
				["at0021"] = <
					text = <"Impressió general">
					description = <"Narrativa concisa, interpretació clínicament rellevant de totes les troballes a les imatges, i inclouen una comparació amb estudis previs si escau.">
					comment = <"També es coneix com a \"Opinió\" o \"Conclusió\". Equivalent a DiagnosticReport.conclusion en FHIR.">
				>
				["at0023"] = <
					text = <"Comentari">
					description = <"Informació addicional sobre l'examen no capturat en altres camps.">
					comment = <"Per exemple: una nota sobre que el pacient estava cobert amb alguna peça. Equivalent a DiagnosticReport.note a FHIR.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Servei de diagnòstic per la imatge">
					description = <"Detalls sobre el servei, organització o personal que realitza l'examen d'imatge.">
				>
				["at0027"] = <
					text = <"Detalls de la sol·licitud d'examen">
					description = <"Detalls sobre un sol examen d'imatge sol·licitat.">
				>
				["at0028"] = <
					text = <"Identificador de comanda del receptor">
					description = <"Identificador únic per a l'ordre d'examen d'imatge assignada pel sol·licitant.">
					comment = <"Equivalent a HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Nom sol·licitat per a l'examen">
					description = <"Identificació de l'examen d'imatge sol·licitat.">
					comment = <"Pot ser útil en situacions en què l'examen sol·licitat difereix de l'examen realitzat.">
				>
				["at0030"] = <
					text = <"Sol·licitant">
					description = <"Detalls sobre el professional d’atenció sanitària i/o organització que sol·licita l'examen d'imatge.">
					comment = <"Equivalent a DICOM Referring Physician's Name (0008,0090)">
				>
				["at0031"] = <
					text = <"Identificador de comanda del receptor">
					description = <"Identificador únic per a l'ordre d'exploració d'imatges assignada pel servei de radiologia.">
					comment = <"Equivalent a HL7 Placer Order Identifier o atribut de número d'adhesió DICOM (0008,0050).">
				>
				["at0041"] = <
					text = <"Tècnica/procediment estructurat">
					description = <"Detalls estructurats addicionals d'aspectes tècnics i clínics de la captura de la imatge/s.">
				>
				["at0044"] = <
					text = <"Resultats d'imatge estructurats">
					description = <"Detalls estructurats sobre els resultats de l'examen d'imatges dirigits a una estructura o regió específica.">
				>
				["at0045"] = <
					text = <"Presentació de l'informe">
					description = <"Presentació digital del resultat de l'examen.">
					comment = <"Es permeten múltiples formats, però han de representar un contingut clínic equivalent. Per exemple, una versió en pdf de l'informe. Equivalent a DiagnosticReport.presentedForm a FHIR.">
				>
				["at0046"] = <
					text = <"Extensió">
					description = <"Informació addicional necessària per a expandir el model amb el contingut local o per alinear-se amb altres models/formalismes de referència.">
					comment = <"Per exemple: requisits locals d'informació o metadades addicionals per alinear-se amb equivalents de FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Factors de confusió">
					description = <"Descripció narrativa de factors, no registrats en altres llocs, que poden influir en els resultats de l'examen i/o el resultat.">
				>
				["at0049"] = <
					text = <"Resum tècnic">
					description = <"Descripció narrativa sobre el procediment radiològic utilitzat per capturar l'estudi.">
					comment = <"Utilitzar aquest element de dades per descriure un resum de la tècnica, incloent limitacions tècniques o desviació dels procediments tècnics normals. Per exemple: «5 mm imatges axials de les bases pulmonars a través de la símfisi púbica van ser adquirides seguint l'administració del contrast intravenós i oral. Les imatges reformatades coronal i sagital es van construir a partir de les dades d'origen.»">
				>
				["at0055"] = <
					text = <"Part del cos">
					description = <"Descripció del lloc o regió del cos simple destinat a la imatge.">
					comment = <"Si la part del cos s'especifica dins del text o codi per a «nom de l'estudi», aquest camp pot ser redundant. És desitjable la codificació del \"lloc del cos de l'artefacte\" amb una terminologia, com SNOMED CT. Equivalent a ImagingStudy.series.bodySite a FHIR.">
				>
				["at0056"] = <
					text = <"Comparació de resultats">
					description = <"Descripció narrativa sobre la comparació d'aquest examen amb estudis similars anteriors.">
					comment = <"Utilitzar també aquest element per indicar si no hi ha imatges de comparació i/o informes disponibles. Els detalls sobre les imatges utilitzades per a la comparació es poden registrar utilitzant el CLUSTER 'Detalls d'estudi de comparació', dins del Protocol.">
				>
				["at0057"] = <
					text = <"Descripció de la qualitat de la imatge">
					description = <"Descripció narrativa sobre la qualitat de l'examen.">
					comment = <"Per exemple: la naturalesa de les limitacions i el seu impacte en la interpretació; la recomanació de repetir l'examen; o si un mètode alternatiu d'avaluació podria ser més adequat.">
				>
				["at0058"] = <
					text = <"Imatge de diagnòstic diferencial">
					description = <"Paraula única, frase o breu descripció que representi una possible condició o diagnòstic.">
					comment = <"Aquest element té múltiples ocurrències per permetre més d'un diagnòstic diferencial. Es prefereix la codificació amb una terminologia com SNOMED CT, sempre que sigui possible. Aquest element de dades s'ha de considerar mútuament excloent al \"diagnòstic d'imatges\", és a dir, només \"diagnòstic d'imatges\" o \"diagnòstic diferencial d'imatges\" en cada instància de l'arquetip \"resultat de l'examen d'imatges\".">
				>
				["at0059"] = <
					text = <"Recomanació">
					description = <"Suggeriment per a imatges, investigacions i/o derivacions, i raons associades.">
					comment = <"Aquest element té ocurrències 0..* per a permetre més d'una recomanació. Ordres formals per a l'exploració d'imatges addicionals, la investigació s'ha de registrar utilitzant un arquetip d'instrucció, com INSTRUCTION.service.request.">
				>
				["at0061"] = <
					text = <"Indicació clínica">
					description = <"Descripció narrativa sobre la raó per la qual es va sol·licitar originalment l'examen.">
					comment = <"També es coneix com a \"qüestió clínica\" o \"consulta clínica\".">
				>
				["at0062"] = <
					text = <"Resum del procediment">
					description = <"Descripció narrativa sobre el procediment clínic o altres consideracions clíniques utilitzades per capturar l'estudi.">
					comment = <"Utilitzar aquest element de dades per descriure un resum del procediment clínic, el posicionament del pacient, intervencions com la maniobra de Valsalva, i l'administració d'agent de contrast, radiofàrmacs i/o tractaments com a part de l'estudi d'imatge. Per exemple: \"80 ml d'Isovue-370 es va administrar per via intravenosa utilitzant una tècnica de rastreig de bolus centrat en l'artèria pulmonar principal\". També es pot utilitzar per registrar una visió general de qualsevol problema, complicacions o reaccions adverses trobades.">
				>
				["at0063"] = <
					text = <"Qualitat de la imatge">
					description = <"Avaluació sobre la qualitat de l'examen.">
					comment = <"Es recomana la codificació de la qualitat de la imatge amb un conjunt de valors. Per exemple: \"excel·lent, bo, suficient, pobre i sense diagnòstic\"; o \"adequat, subòptim i inadequat\".">
				>
				["at0065"] = <
					text = <"Detalls de la sèrie">
					description = <"Detalls sobre una sèrie inclosa en aquest informe.">
				>
				["at0067"] = <
					text = <"Comparació dels detalls de la sèrie">
					description = <"Detalls sobre una sèrie d'imatges que es compara amb l'estudi reportat.">
				>
				["at0069"] = <
					text = <"Dispositiu">
					description = <"Detalls sobre el/s dispositiu/s d'imatge que utilitzat/s per capturar l'estudi.">
				>
				["at0070"] = <
					text = <"Data de l'estudi">
					description = <"Data/hora en què es va iniciar l'estudi d'imatge.">
					comment = <"Equivalent a ImagingStudy.started a FHIR.">
				>
				["at0071"] = <
					text = <"Marca de data/hora">
					description = <"La data i/o l'hora en què es va assignar l'«Estat global del resultat».">
				>
				["at0072"] = <
					text = <"Estat global del resultat">
					description = <"L'estat del resultat de l'examen d'imatge en el seu conjunt.">
					comment = <"Els valors s'han alineat amb l'estat de l'informe de diagnòstic de la FHIR, amb l'excepció de \"Introduït per error\", que només és rellevant per a un paradigma orientat a missatgeria. Altres codis/termes locals es poden utilitzar a través del text 'elecció'. En la majoria dels casos d'ús, només un estat és aplicable en qualsevol moment, però aquest element de dades permet múltiples ocurrències per als casos d'ús on hi ha una varietat de tipus d'estat que cal especificar, normalment dins d'una plantilla. Equivalent a DiagnosticReport.status a FHIR.">
				>
				["at0073"] = <
					text = <"Registrat">
					description = <"L'estudi està registrat, però encara no hi ha cap resultat disponible.">
				>
				["at0074"] = <
					text = <"Parcial">
					description = <"Aquest és un resultat parcial (p. ex. inicial, provisional o preliminar), però les dades poden ser incompletes o no verificades.">
				>
				["at0075"] = <
					text = <"Preliminar">
					description = <"Hi ha disponibles resultats primerencs verificats, però no tots els resultats són definitius. Aquesta és una subcategoria de «Parcial».">
				>
				["at0076"] = <
					text = <"Final">
					description = <"El resultat està complet i verificat per una persona autoritzada.">
				>
				["at0077"] = <
					text = <"Modificat">
					description = <"Modificat després del resultat final.">
				>
				["at0078"] = <
					text = <"Corregit">
					description = <"Corregit després del resultat final per corregir un error. Aquesta és una subcategoria de «Modificat».">
				>
				["at0079"] = <
					text = <"Apèndix">
					description = <"Modificat afegint contingut nou després del resultat final. El contingut existent no ha canviat. Aquesta és una subcategoria de «Modificat».">
				>
				["at0080"] = <
					text = <"Cancel·lat">
					description = <"El resultat no està disponible perquè la prova no s'ha iniciat o no s'ha completat (també de vegades anomenada 'avortada').">
				>
				["at0083"] = <
					text = <"Llista de distribució">
					description = <"Detalls dels professionals d’atenció sanitària o organitzacions addicionals que requereixen una còpia del resultat de l'examen.">
					comment = <"La \"llista de distribució\" és només d'informació, i que el destinatari principal de l'informe és la persona que pretén actuar sobre la informació.">
				>
				["at0087"] = <
					text = <"Tècnica">
					description = <"Nom del procediment radiològic utilitzat per capturar l'estudi.">
					comment = <"Es permet la codificació de la \"Tècnica\" amb terminologia. Les aparicions múltiples permeten identificar una o més tècniques.">
				>
				["at0088"] = <
					text = <"Procediment">
					description = <"Identificació del procediment o gestió clínica utilitzada per capturar l'estudi.">
				>
				["at0090"] = <
					text = <"Desconegut">
					description = <"No es coneix l'estat del resultat.">
				>
				["at0091"] = <
					text = <"Modalitat">
					description = <"El tipus de dispositiu d'imatge, procés o mètode que originalment va adquirir o produir les dades utilitzades per crear les imatges en l'estudi.">
					comment = <"Per exemple: 'radiografia', 'TAC', 'ultrasò', 'RM', 'densitometria de l'os', 'tomografia', 'mamografia', 'medicina nuclear', 'PET', 'angiografia' i 'fluoroscòpia'. Aquest element pot ser redundant si la modalitat està clarament representada en el \"nom de l'estudi\". Alternativament, també pot ser possible autopopular la 'Modalitat' d'un 'nom d'estudi' codificat. Es recomana la codificació de la 'Modalitat' amb un conjunt de valors, com el conjunt de valors de Modalitat d'Adquisició DICOM CID 29. Equivalent a ImagingStudy.modality en FHIR i Modality (0008, 0060) en DICOM.
">
				>
				["at0092"] = <
					text = <"Identificador d'instància d'estudi">
					description = <"Identificador únic per a l'estudi d'imatges assignat pel dispositiu d'imatges.">
					comment = <"Equivalent a la Instància d'Estudi UID (0020,000D) en DICOM i ImagingStudy.identifier en FHIR.">
				>
				["at0093"] = <
					text = <"Comparació dels detalls de l'estudi">
					description = <"Detalls sobre les imatges d'un estudi previ utilitzat per a la comparació amb l'estudi informat.">
				>
				["at0094"] = <
					text = <"Nom de l'estudi">
					description = <"El nom de l'examen de comparació d'imatges realitzat.">
				>
				["at0095"] = <
					text = <"Identificador de l'estudi">
					description = <"Identificador únic per a l'estudi d'imatges de comparació.">
				>
				["at0096"] = <
					text = <"Data de l'estudi">
					description = <"Data i hora en què va començar l'examen de comparació.">
				>
				["at0097"] = <
					text = <"Punt final de l'estudi">
					description = <"Ubicació digital del contingut i les metadades de la comparació d'imatges.">
					comment = <"Aquesta ubicació identifica el servei de xarxa que proporciona accés (per a emmagatzematge, visualització, consulta o recuperació) per a l'estudi. Equivalent a ImagingStudy.endpoint a FHIR.">
				>
				["at0098"] = <
					text = <"Punt final de l'estudi">
					description = <"Digital location of the imaging content and metadata.">
					comment = <"Aquesta ubicació identifica el servei de xarxa que proporciona accés (per a emmagatzematge, visualització, consulta o recuperació) per a l'estudi. Equivalent a ImagingStudy.endpoint a FHIR.">
				>
				["at0099"] = <
					text = <"Estat de l'estudi">
					description = <"L'estat actual de l'estudi d'imatge.">
					comment = <"Equivalent a ImagingStudy.status a FHIR.">
				>
				["at0100"] = <
					text = <"Registrat">
					description = <"L'estudi d'imatges està registrat, però encara no hi ha resultats disponibles.">
				>
				["at0101"] = <
					text = <"Disponible">
					description = <"Almenys una instància s'ha associat amb aquest estudi d'imatges.">
				>
				["at0102"] = <
					text = <"Cancel·lat">
					description = <"L'estudi d'imatges no està disponible perquè l'estudi no s'ha iniciat o no s'ha completat.">
				>
				["at0103"] = <
					text = <"Desconegut">
					description = <"L'estat no es coneix pel sistema de fonts de radiologia.">
				>
				["at0104"] = <
					text = <"Identificador de l'informe">
					description = <"Identificador únic per a l'informe d'imatge assignat pel servei de radiologia.">
					comment = <"Com que s'utilitza una instància d'aquest arquetip per estudi, aquest identificador es pot utilitzar per connectar més d'un estudi d'imatges dins d'un únic informe combinat. Equivalent a DiagnosticReport.identifier a FHIR.">
				>
				["at0105"] = <
					text = <"Descripció de l'estudi">
					description = <"Descripció o classificació generada per serveis radiològics de l'estudi.">
					comment = <"Equivalent a ImagingStudy.description en FHIR i Descripció de l'Estudi (0008, 1030) en DICOM. Possibles exemples DICOM: MR BREAS, UNIT; NEUROHEADHEAD; o MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Detalls de la imatge">
					description = <"Enllaç a tots els detalls de qualsevol imatge realitzada durant l'estudi.">
					comment = <"Normalment, les referències a les imatges utilitzen les modalitats DICOM habilitades, però aquest camp de dades proporciona un mètode alternatiu per accedir a les imatges. Equivalent a DiagnosticReport.imagingStudy a FHIR.">
				>
				["at0107"] = <
					text = <"Elements d'estabilització">
					description = <"Identificació d'un aparell estabilitzador en ús.">
					comment = <"For example: braces, casts, external fixation.">
				>
				["at0108"] = <
					text = <"Posició">
					description = <"Posició del pacient durant l'examen d'imatge.">
					comment = <"Per exemple: dempeus o estirat.">
				>
			>
		>
		["es"] = <
			items = <
				["at0000"] = <
					text = <"Resultado del examen por imagen">
					description = <"Resultado de una prueba de imagen realizada a un individuo mediante técnicas radiológicas.">
				>
				["at0001"] = <
					text = <"Serie de eventos">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Cualquier evento">
					description = <"Evento predeterminado, no especificado, en un punto temporal o intervalo que puede definirse explícitamente en una plantilla o en tiempo de ejecución.">
				>
				["at0003"] = <
					text = <"Árbol">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Nombre del estudio">
					description = <"Nombre de la prueba de imagen realizada.">
					comment = <"Por ejemplo: \"TC de abdomen\"; \"Ecografía Doppler de la arteria renal\" o \"Radiografía simple de tórax\". Se recomienda encarecidamente codificar con una terminología, preferiblemente un término que especifique tanto la modalidad como la localización anatómica, siempre que sea posible. Ejemplos de terminologías candidatas son los códigos de documentos de imágenes LOINC, RadLex Playbook o SNOMED-CT. Equivalente a DiagnosticReport.code en FHIR.">
				>
				["at0006"] = <
					text = <"Sitio estructurado del cuerpo diana">
					description = <"Detalles estructurados sobre la zona o región corporal objetivo para la imagen.">
					comment = <"Si la zona corporal se especifica en el texto o se codifica para \"Nombre del estudio\", este campo puede ser redundante.">
				>
				["at0008"] = <
					text = <"Hallazgos de imagen">
					description = <"Descripción narrativa o resumen de todos los hallazgos clínicos.">
				>
				["at0014"] = <
					text = <"Resumen clínico">
					description = <"Descripción narrativa de la historia clínica relevante que contextualiza el estudio y la interpretación de los resultados.">
					comment = <"Como alternativa, este elemento de datos puede incluir un enlace de RM a la información clínica proporcionada en la solicitud de examen original o ubicada en otra parte del historial clínico.">
				>
				["at0020"] = <
					text = <"Diagnóstico por imagen">
					description = <"Una sola palabra, frase o descripción breve que representa la probable afección o diagnóstico.">
					comment = <"Este elemento de datos se repite varias veces para permitir más de un diagnóstico diferencial. Se prefiere codificar con una terminología como SNOMED CT, siempre que sea posible. Este elemento de datos debe considerarse mutuamente excluyente con \"Diagnóstico diferencial por imágenes\", es decir, solo \"Diagnóstico por imágenes\" o \"Diagnóstico diferencial por imágenes\" en cada instancia del arquetipo \"Resultado del examen por imágenes\". Equivalente a DiagnosticReport.conclusionCode en FHIR.">
				>
				["at0021"] = <
					text = <"Impresión general">
					description = <"Interpretación narrativa concisa y clínicamente relevante de todos los hallazgos de imagen, incluyendo una comparación con estudios previos cuando corresponda.">
					comment = <"También conocido como «Opinión» o «Conclusión». Equivalente a DiagnosticReport.conclusion en FHIR.">
				>
				["at0023"] = <
					text = <"Comentario">
					description = <"Narrativa adicional sobre el estudio no reflejada en otros campos.">
					comment = <"Por ejemplo: una nota que indica que se entregó la radiografía al paciente. Equivalente a DiagnosticReport.note en FHIR.">
				>
				["at0025"] = <
					text = <"Árbol">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Servicio de diagnóstico por imagen">
					description = <"Detalles sobre el servicio, la organización o la persona que realiza el estudio de imagen.">
				>
				["at0027"] = <
					text = <"Detalles de la solicitud de examen">
					description = <"Detalles sobre un único estudio de imagen solicitado.">
				>
				["at0028"] = <
					text = <"Identificador de la orden del solicitante">
					description = <"Identificador único de la orden de estudio de imagen asignado por el solicitante.">
					comment = <"Equivalente al identificador de orden de llenado de HL7.">
				>
				["at0029"] = <
					text = <"Nombre del examen solicitado">
					description = <"Identificación del estudio de imagen solicitado.">
					comment = <"Esto puede ser útil en situaciones donde el examen solicitado difiere del realizado.">
				>
				["at0030"] = <
					text = <"Solicitante">
					description = <"Detalles sobre el profesional clínico o la organización que solicita el estudio de imagen.">
					comment = <"Equivalente al nombre del médico remitente DICOM (0008,0090).">
				>
				["at0031"] = <
					text = <"Identificador de orden del receptor">
					description = <"Identificador único de la orden de estudio de imagen asignado por el servicio de radiología.">
					comment = <"Equivalente al identificador de orden de llenado de HL7 o al atributo de número de acceso DICOM (0008,0050).">
				>
				["at0041"] = <
					text = <"Técnica/procedimiento estructurado">
					description = <"Detalles estructurados adicionales sobre los aspectos técnicos y clínicos de la captura de la(s) imagen(es).">
				>
				["at0044"] = <
					text = <"Hallazgos de imagen estructurada">
					description = <"Detalles estructurados sobre los hallazgos de la prueba de imagen dirigidos a una estructura o región específica.">
				>
				["at0045"] = <
					text = <"Representación del informe">
					description = <"Representación digital del resultado del estudio.">
					comment = <"Se permiten múltiples formatos, pero deben representar contenido clínico equivalente. Por ejemplo, una versión en PDF del informe. Equivalente a DiagnosticReport.presentedForm en FHIR.">
				>
				["at0046"] = <
					text = <"Extensión">
					description = <"Información adicional necesaria para ampliar el modelo con contenido local o para alinearlo con otros modelos de referencia o formalismos.">
					comment = <"Por ejemplo: requisitos de información local o metadatos adicionales para alinearse con FHIR.">
				>
				["at0047"] = <
					text = <"Árbol">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Factores de confusión">
					description = <"Descripción narrativa de los factores, no registrados en otro lugar, que pueden influir en los hallazgos o el resultado del estudio.">
				>
				["at0049"] = <
					text = <"Resumen de la técnica">
					description = <"Descripción narrativa del procedimiento radiológico utilizado para el estudio.">
					comment = <"Utilice este elemento de datos para describir un resumen de la técnica, incluyendo las limitaciones técnicas o las desviaciones de los procedimientos técnicos habituales. Por ejemplo: «Se adquirieron imágenes axiales de 5 mm desde las bases pulmonares hasta la sínfisis púbica tras la administración de contraste intravenoso y oral. Se generaron imágenes coronales y sagitales reformateadas a partir de los datos originales».">
				>
				["at0055"] = <
					text = <"Sitio corporal objetivo">
					description = <"Descripción de la zona o región corporal objetivo para la obtención de imágenes.">
					comment = <"Si la zona corporal se especifica en el texto o en el código de \"Nombre del estudio\", este campo puede ser redundante. Es recomendable codificar \"Zona corporal diana\" con una terminología como SNOMED CT. Equivalente a ImagingStudy.series.bodySite en FHIR.">
				>
				["at0056"] = <
					text = <"Hallazgos comparativos">
					description = <"Descripción narrativa de la comparación de este examen con estudios similares previos.">
					comment = <"Utilice también este elemento de datos para indicar si no hay imágenes ni informes de comparación disponibles. Los detalles sobre las imágenes utilizadas para la comparación pueden registrarse mediante el grupo \"Detalles del estudio de comparación\" dentro del Protocolo.">
				>
				["at0057"] = <
					text = <"Descripción de la calidad de la imagen">
					description = <"Descripción narrativa de la calidad del examen.">
					comment = <"Por ejemplo: la naturaleza de las limitaciones y su impacto en la interpretación; la recomendación de repetir el examen; o si un método de evaluación alternativo podría ser más apropiado.">
				>
				["at0058"] = <
					text = <"Diagnóstico diferencial de la imagen">
					description = <"Palabra, frase o descripción breve que represente una posible afección o diagnóstico.">
					comment = <"Este elemento de datos se repite varias veces para permitir más de un diagnóstico diferencial. Se prefiere codificarlo con una terminología como SNOMED CT, siempre que sea posible. Este elemento de datos debe considerarse mutuamente excluyente de \"Diagnóstico por imagen\", es decir, solo \"Diagnóstico por imagen\" o \"Diagnóstico diferencial por imagen\" en cada instancia del arquetipo \"Resultado del examen por imagen\".">
				>
				["at0059"] = <
					text = <"Recomendación">
					description = <"Sugerencia para estudios de imagen, investigaciones y/o derivación adicionales, y justificación.">
					comment = <"Este elemento de datos tiene 0..* ocurrencias para permitir más de una recomendación. Las solicitudes formales para estudios de imagen adicionales deben registrarse utilizando un arquetipo INSTRUCTION, como INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Indicación clínica">
					description = <"Descripción narrativa del motivo por el que se solicitó originalmente el estudio.">
					comment = <"También conocido como \"Pregunta clínica\" o \"Consulta clínica\".">
				>
				["at0062"] = <
					text = <"Resumen del procedimiento">
					description = <"Descripción narrativa del procedimiento clínico u otras consideraciones clínicas utilizadas para el estudio.">
					comment = <"Utilice este elemento de datos para describir un resumen del procedimiento clínico, la posición del paciente, intervenciones como la maniobra de Valsalva y la administración de contraste, radiofármacos o tratamientos como parte del estudio de imagen. Por ejemplo: \"Se administraron 80 ml de Isovue-370 por vía intravenosa mediante una técnica de seguimiento de bolo centrada en la arteria pulmonar principal\". También puede utilizarse para registrar un resumen de cualquier problema, complicación o reacción adversa encontrada.">
				>
				["at0063"] = <
					text = <"Calidad de la imagen">
					description = <"Evaluación de la calidad del estudio.">
					comment = <"Se recomienda codificar la calidad de la imagen con un conjunto de valores. Por ejemplo: \"Excelente, Buena, Regular, Mala y No Diagnóstica\"; o \"Adecuada, Subóptima e Insuficiente\".">
				>
				["at0065"] = <
					text = <"Detalles de la serie">
					description = <"Detalles sobre una serie incluida en este informe.">
				>
				["at0067"] = <
					text = <"Detalles de la serie comparativa">
					description = <"Detalles sobre una serie de estudios de imagen que se compara con el estudio reportado.">
				>
				["at0069"] = <
					text = <"Dispositivo">
					description = <"Detalles sobre el/los dispositivo(s) de imagen utilizado(s) para capturar el estudio.">
				>
				["at0070"] = <
					text = <"Fecha del estudio">
					description = <"Fecha y hora de inicio de la obtención de imágenes.">
					comment = <"Equivalente a ImagingStudy.started en FHIR.">
				>
				["at0071"] = <
					text = <"Marca de tiempo del estado">
					description = <"Fecha y/o hora en que se asignó el \"Estado general del resultado\".">
				>
				["at0072"] = <
					text = <"Estado general del resultado">
					description = <"Estado del resultado de la prueba de imagen en su conjunto.">
					comment = <"Los valores se han alineado con el estado del informe de diagnóstico de FHIR, con la excepción de \"Ingresado por error\", que solo es relevante para un paradigma orientado a la mensajería. Se pueden usar otros códigos/términos locales mediante la opción \"Texto\". En la mayoría de los casos, solo se aplica un estado a la vez; sin embargo, este elemento de datos permite múltiples ocurrencias en casos donde se deben especificar diversos tipos de estado, generalmente dentro de una plantilla. Equivalente a DiagnosticReport.status en FHIR.">
				>
				["at0073"] = <
					text = <"Registrado">
					description = <"Se registra la existencia del estudio, pero aún no hay resultados disponibles.">
				>
				["at0074"] = <
					text = <"Parcial">
					description = <"Este es un resultado parcial (p. ej., inicial, provisional o preliminar), pero los datos pueden estar incompletos o sin verificar.">
				>
				["at0075"] = <
					text = <"Preliminar">
					description = <"Hay resultados preliminares verificados disponibles, pero no todos son definitivos. Esta es una subcategoría de \"Parcial\".">
				>
				["at0076"] = <
					text = <"Final">
					description = <"El resultado está completo y ha sido verificado por una persona autorizada.">
				>
				["at0077"] = <
					text = <"Enmendado">
					description = <"Tras ser definitivo, el resultado ha sido modificado.">
				>
				["at0078"] = <
					text = <"Corregido">
					description = <"Tras ser definitivo, el resultado ha sido modificado para corregir un error. Esta es una subcategoría de \"Enmendado\".">
				>
				["at0079"] = <
					text = <"Anexado">
					description = <"Tras ser definitivo, el resultado se ha modificado añadiendo nuevo contenido. El contenido existente no se ha modificado. Esta es una subcategoría de \"Enmendado\".">
				>
				["at0080"] = <
					text = <"Cancelado">
					description = <"El resultado no está disponible porque la prueba no se inició o no se completó (también denominada \"abortada\").">
				>
				["at0083"] = <
					text = <"Lista de distribución">
					description = <"Datos de otros profesionales clínicos u organizaciones que requieren una copia del resultado del examen.">
					comment = <"La «Lista de distribución» es solo informativa, y el destinatario principal del informe es la persona que debe actuar en consecuencia.">
				>
				["at0087"] = <
					text = <"Técnica">
					description = <"Nombre del procedimiento radiológico utilizado para capturar el estudio.">
					comment = <"Se permite codificar la «Técnica» con una terminología específica. Su uso múltiple permite identificar una o más técnicas.">
				>
				["at0088"] = <
					text = <"Procedimiento">
					description = <"Identificación del procedimiento o la gestión clínica utilizada para capturar el estudio.">
				>
				["at0090"] = <
					text = <"Desconocido">
					description = <"Se desconoce el estado del resultado.">
				>
				["at0091"] = <
					text = <"Modalidad">
					description = <"Tipo de dispositivo, proceso o método de imagen que originalmente adquirió o produjo los datos utilizados para crear las imágenes del estudio.">
					comment = <"Por ejemplo: «Rayos X», «Tomografía computarizada», «Ecografía», «Resonancia magnética», «Densitometría ósea», «Tomografía», «Mamografía», «Medicina nuclear», «PET», «Angiografía» y «Fluoroscopia». Este dato puede ser redundante si la modalidad está claramente representada en el «Nombre del estudio». Como alternativa, también es posible rellenar automáticamente la «Modalidad» a partir de un «Nombre del estudio» codificado. Se recomienda codificar la «Modalidad» con un conjunto de valores, como el conjunto de valores de la Modalidad de adquisición DICOM CID 29. Equivalente a «ImagingStudy.modality» en FHIR y a «Modalidad» (0008, 0060) en DICOM.">
				>
				["at0092"] = <
					text = <"Identificador de la instancia de estudio">
					description = <"Identificador único del estudio de imagen asignado por el dispositivo de imagen.">
					comment = <"Equivalente a «UID de instancia de estudio» (0020,000D) en DICOM e «ImagingStudy.identifier» en FHIR.">
				>
				["at0093"] = <
					text = <"Detalles del estudio de comparación">
					description = <"Detalles sobre las imágenes de un estudio anterior utilizadas para la comparación con el estudio notificado.">
				>
				["at0094"] = <
					text = <"Nombre del estudio">
					description = <"El nombre del examen de comparación de imágenes realizado.">
				>
				["at0095"] = <
					text = <"Identificador del estudio">
					description = <"Identificador único para el estudio de comparación de imágenes.">
				>
				["at0096"] = <
					text = <"Fecha del estudio">
					description = <"Fecha y hora de inicio del examen comparativo.">
				>
				["at0097"] = <
					text = <"Punto final del estudio">
					description = <"Localización digital del contenido y los metadatos de las imágenes de comparación.">
					comment = <"Esta ubicación identifica el servicio de red que proporciona acceso (para almacenamiento, visualización, consulta o recuperación) al estudio. Equivalente a ImagingStudy.endpoint en FHIR.">
				>
				["at0098"] = <
					text = <"Punto final del estudio">
					description = <"Localización digital del contenido de las imágenes y los metadatos.">
					comment = <"Esta ubicación identifica el servicio de red que proporciona acceso (para almacenamiento, visualización, consulta o recuperación) al estudio. Equivalente a ImagingStudy.endpoint en FHIR.">
				>
				["at0099"] = <
					text = <"Estado del estudio">
					description = <"The current state of the imaging study.">
					comment = <"Equivalente a ImagingStudy.status en FHIR.">
				>
				["at0100"] = <
					text = <"Registrado">
					description = <"The existence of the imaging study is registered, but there is nothing yet available.">
				>
				["at0101"] = <
					text = <"Disponible">
					description = <"Al menos un caso se ha asociado con este estudio de imágenes.">
				>
				["at0102"] = <
					text = <"Cancelado">
					description = <"El estudio de imagen no está disponible porque el estudio no se inició o no se completó">
				>
				["at0103"] = <
					text = <"Desconocido">
					description = <"El sistema de radiología desconoce el estado.">
				>
				["at0104"] = <
					text = <"Identificador del informe">
					description = <"Identificador único del informe de imágenes asignado por el servicio de radiología.">
					comment = <"Dado que se utiliza una instancia de este arquetipo por estudio, este identificador puede utilizarse para conectar más de un estudio de imagen en un único informe combinado. Equivalente a DiagnosticReport.identifier en FHIR.">
				>
				["at0105"] = <
					text = <"Descripción del estudio">
					description = <"Descripción o clasificación del estudio generada por el servicio de radiología.">
					comment = <"Equivalente a ImagingStudy.description en FHIR y a Study description (0008, 1030) en DICOM. Posibles ejemplos en DICOM: MR BREAS, UNIT; NEURO^HEAD; o MR BREASTUNI UE.">
				>
				["at0106"] = <
					text = <"Detalles de la imagen">
					description = <"Enlace a los detalles completos de las imágenes obtenidas durante el estudio.">
					comment = <"Normalmente, las imágenes referenciadas utilizaban las modalidades compatibles con DICOM, pero este campo de datos proporciona un método alternativo para acceder a ellas. Equivalente a DiagnosticReport.imagingStudy en FHIR.">
				>
				["at0107"] = <
					text = <"Dispositivo estabilizador">
					description = <"Identificación de un dispositivo estabilizador en uso.">
					comment = <"Por ejemplo: aparatos ortopédicos, yesos, fijación externa.">
				>
				["at0108"] = <
					text = <"Posición">
					description = <"Posición del individuo durante el estudio de imágenes.">
					comment = <"Por ejemplo: de pie o acostado.">
				>
			>
		>
	>
