	openEHR-EHR-OBSERVATION.dietary_nutrients.v0

concept
	[at0000]	-- Dietary nutrients
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Priscila Maranhão; Gustavo Bacelar">
		["organisation"] = <"MEDCIDS-FMUP; VIRTUAL CARE">
