definition
	OBSERVATION[at0000] matches {    -- Estimated glomerular filtration rate (eGFR)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Single
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Relative eGFR
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::126]>
												list = <
													["1"] = <
														units = <"mL/min/{1.73_m2}">
													>
												>
											>
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Absolute eGFR
										value matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"ml/min">
													>
												>
											>
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0022] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Formula
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013,    -- MDRD formula - non-IDMS-normalized serum Cr
									at0014,    -- MDRD formula - IDMS-normalized serum Cr
									at0015,    -- CKD-EPI Creatinine
									at0017,    -- Bedside Schwartz equation
									at0016,    -- Cockcroft-Gault formula
									at0021,    -- LM Revised
									at0025,    -- CKD-EPI Cystatin C
									at0026]    -- CKD-EPI Creatinine-Cystatin C
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- Automatic entry
									at0008]    -- Direct entry
								}
							}
						}
					}
					allow_archetype CLUSTER[at0024] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


