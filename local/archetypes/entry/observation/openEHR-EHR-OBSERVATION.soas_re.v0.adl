definition
	OBSERVATION[at0000] matches {    -- The Staff Observation Aggression Scale – Revised Emergency (SOAS-RE)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {0..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0132] occurrences matches {0..1} matches {    -- When was the incident?
										value matches {
											DV_DATE_TIME matches {
												value matches {yyyy-mm-ddTHH:MM:SS}
											}
										}
									}
									ELEMENT[at0099] occurrences matches {0..*} matches {    -- Where was the incident?
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0100,    -- At the clinic
													at0101,    -- Over the phone
													at0102,    -- At home visit
													at0103]    -- Other
												}
											}
										}
									}
									ELEMENT[at0104] occurrences matches {0..1} matches {    -- Other place of incident
										value matches {
											DV_TEXT matches {*}
										}
									}
									CLUSTER[at0106] occurrences matches {0..1} matches {    -- Staff member exposed to incident
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0130] occurrences matches {0..1} matches {    -- Age
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..120|}
													}
												}
											}
											ELEMENT[at0107] occurrences matches {0..1} matches {    -- Occupation
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0108] occurrences matches {0..1} matches {    -- Sex
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0109,    -- Female
															at0110]    -- Male
														}
													}
												}
											}
											ELEMENT[at0111] occurrences matches {0..*} matches {    -- Were you alone in the situation?
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0112,    -- Yes
															at0113]    -- No
														}
													}
												}
											}
										}
									}
									CLUSTER[at0114] occurrences matches {0..1} matches {    -- Aggressor
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0115] occurrences matches {0..*} matches {    -- Who
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0116,    -- Patient
															at0117,    -- Next-of-kin
															at0118]    -- Other
														}
													}
												}
											}
											ELEMENT[at0133] occurrences matches {0..1} matches {    -- Other aggressor
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0119] occurrences matches {0..1} matches {    -- Sex
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0120,    -- Female
															at0121]    -- Male
														}
													}
												}
											}
											ELEMENT[at0122] occurrences matches {0..1} matches {    -- Does the aggressor have a known mental illness?
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0124,    -- Yes
															at0125,    -- No
															at0126]    -- Unknown
														}
													}
												}
											}
											ELEMENT[at0123] occurrences matches {0..1} matches {    -- Was the aggressor under influence of drugs or alcohol?
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0127,    -- Yes
															at0128,    -- No
															at0129]    -- Unknown
														}
													}
												}
											}
										}
									}
									CLUSTER[at0007] matches {    -- Provocation of aggresive behaviour
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0020] occurrences matches {0..1} matches {    -- No understandable provocation
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0014] occurrences matches {0..*} matches {    -- Provoked by
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0015,    -- Person had to wait
															at0016,    -- The person was denied something
															at0017,    -- The person disagreed about assessment/advice
															at0018,    -- Involuntary assessment of health condition
															at0030]    -- Other, describe
														}
													}
												}
											}
											ELEMENT[at0019] occurrences matches {0..1} matches {    -- Description
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0008] matches {    -- Means used by the aggressor
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0059] occurrences matches {0..1} matches {    -- Verbal aggression
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {    -- Threat
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0022] occurrences matches {0..1} matches {    -- Ordinary objects
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::at0026]    -- Room contents/ objects.
														}
													}
												}
											}
											ELEMENT[at0023] occurrences matches {0..1} matches {    -- Parts of the body
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0027,    -- Hand
															at0028,    -- Foot
															at0029]    -- Other, describe
														}
													}
												}
											}
											ELEMENT[at0058] occurrences matches {0..1} matches {    -- Descritpion
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0031] occurrences matches {0..1} matches {    -- Dangerous objects or methods
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0032,    -- Attempt of strangulation.
															at0033,    -- Used/had weapon.
															at0034,    -- Used/had pointed weapon.
															at0035]    -- Other dangerous objects incl. syringe.
														}
													}
												}
											}
										}
									}
									CLUSTER[at0010] matches {    -- Target of aggression
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0061] occurrences matches {0..1} matches {    -- None
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0036] occurrences matches {0..*} matches {    -- Target
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0038,    -- Furniture/objects
															at0039,    -- Physician
															at0040,    -- Nurse
															at0041,    -- Ambulance personnel
															at0042,    -- Security guard
															at0043,    -- Police
															at0044,    -- Other patients
															at0045]    -- Other persons, describe
														}
													}
												}
											}
											ELEMENT[at0062] occurrences matches {0..1} matches {    -- Description
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0011] matches {    -- Consequence(s) for victim(s)
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0046] occurrences matches {0..1} matches {    -- None
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0047] occurrences matches {0..1} matches {    -- Objects
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::at0049]    -- Damaged
														}
													}
												}
											}
											ELEMENT[at0048] occurrences matches {0..*} matches {    -- Person
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0050,    -- Psychological/ emotional stress
															at0051,    -- Felt threatened
															at0052,    -- Pain
															at0053,    -- Visible injury
															at0054,    -- Need for treatment by a physician
															at0055,    -- Needed to be taken off duty
															at0056]    -- Other, describe:
														}
													}
												}
											}
											ELEMENT[at0063] occurrences matches {0..1} matches {    -- Description
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0012] occurrences matches {0..1} matches {    -- Measure(s) to stop aggression
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0064] occurrences matches {0..1} matches {    -- None
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0065] occurrences matches {0..*} matches {    -- Measure(s)
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0066,    -- Talked to person
															at0067,    -- Took the person aside
															at0068,    -- Withdrew from situation /ended cal
															at0069,    -- Complied with the person’s wish
															at0070,    -- Asked the person to leave the site
															at0071,    -- Forced the person to leave
															at0072,    -- Held the person by force
															at0073,    -- Medication
															at0074]    -- Other, describe:
														}
													}
												}
											}
											ELEMENT[at0075] occurrences matches {0..1} matches {    -- Description
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0013] occurrences matches {0..1} matches {    -- Persons involved in measure(s) to stop aggression
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0076] occurrences matches {0..*} matches {    -- Persons involved
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0077,    -- Physician
															at0078,    -- Nurse
															at0079,    -- Ambulance personnel
															at0080,    -- Security guard
															at0081,    -- Police
															at0082,    -- Other patients
															at0083,    -- Next-of-kin
															at0084]    -- Others, describe
														}
													}
												}
											}
											ELEMENT[at0085] occurrences matches {0..1} matches {    -- Description
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0086] occurrences matches {0..1} matches {    -- Visual Analogue Scale (VAS)
										value matches {
											0|[local::at0087],
											1|[local::at0089],
											2|[local::at0090],
											3|[local::at0091],
											4|[local::at0092],
											5|[local::at0093],
											6|[local::at0094],
											7|[local::at0095],
											8|[local::at0096],
											9|[local::at0097],
											10|[local::at0098]
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0134] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


