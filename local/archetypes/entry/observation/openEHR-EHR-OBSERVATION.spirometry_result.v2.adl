				>
				["at0182"] = <
					text = <"SNIP">
					description = <"Sniff nasal inspiratory pressure (SNIP) is the maximal inspiratory pressure in one occluded nostril during a maximal sniff performed from relaxed end-expiration through the other patent nostril.">
