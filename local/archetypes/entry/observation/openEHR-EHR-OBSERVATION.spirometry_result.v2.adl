				>
				["at0177"] = <
					text = <"FEF25%">
					description = <"Forsert ekspiratorisk luftstrømshastighet 25 % (FEF25%) er den maksimale momentane ekspiratoriske luftstrømshastigheten når 25% av volumet av forsert vitalkapasitet (FVC) er ekshalert. Også kjent som maksimal ekspiratorisk luftstrømshastighet 75 % (MEF75%).">
				>
				["at0178"] = <
					text = <"FEF50%">
