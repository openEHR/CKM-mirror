				["at0174"] = <
					text = <"IVC">
					description = <"Inspiratorisk vitalkapasitet (IVC) er det maksimale volumet med luft som kan innåndes ved sakte inspirasjon etter en maksimal ekspirasjon. IVC er tilsvarende ERV + TV + IRV.">
				>
				["at0176"] = <
					text = <"MVV">
