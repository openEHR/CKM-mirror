					description = <"Duration of the S' wave.">
				>
				["at0059"] = <
					text = <"Ventricular Activation Time (VAT)">
					description = <"Interval from the onset of the QRS complex to the latest positive peak in the complex, or the latest substantial notch on the latest peak (whichever is later).">
				>
				["at0060"] = <
					text = <"QRS p-p">
					description = <"Amplitude of the peak-to-peak QRS complex.">
				>
				["at0061"] = <
					text = <"QRS duration">
					description = <"Duration of the QRS complex.">
					comment = <"Measured from its onset to the ST segment onset (J point).">
				>
				["at0062"] = <
					text = <"QRS area">
					description = <"Area of the QRS complex.">
				>
				["at0063"] = <
					text = <"ST onset">
					description = <"Elevation or depression at the onset (J point) of the ST segment.">
				>
				["at0064"] = <
					text = <"ST midpoint">
					description = <"Elevation or depression at the midpoint of the ST segment.">
