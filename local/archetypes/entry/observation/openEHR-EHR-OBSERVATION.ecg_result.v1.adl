				>
				["at0069"] = <
					text = <"ST-Segment Morphologie">
					description = <"Form des ST-Segments.">
					comment = <"Das Valueset stammt von DICOM PS3.16 2018 c - Content Mapping Resource -http://dicom.nema.org/medical/dicom/current/output/html/part16.html#sect_CID_3233">
				>
				["at0070"] = <
					text = <"ST-Senkung - horizontal">
					description = <"Die ST-Strecke weist eine Absenkung auf, jedoch keine fallende Steigung. Dies ist eine spezifischere Form der \"ST-Senkung\".">
