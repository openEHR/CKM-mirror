																}
															}
															ELEMENT[at0.8] occurrences matches {0..1} matches {	-- Sensitivity
																value matches {
																	DV_CODED_TEXT matches {
																		defining_code matches {
																			[local::
																			at0.11, 	-- Sensitive
																			at0.12, 	-- Resistant
																			at0.13]	-- Borderline resistant
																		}
																	}
																}
															}
															ELEMENT[at0.10] occurrences matches {0..1} matches {	-- Quantified sensitivity
																value matches {
