											ELEMENT[at0.18] occurrences matches {0..1} matches {	-- Field count
												value matches {
													DV_COUNT matches {
														magnitude matches {|>=0|}
													}
												}
											}
											ELEMENT[at0078.6] occurrences matches {0..*} matches {	-- Gram smear result
