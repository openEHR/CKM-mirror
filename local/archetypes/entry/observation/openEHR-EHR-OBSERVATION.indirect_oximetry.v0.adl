ontology
	terminologies_available = <"SNOMED-CT", "LOINC">
	term_definitions = <
		["ar-sy"] = <
			items = <
				["at0000"] = <
					text = <"  قياس التأكسج غير المباشر">
					description = <" 
طريقة لمراقبة قياسات غازات الدم مثل تشبع الدم الطرفي بالأكسجين و الضغط الجزئي للأكسجين في الدم الطرفي, بطريقة غير مباشرة و غير باضعة.
">
				>
				["at0001"] = <
					text = <"*Event Series(en)">
					description = <"*@ internal @(en)">
				>
				["at0002"] = <
					text = <"  إحدى الوقائع">
					description = <" 
واقعة زمنية
">
				>
				["at0003"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0006"] = <
					text = <"  تشبع الدم الطرفي بالأكسجين">
					description = <" 
يتم قياسه باستخدام قياس التأكسج عند النبض, و هو يمثل تشبع الأكسجين في الدم الطرفي.
">
					comment = <" 
يُعَرَّف تشبع الدم الطرفي بالأكسجين على أنه النسبة بين الأكسي هيموغلوبين إلى إجمالي تركيز الهيموغلوبين الذي يمثل مجموع الأكسي هيموغلوبين و الديوكسي هيموغلوبين.
">
				>
				["at0007"] = <
					text = <"*List(en)">
					description = <"*@ internal @(en)">
				>
				["at0009"] = <
					text = <"  مكان المجس – الحاسة">
					description = <" 
مكان مجس (حاسة) القياس.
">
				>
				["at0014"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0015"] = <
					text = <"  الأكسجين المحيط">
					description = <" 
تفاصيل حول كمية الأكسجين المتاحة للمريض في وقت الملاحظة. في وجود قيم مفترضة من 21% تركيز للأكسجين, و نسبة من الأكسجين المستنشق تساوي 0.21 و معدل لسريان الأكسجين يساوي 0 ليتر لكل دقيقة أو 0 ميلليليتر لكل دقيقة.
">
				>
				["at0016"] = <
					text = <"  العوامل المربكة">
					description = <" 
تعليق حول و تسجيل للعوامل الأخرى التي قد تطرأ و تؤثر على تفسير الملاحظة. مثل الحركة, الألم, ضعف الإرواء, و إرضاع الطفل.
">
				>
				["at0018"] = <
					text = <"الجهيزة">
					description = <" 
تفاصيل حول جهيزة قياس التأكسج بطريقة غير باضعة.
">
				>
				["at0019"] = <
					text = <"  صورة الوسائط المتعدةة">
					description = <"تفاصيل سلسلة قراءات التأكسج, بخلاف التموجات, و يتم التعبير عنها على هيئة صورة أو سلسلة من الصور. 
ينبغي تسجيل التموجات في شرفة التموجات التي يتم ربطها بعنقود نماذج مناسبة.
">
				>
				["at0020"] = <
					text = <"  نوع المجس – الحاسة">
					description = <" 
نوع مجس (حاسة) القياس الذي يتم استخدامه, مثل الامتراق, الانعكاس, الملقاط, الشريط أو المَسْرى - الكهربي.
">
					comment = <"*For example: transmittal electrode; reflective electrode; clamp electrode; or strip electrode.(en)">
				>
				["at0034"] = <
					text = <"المجهود">
					description = <" 
تفاصيل حول النشاط البدني الذي تتم ممارسته في وقت القياس.
">
				>
				["at0036"] = <
					text = <"تعليق">
					description = <" 
تعليق نَصي حول نتيجة قياس التأكسج غير المباشر.
">
				>
				["at0037"] = <
					text = <"  درجة حرارة تشغيل المسرى الكهربي">
					description = <" 
درجة حرارة تشغيل المسرى الكهربي الذي يقيس من خلال الجلد.
">
				>
				["at0038"] = <
					text = <"  وقت وضع المجس">
					description = <" 
التاريخ و الوقت الذي تم فيه وضع المجس. و هو معتدّ (هام) في قياس التأكسج عبر الجلد.
">
				>
				["at0039"] = <
					text = <"  الضغط الجزئي للأكسجين في الدم الطرفي">
					description = <" 
يتم قياسه عن طريق قياس التأكسج بالمَسْرى (الكهربي) بطريق الجلد, و هو يمثل الضغط الجزئي للأكسجين في الدم الطرفي.
">
				>
				["at0040"] = <
					text = <"  الضغط الجزئي لثاني أكسيد الكربون في الدم الطرفي">
					description = <" 
يتم قياسه عن طريق قياس التأكسج بالمَسْرى (الكهربي) بطريق الجلد, و هو يمثل الضغط الجزئي لثاني أكسيد الكربون في الدم الطرفي.
">
				>
				["at0044"] = <
					text = <"  المحتوى الأكسجيني في الدم الطرفي">
					description = <" 
يتم قياسه عن طريق قياس التأكسج عند النبض, و هو يمثل المحتوى الأكسجيني للدم الطرفي.
">
				>
				["at0045"] = <
					text = <"  تشبع الكاربوكسي هيموغلوبين في الدم الطرفي">
					description = <" 
يتم قياسه عن طريق قياس التأكسج عند النبض, و هو يمثل تشبع الكاربوكسي هيموغلوبين في الدم الطرفي.
">
				>
				["at0046"] = <
					text = <"  تشبع الميثوكسيهيموغلوبين في الدم الطرفي">
					description = <" 
يتم قياسه عن طريق قياس التأكسج عند النبض, و هو يمثل تشبع الميثوكسيهيموغلوبين في الدم الطرفي.
">
				>
				["at0048"] = <
					text = <"  الامتراق">
					description = <" 
تم استخدام مجس لقياس التأكسج عند النبض يعمل بالامتراق.
">
				>
				["at0049"] = <
					text = <"الانعكاس">
					description = <" 
تم استخدام مجس لقياس التأكسج عند النبض يعمل بالانعكاس.
">
				>
				["at0050"] = <
					text = <"الملقاط">
					description = <" 
تم استخدام مجس لقياس التأكسج عند النبض يعمل كملقاط.
">
				>
				["at0051"] = <
					text = <"شريط">
					description = <" 
تم استخدام مجس لقياس التأكسج عند النبض على هيئة شريط.
">
				>
				["at0052"] = <
					text = <"مَسْرى">
					description = <" 
تم استخدام مجس لقياس التأكسج عند النبض يعمل عن طريق مَسْرى كهربي.
">
				>
				["at0054"] = <
					text = <"التموج">
					description = <" 
قراءة تموجية متعلقة بقياس التأكسج.
">
				>
				["at0056"] = <
					text = <"*Oxygen Delivery Device(en)">
					description = <"*Details of the oxygen delivery device used.(en)">
				>
				["at0057"] = <
					text = <"*Method of Oxygen Delivery(en)">
					description = <"*Method of oxygen delivery device used to supply supplemental oxygen.(en)">
					comment = <"*For example: nasal cannulae; Venturi mask; non-rebreathing mask with reservoir; or ventilator.(en)">
				>
				["at0058"] = <
					text = <"*Interpretation(en)">
					description = <"*Single word, phrase or brief description represents the clinical meaning and significance of the physical examination findings.(en)">
					comment = <"*Coding with a terminology is preferred, if possible. For example, normal oxygen saturation or hypoxaemia.(en)">
				>
				["at0059"] = <
					text = <"*Extension(en)">
					description = <"*Additional information required to capture local context or to align with other reference models/formalisms.(en)">
					comment = <"*e.g. Local hospital departmental infomation or additional metadata to align with HL7 or CDISC equivalents.(en)">
				>
			>
		>
