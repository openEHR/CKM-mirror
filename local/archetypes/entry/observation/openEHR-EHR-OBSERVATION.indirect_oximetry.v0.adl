				>
				["at0039"] = <
					text = <"PtcO2">
					description = <"Medida por medio de un electrodo transcutáneo de oximetría , la presión parcial de oxígeno en la sangre periférica.">
				>
				["at0040"] = <
					text = <"PtcCO2">
