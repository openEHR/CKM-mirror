definition
	OBSERVATION[at0000] matches {    -- NIHSS
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Baseline
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- LOC responsiveness
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008]
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- LOC questions
										value matches {
											0|[local::at0010],
											1|[local::at0011],
											2|[local::at0012]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- LOC commands
										value matches {
											0|[local::at0014],
											1|[local::at0015],
											2|[local::at0016]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Best Gaze
										value matches {
											0|[local::at0018],
											1|[local::at0019],
											2|[local::at0020]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Visual
										value matches {
											0|[local::at0022],
											1|[local::at0023],
											2|[local::at0024],
											3|[local::at0025]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- Facial palsy
										value matches {
											0|[local::at0027],
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- Motor - left arm
										value matches {
											0|[local::at0032],
											1|[local::at0033],
											2|[local::at0034],
											3|[local::at0124],
											4|[local::at0035],
											99|[local::at0036]
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- Reason left arm untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- Motor - right arm
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043],
											99|[local::at0044]
										}
									}
									ELEMENT[at0045] occurrences matches {0..1} matches {    -- Reason right arm untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Motor - left leg
										value matches {
											0|[local::at0047],
											1|[local::at0048],
											2|[local::at0049],
											3|[local::at0050],
											4|[local::at0051],
											99|[local::at0052]
										}
									}
									ELEMENT[at0053] occurrences matches {0..1} matches {    -- Reason left leg untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0141] occurrences matches {0..1} matches {    -- Motor - right leg
										value matches {
											0|[local::at0054],
											1|[local::at0055],
											2|[local::at0056],
											3|[local::at0057],
											4|[local::at0058],
											99|[local::at0059]
										}
									}
									ELEMENT[at0060] occurrences matches {0..1} matches {    -- Reason right leg untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {    -- Limb ataxia
										value matches {
											0|[local::at0062],
											1|[local::at0063],
											2|[local::at0064],
											99|[local::at0065]
										}
									}
									ELEMENT[at0066] occurrences matches {0..1} matches {    -- Reason limb ataxia untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0067] occurrences matches {0..1} matches {    -- Sensory
										value matches {
											0|[local::at0068],
											1|[local::at0069],
											2|[local::at0070]
										}
									}
									ELEMENT[at0071] occurrences matches {0..1} matches {    -- Best Language
										value matches {
											0|[local::at0072],
											1|[local::at0073],
											2|[local::at0074],
											3|[local::at0075]
										}
									}
									ELEMENT[at0076] occurrences matches {0..1} matches {    -- Dysarthria
										value matches {
											0|[local::at0077],
											1|[local::at0078],
											2|[local::at0079],
											99|[local::at0080]
										}
									}
									ELEMENT[at0081] occurrences matches {0..1} matches {    -- Reason speech untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0082] occurrences matches {0..1} matches {    -- Extinction and inattention
										value matches {
											0|[local::at0083],
											1|[local::at0084],
											2|[local::at0085]
										}
									}
									ELEMENT[at0086] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..42|}
											}
										}
									}
								}
							}
						}
					}
					POINT_EVENT[at0087] occurrences matches {0..1} matches {    -- 2 hours post treatment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0088] occurrences matches {0..1} matches {    -- 24 hours post symptom onset
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0089] occurrences matches {0..1} matches {    -- 7–10 days
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0090] occurrences matches {0..1} matches {    -- 3 months
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					EVENT[at0091] occurrences matches {0..*} matches {    -- Any event
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0092] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0093] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"NIHSS">
					description = <"15-item neurologic examination stroke scale that provides a quantitative measure of stroke-related neurological deficit.">
					comment = <"NIHSS - National Institutes of Health Stroke Scale.">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Baseline">
					description = <"Baseline assessment.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"LOC responsiveness">
					description = <"Level of Consciousness">
				>
				["at0005"] = <
					text = <"Alert; keenly responsive.">
					description = <"">
				>
				["at0006"] = <
					text = <"Not Alert; but arousable by minor stimulation to obey, answer, or respond.">
					description = <"">
				>
				["at0007"] = <
					text = <"Not Alert; requires repeated stimulation to attend, or is obtunded and requires strong or painful">
					description = <"">
				>
				["at0008"] = <
					text = <"Responds only with reflex motor or autonomic effects, or totally unresponsive, flaccid, and areflexic.">
					description = <"">
				>
				["at0009"] = <
					text = <"LOC questions">
					description = <"Level of Consciousness">
				>
				["at0010"] = <
					text = <"Answers both questions correctly.">
					description = <"">
				>
				["at0011"] = <
					text = <"Answers one question correctly.">
					description = <"">
				>
				["at0012"] = <
					text = <"Answers neither question correctly.">
					description = <"">
				>
				["at0013"] = <
					text = <"LOC commands">
					description = <"Level of Consciousness">
				>
				["at0014"] = <
					text = <"Performs both tasks correctly.">
					description = <"">
				>
				["at0015"] = <
					text = <"Performs one task correctly.">
					description = <"">
				>
				["at0016"] = <
					text = <"Performs neither task correctly.">
					description = <"">
				>
				["at0017"] = <
					text = <"Best Gaze">
					description = <"">
				>
				["at0018"] = <
					text = <"Normal.">
					description = <"">
				>
				["at0019"] = <
					text = <"Partial gaze palsy; gaze is abnormal in one or both eyes, but forced deviation or total gaze paresis is not present.">
					description = <"">
				>
				["at0020"] = <
					text = <"Forced deviation, or total gaze paresis is not overcome by the oculocephalic maneuver.">
					description = <"">
				>
				["at0021"] = <
					text = <"Visual">
					description = <"">
				>
				["at0022"] = <
					text = <"No visual loss.">
					description = <"">
				>
				["at0023"] = <
					text = <"Partial hemianopia.">
					description = <"">
				>
				["at0024"] = <
					text = <"Complete hemianopia.">
					description = <"">
				>
				["at0025"] = <
					text = <"Bilateral hemianopia (blind including cortical blindness).">
					description = <"">
				>
				["at0026"] = <
					text = <"Facial palsy">
					description = <"">
				>
				["at0027"] = <
					text = <"Normal symmetrical movements.">
					description = <"">
				>
				["at0028"] = <
					text = <"Minor paralysis (flattened nasolabial fold, asymmetry on smiling).">
					description = <"">
				>
				["at0029"] = <
					text = <"Partial paralysis (total or near-total paralysis of lower face).">
					description = <"">
				>
				["at0030"] = <
					text = <"Complete paralysis of one or both sides (absence of facial movement in the upper and lower face).">
					description = <"">
				>
				["at0067"] = <
					text = <"Sensory">
					description = <"">
				>
				["at0068"] = <
					text = <"Normal; no sensory loss.">
					description = <"">
				>
				["at0069"] = <
					text = <"Mild-to-moderate sensory loss; patient feels pinprick is less sharp or is dull on the affected side; or  there is a loss of superficial pain with pinprick, but patient is aware of being touched.">
					description = <"">
				>
				["at0070"] = <
					text = <"Severe or total sensory loss; patient is not aware of being touched in the face, arm, and leg.">
					description = <"">
				>
				["at0071"] = <
					text = <"Best Language">
					description = <"">
				>
				["at0072"] = <
					text = <"No aphasia; normal.">
					description = <"">
				>
				["at0073"] = <
					text = <"Mild-to-moderate aphasia; some obvious loss of fluency or facility of comprehension, without  significant limitation on ideas expressed or form of expression. Reduction of speech and/or  comprehension, however, makes conversation about provided materials difficult or impossible. For  example, in conversation about provided materials, examiner can identify picture or naming card  content from patient’s response.">
					description = <"">
				>
				["at0074"] = <
					text = <"Severe aphasia; all communication is through fragmentary expression; great need for inference, questioning, and guessing by the listener. Range of information that can be exchanged is limited; listener carries burden of communication. Examiner cannot identify materials provided from patient response.">
					description = <"">
				>
				["at0075"] = <
					text = <"Mute, global aphasia; no usable speech or auditory comprehension.">
					description = <"">
				>
				["at0082"] = <
					text = <"Extinction and inattention">
					description = <"">
				>
				["at0083"] = <
					text = <"No abnormality.">
					description = <"">
				>
				["at0084"] = <
					text = <"Visual, tactile, auditory, spatial, or personal inattention, or extinction to bilateral simultaneous stimulation in one of the sensory modalities.">
					description = <"">
				>
				["at0085"] = <
					text = <"Profound hemi-inattention or extinction to more than one modality; does not recognize own hand or orients to only one side of space.">
					description = <"">
				>
				["at0086"] = <
					text = <"Total score">
					description = <"The total score is the arithmetic sum of all 15 responses, excluding any untestable responses.">
				>
				["at0092"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0031"] = <
					text = <"Motor - left arm">
					description = <"Observation of left arm motor function.">
					comment = <"Only in the case of amputation or joint fusion at the shoulder, the examiner should record the score as 'Untestable' and record the reason in the 'Reason left arm untestable' data element. 
Implementation note: Although the 'Untestable' value is included in the ordinal to allow its inclusion in the user interface dropdown menu, NIHSS does not assign a numerical score for the 'Untestable' option in the total NIHSS score.">
				>
				["at0032"] = <
					text = <"No drift; limb holds 90 (or 45) degrees for full 10 seconds.">
					description = <"">
				>
				["at0033"] = <
					text = <"Drift; limb holds 90 (or 45) degrees, but drifts down before full 10 seconds; does not hit bed or other support.">
					description = <"">
				>
				["at0034"] = <
					text = <"Some effort against gravity; limb cannot get to or maintain (if cued) 90 (or 45) degrees, drifts down  to bed, but has some effort against gravity.">
					description = <"">
				>
				["at0124"] = <
					text = <"No effort against gravity; limb falls.">
					description = <"">
				>
				["at0035"] = <
					text = <"No movement.">
					description = <"">
				>
				["at0036"] = <
					text = <"Untestable: Amputation or joint fusion.">
					description = <"">
				>
				["at0038"] = <
					text = <"Motor - right arm">
					description = <"Observation of right arm motor function.">
					comment = <"Only in the case of amputation or joint fusion at the shoulder, the examiner should record the score as 'Untestable' and record the reason in the 'Reason right arm untestable' data element. 
Implementation note: Although the 'Untestable' value is included in the ordinal to allow its inclusion in the user interface dropdown menu, NIHSS does not assign a numerical score for the 'Untestable' option in the total NIHSS score.">
				>
				["at0039"] = <
					text = <"No drift; limb holds 90 (or 45) degrees for full 10 seconds.">
					description = <"">
				>
				["at0040"] = <
					text = <"Drift; limb holds 90 (or 45) degrees, but drifts down before full 10 seconds; does not hit bed or other support.">
					description = <"">
				>
				["at0041"] = <
					text = <"Some effort against gravity; limb cannot get to or maintain (if cued) 90 (or 45) degrees, drifts down  to bed, but has some effort against gravity.">
					description = <"">
				>
				["at0042"] = <
					text = <"No effort against gravity; limb falls.">
					description = <"">
				>
				["at0043"] = <
					text = <"No movement.">
					description = <"">
				>
				["at0044"] = <
					text = <"Untestable: Amputation or joint fusion.">
					description = <"">
				>
				["at0046"] = <
					text = <"Motor - left leg">
					description = <"Observation of left leg motor function.">
					comment = <"Only in the case of amputation or joint fusion at the hip, the examiner should record the score as 'Untestable' and record the reason in the 'Reason left leg untestable' data element. 
Implementation note: Although the 'Untestable' value is included in the ordinal to allow its inclusion in the user interface dropdown menu, NIHSS does not assign a numerical score for the 'Untestable' option in the total NIHSS score.">
				>
				["at0047"] = <
					text = <"No drift; leg holds 30-degree position for full 5 seconds.">
					description = <"">
				>
				["at0048"] = <
					text = <"Drift; leg falls by the end of the 5-second period but does not hit the bed.">
					description = <"">
				>
				["at0049"] = <
					text = <"Some effort against gravity; leg falls to bed by 5 seconds but has some effort against gravity.">
					description = <"">
				>
