description
	original_author = <
		["name"] = <"Ian McNicoll">
		["organisation"] = <"freshEHR Clinical Informatics, United Kingdom">
		["email"] = <"ian.mcnicoll@freshEHR.com">
		["date"] = <"2009-07-17">
