					description = <"Häufige ungezielte Bewegungen, atmet gegen den Respirator.">
				>
				["at0008"] = <
					text = <"Unruhig">
					description = <"Ängstlich aber Bewegungen nicht aggressiv oder lebhaft.">
				>
				["at0009"] = <
					text = <"Aufmerksam und ruhig">
