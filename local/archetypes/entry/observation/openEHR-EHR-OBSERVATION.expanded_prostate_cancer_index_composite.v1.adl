definition
	OBSERVATION[at0000] matches {    -- Expanded Prostate Cancer Index Composite (EPIC)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0313] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Over the past 4 weeks, how often have you leaked urine?
										value matches {
											1|[local::at0005],
											2|[local::at0006],
											3|[local::at0007],
											4|[local::at0008],
											5|[local::at0009]
										}
									}
									ELEMENT[at0162] occurrences matches {0..1} matches {    -- Over the past 4 weeks, how often have you urinated blood?
										value matches {
											1|[local::at0163],
											2|[local::at0164],
											3|[local::at0165],
											4|[local::at0166],
											5|[local::at0167]
										}
									}
									ELEMENT[at0168] occurrences matches {0..1} matches {    -- Over the past 4 weeks, how often have you had pain or burning with urination?
										value matches {
											1|[local::at0169],
											2|[local::at0170],
											3|[local::at0171],
											4|[local::at0172],
											5|[local::at0173]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Which of the following best describes your urinary control during the last 4 weeks?
										value matches {
											1|[local::at0011],
											2|[local::at0012],
											3|[local::at0013],
											4|[local::at0014]
										}
									}
									ELEMENT[at0222] occurrences matches {0..1} matches {    -- How many pads or adult diapers per day did you usually use to control leakage during the last 4 weeks?
										value matches {
											0|[local::at0315],
											1|[local::at0316],
											2|[local::at0317],
											3|[local::at0318]
										}
									}
									CLUSTER[at0319] occurrences matches {0..1} matches {    -- How big a problem, if any, had each of the following been for you during the last 4 weeks?
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0022] occurrences matches {0..1} matches {    -- Dripping or leaking urine
												value matches {
