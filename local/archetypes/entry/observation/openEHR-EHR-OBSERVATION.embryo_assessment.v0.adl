definition
	OBSERVATION[at0000] matches {    -- Oocyte and embryo assessment
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Label
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {    -- ID
										value matches {
											DV_IDENTIFIER matches {*}
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Developmental stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0043,    -- Oocyte
													at0037,    -- Pronuclear stage
													at0038,    -- 2-cell stage
													at0039,    -- 4-cell stage
													at0040,    -- 8-cell stage
													at0041,    -- Morula
													at0042]    -- Blastocyst
												}
											}
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0044] occurrences matches {0..*} matches {    -- Developmental event
										value matches {
											DV_TEXT matches {*}
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0045,    -- tPB2 extrusion
													at0046,    -- tPNa
													at0047,    -- tPNf
													at0048,    -- t2
													at0049,    -- t3
													at0050,    -- t4
													at0051,    -- t5
													at0052,    -- t6
													at0053,    -- t7
													at0054,    -- t8
													at0055,    -- tSC
													at0056,    -- tM
													at0057,    -- tSB
													at0058,    -- tB
													at0059,    -- tE
													at0060,    -- tHN
													at0061]    -- tHB
												}
											}
										}
									}
									allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {    -- Morphological findings
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exam_embryo\.v0|openEHR-EHR-CLUSTER\.exam_blastocyst\.v0|openEHR-EHR-CLUSTER\.exam_oocyte\.v0|openEHR-EHR-CLUSTER\.exam_zygote\.v0/}
									}
									allow_archetype CLUSTER[at0022] occurrences matches {0..*} matches {    -- Image representation
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Time post-insemination
										value matches {
											DV_DURATION matches {*}
										}
									}
									ELEMENT[at0024] occurrences matches {0..*} matches {    -- Interpretation
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0025,    -- Suitable for clinical use
													at0026]    -- Unsuitable for clinical use
												}
											}
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					POINT_EVENT[at0027] occurrences matches {0..1} matches {    -- Fertilisation check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0028] occurrences matches {0..1} matches {    -- Syngamy check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0029] occurrences matches {0..1} matches {    -- Early cleavage check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0030] occurrences matches {0..1} matches {    -- Day-2 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0031] occurrences matches {0..1} matches {    -- Day-3 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0032] occurrences matches {0..1} matches {    -- Day-4 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0033] occurrences matches {0..1} matches {    -- Day-5 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Culture dish ID
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008,    -- Manual
									at0009]    -- Time-lapse
								}
							}
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


