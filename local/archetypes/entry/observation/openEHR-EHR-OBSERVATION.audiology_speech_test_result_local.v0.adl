concept
	[at0000]	-- Speech intelligibility test result
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Mareike Buhl ">
		["organisation"] = <"Hearing Institute, Institut Pasteur, Paris, France ">
		["email"] = <"mareike.buhl@pasteur.fr">
