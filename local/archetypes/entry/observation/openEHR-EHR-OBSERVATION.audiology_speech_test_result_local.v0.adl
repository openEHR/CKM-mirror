definition
	OBSERVATION[at0000] matches {    -- Speech intelligibility test result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0132] matches {    -- Tested Side
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0133,    -- Left ear
													at0134,    -- Right ear
													at0135]    -- Binaural
												}
											}
										}
									}
									CLUSTER[at0010] occurrences matches {0..*} matches {    -- Measurement
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0011] matches {    -- Stimulus level
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::123]>
														list = <
															["1"] = <
																units = <"dB[SPL]{SPL}">
																magnitude = <|-50.0..200.0|>
																precision = <|2|>
															>
														>
													>
												}
											}
											ELEMENT[at0015] matches {    -- Speech intelligibility
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														type matches {2}
														precision matches {|0|}
													}
												}
											}
											ELEMENT[at0202] occurrences matches {0..1} matches {    -- Number of test list
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0017] occurrences matches {0..*} matches {    -- Speech recognition threshold
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0194] matches {    -- Target speech intelligibility
												value matches {
													DV_PROPORTION matches {
														type matches {2,3}
													}
												}
											}
											ELEMENT[at0018] occurrences matches {0..1} matches {    -- Speech level to achieve target
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::123]>
														list = <
															["1"] = <
																units = <"dB[SPL]{SPL}">
																magnitude = <|-50.0..200.0|>
																precision = <|2|>
															>
														>
													>
												}
											}
											ELEMENT[at0019] occurrences matches {0..1} matches {    -- Signal-to-noise ratio to achieve target
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::123]>
														list = <
															["1"] = <
																units = <"dB[SPL]{SNR}">
																magnitude = <|-50.0..200.0|>
																precision = <|2|>
															>
														>
													>
												}
											}
											ELEMENT[at0201] occurrences matches {0..1} matches {    -- SRT calculation method
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0203] occurrences matches {0..1} matches {    -- Number of test list
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Overall comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0163] occurrences matches {0..1} matches {    -- No test result
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam\.v1/}
									}
									ELEMENT[at0200] matches {    -- Training condition
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0023] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0164] occurrences matches {0..1} matches {    -- Hearing device during test
										items cardinality matches {1..*; unordered} matches {
											allow_archetype CLUSTER[at0165] matches {    -- Hearing device
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.device\.v1/}
											}
											ELEMENT[at0166] matches {    -- Side of hearing device
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0167,    -- Left
															at0168]    -- Right
														}
													}
												}
											}
											ELEMENT[at0169] occurrences matches {0..1} matches {    -- Comment
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0170] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0153] occurrences matches {0..1} matches {    -- Language proficiency of the listener
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0154,    -- Native
													at0155,    -- Fluent
													at0156,    -- Basic
													at0157]    -- None
												}
											}
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0032] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0145] occurrences matches {0..1} matches {    -- Test name
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0186] occurrences matches {0..1} matches {    -- Test language
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0187,    -- English
									at0188,    -- French
									at0189,    -- Spanish
									at0190,    -- German
									at0191,    -- Italian
									at0192]    -- ...
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {    -- Voice type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0055,    -- Female
									at0056,    -- Male
									at0204,    -- Synthetic female
									at0205]    -- Synthetic male
								}
							}
						}
					}
					ELEMENT[at0101] occurrences matches {0..1} matches {    -- Announcement
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0102,    -- Sentence
									at0103,    -- Non-speech auditory cue
									at0104,    -- Visual cue
									at0105]    -- Without
								}
							}
						}
					}
					ELEMENT[at0196] occurrences matches {0..1} matches {    -- Fixed stimulus
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0197,    -- Speech
									at0198]    -- Noise
								}
							}
						}
					}
					ELEMENT[at0110] occurrences matches {0..1} matches {    -- Presentation level of fixed stimulus
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::123]>
								list = <
									["1"] = <
										units = <"dB[SPL]{SPL}">
										magnitude = <|-50.0..200.0|>
										precision = <|2|>
									>
								>
							>
						}
					}
					CLUSTER[at0107] occurrences matches {0..1} matches {    -- Adaptive test procedure details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0091] occurrences matches {0..1} matches {    -- Starting level of varied stimulus
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::123]>
										list = <
											["1"] = <
												units = <"dB">
												precision = <|0|>
											>
										>
									>
								}
							}
							ELEMENT[at0090] occurrences matches {0..1} matches {    -- Starting step size
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::123]>
										list = <
											["1"] = <
												units = <"dB">
												precision = <|0|>
											>
										>
									>
								}
							}
							ELEMENT[at0109] occurrences matches {0..1} matches {    -- Final step size
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::123]>
										list = <
											["1"] = <
												units = <"dB">
												precision = <|0|>
											>
										>
									>
								}
							}
						}
					}
					ELEMENT[at0070] matches {    -- Type of speech stimulus
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0071,    -- Nonsense syllable
									at0072,    -- Nonsense CVC
									at0073,    -- Nonsense word
									at0074,    -- Monosyllabic word
									at0075,    -- Spondee word
									at0076,    -- Trochee word
									at0137,    -- Matrix sentence
									at0138,    -- Meaningful sentence
									at0099,    -- Number
									at0100,    -- Digit triplet
									at0179]    -- Phoneme
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0040] occurrences matches {0..1} matches {    -- Type of noise stimulus
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0041,    -- White noise
									at0042,    -- Speech spectrum noise
									at0043,    -- Multitalker babble
									at0044]    -- Alternate speaker
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0140] occurrences matches {0..1} matches {    -- Noise stimulus details
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0181] occurrences matches {0..1} matches {    -- Test environment
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0182,    -- Sound treated room
									at0183,    -- Non-sound treated room
									at0184]    -- Free-field
								}
							}
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0185] occurrences matches {0..1} matches {    -- Test environment details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device\.v1/}
					}
					ELEMENT[at0172] occurrences matches {0..1} matches {    -- Presentation method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0173,    -- Loudspeaker
									at0174,    -- Insert earphone
									at0175,    -- Headphone
									at0176,    -- Direct streaming to sound processor
									at0177]    -- Live voice
								}
							}
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0178] occurrences matches {0..1} matches {    -- Presentation method details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device\.v1/}
					}
					ELEMENT[at0093] matches {    -- Presented sensory modalities
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0094,    -- Auditory test
									at0095,    -- Audiovisual test
									at0096]    -- Visual test
								}
							}
						}
					}
					ELEMENT[at0064] occurrences matches {0..1} matches {    -- Response set
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0065,    -- Open set
									at0066]    -- Closed set
								}
							}
						}
					}
					ELEMENT[at0060] occurrences matches {0..1} matches {    -- Response mode
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0171,    -- Computer
									at0061,    -- Vocal
									at0062,    -- Picture pointing
									at0063,    -- Written response alternatives
									at0086]    -- Concrete object pointing
								}
							}
						}
					}
					ELEMENT[at0113] matches {    -- Scoring method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0114,    -- Word scoring
									at0115,    -- Sentence scoring
									at0116,    -- Phoneme scoring
									at0117]    -- Keyword scoring
								}
							}
						}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Speech intelligibility test result">
					description = <"Record of results from an audiological speech intelligibility test conducted for the purpose of assessing speech recognition, speech discrimination and speech intelligibility.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0010"] = <
					text = <"Measurement">
					description = <"The obtained performance result (speech intelligibility in percent) for a test list, sentence or word presented at the given stimulus level.">
					comment = <"Can be used for any speech intelligibility result in fixed or adaptive measurements. ">
				>
				["at0011"] = <
					text = <"Stimulus level">
					description = <"The level of the varied stimulus.">
					comment = <"Corresponds to speech or noise, depending on which level is varied during the measurement.">
				>
				["at0015"] = <
					text = <"Speech intelligibility">
					description = <"The obtained performance result in percent (speech intelligibility) at a given stimulus level.">
				>
				["at0017"] = <
					text = <"Speech recognition threshold">
					description = <"The speech recognition threshold (or speech reception threshold; SRT) describing a speech level or signal-to-noise ratio required to reach the given target speech intelligibility.">
				>
				["at0018"] = <
					text = <"Speech level to achieve target">
					description = <"The level of the speech stimulus required to achieve the target speech intelligibility in a measurement in quiet.">
				>
				["at0019"] = <
					text = <"Signal-to-noise ratio to achieve target">
					description = <"The level difference (signal-to-noise ratio; SNR) between the speech and noise stimulus required to achieve the target speech intelligibility in a measurement in noise.">
				>
				["at0022"] = <
					text = <"Overall comment">
					description = <"Additional narrative about the speech intelligibility measurement not captured in other fields.">
				>
				["at0023"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0032"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0040"] = <
					text = <"Type of noise stimulus">
					description = <"The type of noise stimulus used during speech in noise testing. Further details can be given in 'Noise type details'.">
					comment = <"Not applicable for speech in quiet tests.">
				>
				["at0041"] = <
					text = <"White noise">
					description = <"Noise that has the same power at all frequencies (i.e., a flat power spectrum).">
				>
				["at0042"] = <
					text = <"Speech spectrum noise">
					description = <"Noise spectrum that approximates the average long term spectrum of adult male speech and has a slope below 100 Hz of +6 dB/octave, a flat spectrum between 100 Hz and 320 Hz, and above 320 Hz a slope of -6 dB/octave.">
				>
				["at0043"] = <
					text = <"Multitalker babble">
					description = <"A recording of the voices of many people who are talking simultaneously, resulting in an unintelligible babble.">
				>
				["at0044"] = <
					text = <"Alternate speaker">
					description = <"The masker is a single person speaking and this speaker is different to the speaker used for the test stimulus.">
				>
				["at0054"] = <
					text = <"Voice type">
					description = <"The voice type (sex) of the speech stimulus.">
				>
				["at0055"] = <
					text = <"Female">
					description = <"Stimuli presented by a female talker.">
				>
				["at0056"] = <
					text = <"Male">
					description = <"Stimuli presented by a male talker.">
				>
				["at0060"] = <
					text = <"Response mode">
					description = <"The mode used to enter or record the response.">
				>
				["at0061"] = <
					text = <"Vocal">
					description = <"The response is given verbally.">
				>
				["at0062"] = <
					text = <"Picture pointing">
					description = <"The response is given by pointing to a picture of the stimulus item.">
				>
				["at0063"] = <
					text = <"Written response alternatives">
					description = <"The response is given by pointing to written text corresponding to the stimulus item.">
				>
				["at0064"] = <
					text = <"Response set">
					description = <"The type of the response set.">
				>
				["at0065"] = <
					text = <"Open set">
					description = <"The response set is unlimited.">
				>
				["at0066"] = <
					text = <"Closed set">
					description = <"The response set is limited.">
				>
				["at0070"] = <
					text = <"Type of speech stimulus">
					description = <"The type of speech stimulus used.">
				>
				["at0071"] = <
					text = <"Nonsense syllable">
					description = <"A consonant-vowel (CV) or CCV or VC or VCC item that is not a real word but is phonotactically correct.">
				>
				["at0072"] = <
					text = <"Nonsense CVC">
					description = <"Nonsense word comprising a consonant, then a vowel, then a final consonant, for example, \"wub\" or \"yat\".">
				>
				["at0073"] = <
					text = <"Nonsense word">
					description = <"A speech stimulus that is not a real word but is phonotactically correct.">
				>
				["at0074"] = <
					text = <"Monosyllabic word">
					description = <"A word comprised of a single syllable. For example, 'green'.">
				>
				["at0075"] = <
					text = <"Spondee word">
					description = <"A word comprised of 2 syllables with equal stress on each syllable. For example, 'sunshine'.">
				>
				["at0076"] = <
					text = <"Trochee word">
					description = <"A word that is comprised of two syllables with stress on the first syllable. For example 'bucket'.">
				>
				["at0086"] = <
					text = <"Concrete object pointing">
					description = <"The response is given by pointing to a 3-dimensional object.">
				>
				["at0090"] = <
					text = <"Starting step size">
					description = <"The initial level stepsize for level changes after a response.">
					comment = <"Applicable for adaptive tests.">
				>
				["at0091"] = <
					text = <"Starting level of varied stimulus">
					description = <"The initial presentation level of the stimulus varied during the measurement.">
					comment = <"Speech level for measurements in quiet and varied stimulus level for measurements in noise. ">
				>
				["at0093"] = <
					text = <"Presented sensory modalities">
					description = <"The method used to present the speech test stimulus.">
					comment = <"For example: a visual stimulus can be used to test lip reading skills.">
				>
				["at0094"] = <
					text = <"Auditory test">
					description = <"An auditory stimulus is presented to the test subject.">
				>
				["at0095"] = <
					text = <"Audiovisual test">
					description = <"A combination of auditory and visual stimuli are presented to the test subject.">
				>
