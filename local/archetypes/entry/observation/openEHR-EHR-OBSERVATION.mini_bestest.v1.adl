definition
	OBSERVATION[at0000] matches {    -- Mini-Balance Evaluation Systems Test (Mini-BESTest)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0005] occurrences matches {0..1} matches {    -- Anticipatory
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {    -- 1. Sit to stand
												value matches {
													2|[local::at0007],
													1|[local::at0008],
													0|[local::at0009]
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- 2. Rise to toes
												value matches {
													2|[local::at0011],
													1|[local::at0012],
													0|[local::at0013]
												}
											}
											CLUSTER[at0015] occurrences matches {0..1} matches {    -- 3. Stand on one leg
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0024] occurrences matches {0..1} matches {    -- Time left leg trial 1
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0025] occurrences matches {0..1} matches {    -- Time left leg trial 2
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0016] occurrences matches {0..1} matches {    -- Left stand on one leg
														value matches {
															2|[local::at0017],
															1|[local::at0018],
															0|[local::at0019]
														}
													}
													ELEMENT[at0026] occurrences matches {0..1} matches {    -- Time right leg trial 1
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0027] occurrences matches {0..1} matches {    -- Time right leg trial 2
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0020] occurrences matches {0..1} matches {    -- Right stand on one leg
														value matches {
															2|[local::at0021],
															1|[local::at0022],
															0|[local::at0023]
														}
													}
												}
											}
											ELEMENT[at0028] occurrences matches {0..1} matches {    -- Sub score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..6|}
													}
												}
											}
										}
									}
									CLUSTER[at0029] occurrences matches {0..1} matches {    -- Reactive postural control
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0030] occurrences matches {0..1} matches {    -- 4. Compensatory stepping correction - forward
												value matches {
													2|[local::at0031],
													1|[local::at0032],
													0|[local::at0033]
												}
											}
											ELEMENT[at0034] occurrences matches {0..1} matches {    -- 5. Compensatory stepping correction - backward
												value matches {
													2|[local::at0035],
													1|[local::at0036],
													0|[local::at0037]
												}
											}
											CLUSTER[at0039] occurrences matches {0..1} matches {    -- 6. Compensatory stepping correction - lateral
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0040] occurrences matches {0..1} matches {    -- Left
														value matches {
															2|[local::at0041],
															1|[local::at0042],
															0|[local::at0043]
														}
													}
													ELEMENT[at0044] occurrences matches {0..1} matches {    -- Right
														value matches {
															2|[local::at0045],
															1|[local::at0046],
															0|[local::at0047]
														}
													}
												}
											}
											ELEMENT[at0052] occurrences matches {0..1} matches {    -- Sub score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..6|}
													}
												}
											}
										}
									}
									CLUSTER[at0055] occurrences matches {0..1} matches {    -- Sensory orientation
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0102] occurrences matches {0..1} matches {    -- Stance time - eyes open
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::128]>
														list = <
															["1"] = <
																units = <"s">
																magnitude = <|>=0.0|>
																precision = <|0|>
															>
														>
													>
												}
											}
											ELEMENT[at0056] occurrences matches {0..1} matches {    -- 7. Stance (feet together); eyes open, firm surface
												value matches {
													2|[local::at0057],
													1|[local::at0058],
													0|[local::at0059]
												}
											}
											ELEMENT[at0103] occurrences matches {0..1} matches {    -- Stance time - eyes closed
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::128]>
														list = <
															["1"] = <
																units = <"s">
																magnitude = <|>=0.0|>
																precision = <|0|>
															>
														>
													>
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {    -- 8. Stance (feet together); eyes closed, foam surface
												value matches {
													2|[local::at0061],
													1|[local::at0062],
													0|[local::at0063]
												}
											}
											ELEMENT[at0104] occurrences matches {0..1} matches {    -- Incline time
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::128]>
														list = <
															["1"] = <
																units = <"s">
																magnitude = <|>=0.0|>
																precision = <|0|>
															>
														>
													>
												}
											}
											ELEMENT[at0064] occurrences matches {0..1} matches {    -- 9. Incline - eyes closed
												value matches {
													2|[local::at0065],
													1|[local::at0066],
													0|[local::at0067]
												}
											}
											ELEMENT[at0068] occurrences matches {0..1} matches {    -- Sub score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..6|}
													}
												}
											}
										}
									}
									CLUSTER[at0069] occurrences matches {0..1} matches {    -- Dynamic gait
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0070] occurrences matches {0..1} matches {    -- 10. Change in gait speed
												value matches {
													2|[local::at0071],
													1|[local::at0072],
													0|[local::at0073]
												}
											}
											ELEMENT[at0079] occurrences matches {0..1} matches {    -- 11. Walk with head turns - horizontal
												value matches {
													2|[local::at0080],
													1|[local::at0081],
													0|[local::at0082]
												}
											}
											ELEMENT[at0074] occurrences matches {0..1} matches {    -- 12. Walk with pivot turns
												value matches {
													2|[local::at0075],
													1|[local::at0076],
													0|[local::at0077]
												}
											}
											ELEMENT[at0083] occurrences matches {0..1} matches {    -- 13. Step over obstacles
												value matches {
													2|[local::at0084],
													1|[local::at0085],
													0|[local::at0086]
												}
											}
											CLUSTER[at0088] occurrences matches {0..1} matches {    -- 14. Timed up & go with dual task [3 meter walk]
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0097] occurrences matches {0..1} matches {    -- Time TUG
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0098] occurrences matches {0..1} matches {    -- Time Dual Task TUG
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"s">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0105] occurrences matches {0..1} matches {    -- Speed change
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::380]>
																list = <
																	["1"] = <
																		units = <"%">
																	>
																>
															>
														}
													}
													ELEMENT[at0093] occurrences matches {0..1} matches {    -- Dual Task TUG
														value matches {
															2|[local::at0094],
															1|[local::at0095],
															0|[local::at0096]
														}
													}
												}
											}
											ELEMENT[at0099] occurrences matches {0..1} matches {    -- Sub score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..10|}
													}
												}
											}
										}
									}
									ELEMENT[at0100] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..28|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {*}    -- Item tree
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Mini-Balance Evaluation Systems Test (Mini-BESTest)">
					description = <"A quantitative balance assessment tool.">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0005"] = <
					text = <"Anticipatory">
					description = <"">
				>
				["at0006"] = <
					text = <"1. Sit to stand">
					description = <"">
				>
				["at0007"] = <
					text = <"Normal">
					description = <"Comes to stand without use of hands and stabilizes independently.">
				>
				["at0008"] = <
					text = <"Moderate">
					description = <"Comes to stand WITH use of hands on first attempt.">
				>
				["at0009"] = <
					text = <"Severe">
					description = <"Unable to stand up from chair without assistance, OR needs several attempts with use of hands.">
				>
				["at0010"] = <
					text = <"2. Rise to toes">
					description = <"">
				>
				["at0011"] = <
					text = <"Normal">
					description = <"Stable for 3 s with maximum height.">
				>
				["at0012"] = <
					text = <"Moderate">
					description = <"Heels up, but not full range (smaller than when holding hands), OR noticeable instability for 3 s.">
				>
				["at0013"] = <
					text = <"Severe">
					description = <"≤ 3 s.">
				>
				["at0015"] = <
					text = <"3. Stand on one leg">
					description = <"">
				>
				["at0016"] = <
					text = <"Left stand on one leg">
					description = <"">
					comment = <"Use the trial with the longest time.">
				>
				["at0017"] = <
					text = <"Normal">
					description = <"20 s.">
				>
				["at0018"] = <
					text = <"Moderate">
					description = <"< 20 s.">
				>
				["at0019"] = <
					text = <"Severe">
					description = <"Unable.">
				>
				["at0020"] = <
					text = <"Right stand on one leg">
					description = <"">
					comment = <"Use the trial with the longest time.">
				>
				["at0021"] = <
					text = <"Normal">
					description = <"20 s.">
				>
				["at0022"] = <
					text = <"Moderate">
					description = <"< 20 s.">
				>
				["at0023"] = <
					text = <"Severe">
					description = <"Unable.">
				>
				["at0024"] = <
					text = <"Time left leg trial 1">
					description = <"">
				>
				["at0025"] = <
					text = <"Time left leg trial 2">
					description = <"">
				>
				["at0026"] = <
					text = <"Time right leg trial 1">
					description = <"">
				>
				["at0027"] = <
					text = <"Time right leg trial 2">
					description = <"">
				>
				["at0028"] = <
					text = <"Sub score">
					description = <"The sum of all items in the anticipatory section.">
					comment = <"To calculate the sub-score use the side (left or right) with the lowest numerical score (i.e. the worse side).">
				>
				["at0029"] = <
					text = <"Reactive postural control">
					description = <"">
				>
				["at0030"] = <
					text = <"4. Compensatory stepping correction - forward">
					description = <"">
				>
				["at0031"] = <
					text = <"Normal">
					description = <"Recovers independently with a single, large step (second realignment step is allowed).">
				>
				["at0032"] = <
					text = <"Moderate">
					description = <"More than one step used to recover equilibrium.">
				>
				["at0033"] = <
					text = <"Severe">
					description = <"No step, OR would fall if not caught, OR falls spontaneously.">
				>
				["at0034"] = <
					text = <"5. Compensatory stepping correction - backward">
					description = <"">
				>
				["at0035"] = <
					text = <"Normal">
					description = <"Recovers independently with a single, large step.">
				>
				["at0036"] = <
					text = <"Moderate">
					description = <"More than one step used to recover equilibrium.">
				>
				["at0037"] = <
					text = <"Severe">
					description = <"No step, OR would fall if not caught, OR falls spontaneously.">
				>
				["at0039"] = <
					text = <"6. Compensatory stepping correction - lateral">
					description = <"">
				>
				["at0040"] = <
					text = <"Left">
					description = <"">
				>
				["at0041"] = <
					text = <"Normal">
					description = <"Recovers independently with 1 step  (crossover or lateral OK).">
				>
				["at0042"] = <
					text = <"Moderate">
