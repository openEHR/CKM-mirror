definition
	OBSERVATION[at0000] matches {    -- Intraocular pressure
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0057] matches {    -- Eye examined
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0058,    -- Left eye
													at0059]    -- Right eye
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {    -- Pressure
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|0.0..999.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Tonometry type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0047,    -- Goldmann
													at0048,    -- Perkins
													at0049,    -- Tono-Pen
													at0050,    -- Rebound
													at0051,    -- Dynamic Contour
													at0053,    -- Transpalpebral
													at0066,    -- Non-contact
													at0095,    -- Implanted pressure sensor
													at0096,    -- Goldmann-correlated ocular response analysis
													at0099,    -- Pneumatotonometer
													at0100]    -- Schiøtz
												}
											}
										}
									}
									ELEMENT[at0081] occurrences matches {0..1} matches {    -- Corrected pressure
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|0.0..30000.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0082] occurrences matches {0..1} matches {    -- Correction type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0090,    -- Corneal-compensated ocular response analysis
													at0091,    -- Biomechanically corrected noncontact tonometry
													at0092]    -- Corrected for central corneal thickness
												}
											}
										}
									}
									allow_archetype CLUSTER[at0093] occurrences matches {0..1} matches {    -- Citation of central corneal thickness used for correction
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.citation\.v0/}
									}
									ELEMENT[at0083] occurrences matches {0..1} matches {    -- Palpation finding
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0085,    -- Unpressurized
													at0086,    -- Softer than normal
													at0087,    -- Normal
													at0088,    -- Harder than normal
													at0089]    -- Very hard
												}
											}
										}
									}
									ELEMENT[at0123] occurrences matches {0..1} matches {    -- Inspection finding
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0124,    -- The eye shows signs of being unpressurized.
													at0125,    -- The eye shows no signs of being unpressurized.
													at0126]    -- Uncertain
												}
											}
										}
									}
									ELEMENT[at0094] occurrences matches {0..1} matches {    -- Waveform score
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::755]>
												list = <
													["1"] = <
														units = <"[arb'U]">
														magnitude = <|0.0..10.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0061] occurrences matches {0..*} matches {    -- Clinical interpretation
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0078] occurrences matches {0..*} matches {    -- Test not done
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0007] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0117] occurrences matches {0..1} matches {    -- Pupil state
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0118,    -- Medically induced miosis
													at0122,    -- Medically induced mydriasis
													at0120,    -- Pathological pupil state
													at0121]    -- Normal
												}
											}
										}
									}
									ELEMENT[at0109] occurrences matches {0..1} matches {    -- Position
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0110,    -- Standing
													at0111,    -- Sitting
													at0112,    -- Lying in supine position
													at0113,    -- Lying in prone position
													at0114,    -- Lying in left lateral decubitus position
													at0115]    -- Lying in right lateral decubitus position
												}
											}
										}
									}
									ELEMENT[at0127] occurrences matches {0..1} matches {    -- Anaesthesia state
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0128,    -- General anesthesia
													at0129,    -- Sedation
													at0130]    -- No anesthesia
												}
											}
										}
									}
									ELEMENT[at0074] occurrences matches {0..*} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0134] occurrences matches {0..1} matches {    -- Medication regimen exposure
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0135,    -- Complete regimen exposure (presumed)
													at0136,    -- Incomplete regimen exposure (presumed)
													at0137,    -- Uncertain regimen exposure.
													at0138]    -- No IOP lowering drugs
												}
											}
										}
									}
									ELEMENT[at0132] occurrences matches {0..1} matches {    -- Number of topical pressure lowering medication classes
										value matches {
											DV_COUNT matches {*}
										}
									}
									ELEMENT[at0133] occurrences matches {0..1} matches {    -- Number of systemic pressure lowering medication classes
										value matches {
											DV_COUNT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0068] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0079] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0055] occurrences matches {0..1} matches {    -- Device details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
		}
	}


ontology
	terminologies_available = <"SNOMED-CT", "LOINC">
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Intraocular pressure">
					description = <"The local measurement of intraocular pressure, most commonly using a tonometry device.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0042"] = <
					text = <"Pressure">
					description = <"Intraocular pressure, as measured by the type of tonometry described in the 'Tonometry type' element.">
					comment = <"For devices that only output one pressure value per measurement, this element is used. For devices that output multiple pressure values per measurement, this element is used for the output that did not undergo a correction step or is correlated with an uncorrected method by design. Use the 'Corrected pressure' and 'Correction type' elements for outputs that have been corrected for factors such as corneal thickness or biomechanics.">
				>
				["at0046"] = <
					text = <"Tonometry type">
					description = <"Type of tonometry used to measure the intraocular pressure.">
				>
				["at0047"] = <
					text = <"Goldmann">
					description = <"A Goldmann applanation tonometer was used to perform the test.">
				>
				["at0048"] = <
					text = <"Perkins">
					description = <"A Perkins tonometer was used to perform the test.">
				>
				["at0049"] = <
					text = <"Tono-Pen">
					description = <"A Tono-Pen applanation tonometer was used to perform the test.">
				>
				["at0050"] = <
					text = <"Rebound">
					description = <"A rebound tonometer, such as an iCare® device, was used to perform the test.">
				>
				["at0051"] = <
					text = <"Dynamic Contour">
					description = <"A Dynamic Contour Tonometer, such as a PASCAL® device was used to perform the test.">
				>
				["at0053"] = <
					text = <"Transpalpebral">
					description = <"A tonometer that measures through the eyelid, such as the TGDc-01, was used to perform the test.">
				>
				["at0055"] = <
					text = <"Device details">
					description = <"Details about the tonometry device used to measure intraocular pressure.">
				>
				["at0057"] = <
					text = <"Eye examined">
					description = <"Identification of the eye under examination.">
				>
				["at0058"] = <
					text = <"Left eye">
					description = <"The left eye was examined.">
				>
				["at0059"] = <
					text = <"Right eye">
					description = <"The right eye was examined.">
				>
				["at0061"] = <
					text = <"Clinical interpretation">
					description = <"Single word, phrase or brief description that represents the clinical meaning and significance of the physical examination findings.">
				>
				["at0063"] = <
					text = <"Comment">
					description = <"Additional narrative about the measurement, not captured in other fields.">
				>
				["at0066"] = <
					text = <"Non-contact">
					description = <"A non-contact tonometer was used to perform the test.">
				>
				["at0068"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0074"] = <
					text = <"Confounding factors">
					description = <"Description of any incidental factors related to the state of the subject which may affect clinical interpretation of the measurement.">
					comment = <"Examples: blepharospasm, reduced cooperation, measurement interrupted, difficulty holding lid open.">
				>
				["at0078"] = <
					text = <"Test not done">
					description = <"Details to explicitly record that this test was not performed.">
				>
				["at0079"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0081"] = <
					text = <"Corrected pressure">
					description = <"Corrected value for intraocular pressure.">
					comment = <"For devices that output multiple pressure values, this element should be used for the values that take additional parameters into account for a correction of the raw pressure value. The method of correction used is specified in the 'correction type' element.">
				>
				["at0082"] = <
					text = <"Correction type">
					description = <"The type of correction applied to produce the 'Corrected pressure' value.">
				>
				["at0083"] = <
					text = <"Palpation finding">
					description = <"Semi-quantitative description of the softness or hardness of the eye based on palpation with the examiners fingers.">
					comment = <"Palpation of unpressurized eyes.">
				>
				["at0085"] = <
					text = <"Unpressurized">
					description = <"The eyeball appears to lack any internal pressure.">
				>
				["at0086"] = <
					text = <"Softer than normal">
					description = <"The pressure is approximated to be below the normal range, but some pressure is present.">
				>
				["at0087"] = <
					text = <"Normal">
					description = <"The pressure is approximated to be within the normal range (10-21mmHg).">
				>
				["at0088"] = <
					text = <"Harder than normal">
					description = <"The pressure is approximated to be above the normal range, but not very hard or rock-hard.">
				>
				["at0089"] = <
					text = <"Very hard">
					description = <"The pressure appears to be extremely elevated, sometimes referred to as a rock-hard eye.">
				>
				["at0090"] = <
					text = <"Corneal-compensated ocular response analysis">
					description = <"The corneal-compensated intraocular pressure as measured by the Ocular Response Analyzer.">
				>
				["at0091"] = <
					text = <"Biomechanically corrected noncontact tonometry">
					description = <"The biomechanically corrected intraocular pressure as measured by the Corvis ST scheimpflug noncontact tonometer.">
				>
				["at0092"] = <
					text = <"Corrected for central corneal thickness">
					description = <"The pressure was corrected for central corneal thickness (CCT).">
				>
				["at0093"] = <
					text = <"Citation of central corneal thickness used for correction">
					description = <"A SLOT to cite the central corneal thickness measurement that was used as a basis for the correction.">
				>
				["at0094"] = <
					text = <"Waveform score">
					description = <"Waveform score produced by the Ocular Response Analyzer device.">
					comment = <"Higher values indicate higher reliability.">
				>
				["at0095"] = <
					text = <"Implanted pressure sensor">
					description = <"An implanted pressure sensor was used to perform the test.">
				>
				["at0096"] = <
					text = <"Goldmann-correlated ocular response analysis">
					description = <"An Ocular Response Analyzer® was used to perform the test, with the goldmann-correlated (IOPg) output.">
				>
				["at0099"] = <
					text = <"Pneumatotonometer">
					description = <"A pneumatotonometer was used to perform the test.">
				>
				["at0100"] = <
					text = <"Schiøtz">
					description = <"A Schiøtz tonometer was used to perform the test.">
				>
				["at0109"] = <
					text = <"Position">
					description = <"The position of the individual at the time of measurement.">
				>
				["at0110"] = <
					text = <"Standing">
					description = <"Standing at the time of the intraocular pressure measurement.">
				>
				["at0111"] = <
					text = <"Sitting">
					description = <"Sitting at the time of the intraocular pressure measurement.">
				>
				["at0112"] = <
					text = <"Lying in supine position">
					description = <"Lying in supine position (face up) at the time of the intraocular pressure measurement.">
				>
				["at0113"] = <
					text = <"Lying in prone position">
					description = <"Lying in prone position (face down) at the time of the intraocular pressure measurement.">
				>
				["at0114"] = <
					text = <"Lying in left lateral decubitus position">
					description = <"Lying on the left hand side at the time of the intraocular pressure measurement.">
				>
				["at0115"] = <
					text = <"Lying in right lateral decubitus position">
					description = <"Lying on the right hand side at the time of the intraocular pressure measurement.">
				>
				["at0117"] = <
					text = <"Pupil state">
					description = <"The state of the individual's pupils during the measurement.">
					comment = <"Note this is not an examination of the pupil, but an assertion on its state.">
				>
				["at0118"] = <
					text = <"Medically induced miosis">
					description = <"The pupil was constricted due to prior administration of miotic agents.">
				>
				["at0120"] = <
					text = <"Pathological pupil state">
					description = <"The pupil was in an unphysiological state due to disease, trauma, or structural abnormalities.">
				>
				["at0121"] = <
					text = <"Normal">
					description = <"The pupil is in its natural physiological state without pharmacological intervention or pathological changes.">
				>
				["at0122"] = <
					text = <"Medically induced mydriasis">
					description = <"The pupil was dilated due to prior administration of mydriatic agents.">
				>
				["at0123"] = <
					text = <"Inspection finding">
					description = <"Record of presence or absence of visible signs of a total lack of internal pressure of the examined eye.">
					comment = <"Typically reserved for rare situations where physical interaction with the globe is inadviseable, such as physically aggressive individuals or when an open globe injury is strongly suspected.">
				>
				["at0124"] = <
					text = <"The eye shows signs of being unpressurized.">
					description = <"Based on visual examination, there are signs of a total loss of intraocular pressure. For instance, the eye may have lost its round shape in the setting of significant trauma.">
				>
				["at0125"] = <
					text = <"The eye shows no signs of being unpressurized.">
					description = <"Based on visual examination, there are no signs of a total loss of intraocular pressure.">
				>
				["at0126"] = <
					text = <"Uncertain">
					description = <"Based on visual examination, it is uncertain if there are signs of a total loss of intraocular pressure.">
				>
				["at0127"] = <
					text = <"Anaesthesia state">
					description = <"An assertion on the anesthesia of the individual during the measurement.">
					comment = <"The detail of the anaesthesia procedure and specific medication used is to be recorded in other archetypes.">
				>
				["at0128"] = <
					text = <"General anesthesia">
					description = <"The patient was under general anaesthesia during the measurement.">
				>
				["at0129"] = <
					text = <"Sedation">
					description = <"The patient was under sedation during the measurement.">
				>
				["at0130"] = <
					text = <"No anesthesia">
					description = <"The patient was not under anaesthesia during the measurement.">
