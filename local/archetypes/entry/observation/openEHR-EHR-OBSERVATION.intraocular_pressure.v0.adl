definition
	OBSERVATION[at0000] matches {    -- Intraocular pressure measurement
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0057] matches {    -- Eye examined
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0058,    -- Left eye
													at0059]    -- Right eye
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {    -- Pressure
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|0.0..30000.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0081] occurrences matches {0..1} matches {    -- Corrected pressure
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|0.0..30000.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0094] occurrences matches {0..1} matches {    -- Waveform Score
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0083] occurrences matches {0..1} matches {    -- Semiquantitative Estimation
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0085,    -- Unpressurized
													at0086,    -- Low
													at0087,    -- Normal
													at0088,    -- High
													at0089]    -- Very HIgh
												}
											}
										}
									}
									ELEMENT[at0061] occurrences matches {0..*} matches {    -- Clinical interpretation
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0078] occurrences matches {0..*} matches {    -- Test not done
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0007] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Method
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0047,    -- Goldmann
													at0048,    -- Perkins
													at0049,    -- Tono-Pen
													at0050,    -- Rebound
													at0051,    -- Dynamic Contour
													at0053,    -- Transpalpebral tonometer
													at0066,    -- Non-contact tonometry
													at0095,    -- Implanted pressure sensor
													at0096,    -- Goldmann-Correlated Ocular Response Analysis
													at0097,    -- Palpation
													at0098,    -- Visual Inspection
													at0099,    -- Pneumatotonometer
													at0100]    -- Schiøtz
												}
											}
										}
									}
									ELEMENT[at0082] occurrences matches {0..1} matches {    -- Correction Type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0090,    -- IOPcc - corneal-compensated ocular response analysis
													at0091,    -- bIOP - biomechanically corrected noncontact IOP
													at0092]    -- Corrected for CCT
												}
											}
										}
									}
									ELEMENT[at0074] occurrences matches {0..*} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0101,    -- General anaesthesia
													at0102,    -- Sedation
													at0103,    -- Blepharospasm
													at0104,    -- Reduced Cooperation
													at0105]    -- Measurement Interrupted
												}
											}
										}
									}
									allow_archetype CLUSTER[at0093] occurrences matches {0..*} matches {    -- Citation of central corneal thickness used for correction
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.citation\.v0/}
									}
									ELEMENT[at0107] occurrences matches {0..1} matches {    -- Prism graduation mark on 43° offset mark
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::497]>
												list = <
													["1"] = <
														units = <"deg">
													>
												>
											>
										}
									}
									ELEMENT[at0108] occurrences matches {0..1} matches {    -- Prism graduation mark on horizontal meridian
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::497]>
												list = <
													["1"] = <
														units = <"deg">
													>
												>
											>
										}
									}
									allow_archetype CLUSTER[at0055] occurrences matches {0..1} matches {    -- Device details
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0109] occurrences matches {0..1} matches {    -- Position
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0110,    -- Standing
													at0111,    -- Sitting
													at0112,    -- Lying in supine position
													at0113,    -- Lying in prone position
													at0114,    -- Lying in left lateral decubitus position
													at0115]    -- Lying in right lateral decubitus position
												}
											}
										}
									}
									ELEMENT[at0117] occurrences matches {0..1} matches {    -- Pupil state
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0118,    -- Medically induced Miosis
													at0122,    -- Medically induced Mydriasis
													at0120,    -- Pathological Pupil State
													at0121]    -- Normal
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0068] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0079] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


