					text = <"Extension">
					description = <"Additional information required to capture local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.">
				>
				["at0081"] = <
					text = <"Corrected pressure">
					description = <"Corrected value for intraocular pressure.">
					comment = <"For devices that output multiple pressure values, this element should be used for the values that take aditional parameters into account for a correction of the raw pressure value.  The method of correction used is specified in the 'correction type' element. ">
				>
				["at0082"] = <
					text = <"Correction Type">
					description = <"The method used to produce the corrected pressure value.">
				>
				["at0083"] = <
					text = <"Semiquantitative Estimation">
					description = <"Semi-quantitative description of the hardness of the eye. Typically performed by palpation with the examiners fingers.">
