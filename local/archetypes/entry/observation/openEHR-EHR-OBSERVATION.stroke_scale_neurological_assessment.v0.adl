definition
	OBSERVATION[at0000] matches {    -- NIH Stroke Scale (NIHSS)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Baseline
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- LOC responsiveness
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008]
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- LOC questions
										value matches {
											0|[local::at0010],
											1|[local::at0011],
											2|[local::at0012]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- LOC commands
										value matches {
											0|[local::at0014],
											1|[local::at0015],
											2|[local::at0016]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Horizontal eye movements
										value matches {
											0|[local::at0018],
											1|[local::at0019],
											2|[local::at0020]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Visual fields
										value matches {
											0|[local::at0022],
											1|[local::at0023],
											2|[local::at0024],
											3|[local::at0025]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- Facial palsy
										value matches {
											0|[local::at0027],
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- Motor - left arm
										value matches {
											0|[local::at0032],
											1|[local::at0033],
											2|[local::at0034],
											3|[local::at0124],
											4|[local::at0035],
											0|[local::at0036]
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- Reason left arm untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- Motor - right arm
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043],
											0|[local::at0044]
										}
									}
									ELEMENT[at0045] occurrences matches {0..1} matches {    -- Reason right arm untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Motor - left leg
										value matches {
											0|[local::at0047],
											1|[local::at0048],
											2|[local::at0049],
											3|[local::at0050],
											4|[local::at0051],
											0|[local::at0052]
										}
									}
									ELEMENT[at0053] occurrences matches {0..1} matches {    -- Reason left leg untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0141] occurrences matches {0..1} matches {    -- Motor - right leg
										value matches {
											0|[local::at0054],
											1|[local::at0055],
											2|[local::at0056],
											3|[local::at0057],
											4|[local::at0058],
											0|[local::at0059]
										}
									}
									ELEMENT[at0060] occurrences matches {0..1} matches {    -- Reason right leg untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {    -- Limb ataxia
										value matches {
											0|[local::at0062],
											1|[local::at0063],
											2|[local::at0064],
											0|[local::at0065]
										}
									}
									ELEMENT[at0066] occurrences matches {0..1} matches {    -- Reason limb ataxia untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0067] occurrences matches {0..1} matches {    -- Sensory
										value matches {
											0|[local::at0068],
											1|[local::at0069],
											2|[local::at0070]
										}
									}
									ELEMENT[at0071] occurrences matches {0..1} matches {    -- Language
										value matches {
											0|[local::at0072],
											1|[local::at0073],
											2|[local::at0074],
											3|[local::at0075]
										}
									}
									ELEMENT[at0076] occurrences matches {0..1} matches {    -- Speech
										value matches {
											0|[local::at0077],
											1|[local::at0078],
											2|[local::at0079],
											0|[local::at0080]
										}
									}
									ELEMENT[at0081] occurrences matches {0..1} matches {    -- Reason speech untestable
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0082] occurrences matches {0..1} matches {    -- Extinction and inattention
										value matches {
											0|[local::at0083],
											1|[local::at0084],
											2|[local::at0085]
										}
									}
									ELEMENT[at0086] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..42|}
											}
										}
									}
								}
							}
						}
					}
					POINT_EVENT[at0087] occurrences matches {0..1} matches {    -- 2 hours post treatment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0088] occurrences matches {0..1} matches {    -- 24 hours post symptom onset
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0089] occurrences matches {0..1} matches {    -- 7–10 days
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0090] occurrences matches {0..1} matches {    -- 3 months
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					EVENT[at0091] occurrences matches {0..*} matches {    -- Any event
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0092] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0093] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}




ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"NIH Stroke Scale (NIHSS)">
					description = <"15-item neurologic examination stroke scale that provides a quantitative measure of stroke-related neurological deficit.">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Baseline">
					description = <"Baseline assessment.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"LOC responsiveness">
					description = <"Observed level of consciousness of the patient.">
				>
				["at0005"] = <
					text = <"Alert">
					description = <"Keenly responsive.">
				>
				["at0006"] = <
					text = <"Not alert, but arousable by minor stimulation">
					description = <"Not alert; but arousable by minor stimulation to obey, answer or respond.">
				>
				["at0007"] = <
					text = <"Not alert, requires repeated stimulation or strong stimuli.">
					description = <"Not alert; requires repeated stimulation to attend, or is obtunded and requires strong or painful stimulation to make movements.">
				>
				["at0008"] = <
					text = <"Reflex reponses only or totally unreponsive">
					description = <"Responds only with reflex motor or autonomic effects, or totally unresponsive, flaccid, and areflexic.">
				>
				["at0009"] = <
					text = <"LOC questions">
					description = <"The patient is asked the month and his/her age.">
				>
				["at0010"] = <
					text = <"Both correct">
					description = <"Patient answered both questions correctly.">
				>
				["at0011"] = <
					text = <"One correct">
					description = <"Patient answered one question correctly or is unable to communicate because of a problem not secondary to aphasia.">
				>
				["at0012"] = <
					text = <"Neither correct">
					description = <"Patient answered neither question correctly or is comatose or does not comprehend the question.">
				>
				["at0013"] = <
					text = <"LOC commands">
					description = <"The patient is asked to open and close the eyes and then to grip and release the non-paretic hand.">
				>
				["at0014"] = <
					text = <"Both correct">
					description = <"Patient performs both tasks correctly.">
				>
				["at0015"] = <
					text = <"One correct">
					description = <"Patient performs one task correctly.">
				>
				["at0016"] = <
					text = <"Neither correct">
					description = <"Patient performs neither task correctly.">
				>
				["at0017"] = <
					text = <"Horizontal eye movements">
					description = <"Horizontal eye movement observation.">
				>
				["at0018"] = <
					text = <"Normal">
					description = <"Normal eye movement.">
				>
				["at0019"] = <
					text = <"Partial gaze palsy">
					description = <"Gaze is abnormal in one or both eyes, but forced deviation or total gaze paresis is not present.">
				>
				["at0020"] = <
					text = <"Forced deviation or total gaze paresis">
					description = <"Forced deviation or total gaze paresis is not overcome by the oculocephalic maneuver.">
				>
				["at0021"] = <
					text = <"Visual fields">
					description = <"Visual field test observation.">
				>
				["at0022"] = <
					text = <"No visual loss">
					description = <"No visual loss detected.">
				>
				["at0023"] = <
					text = <"Partial hemianopia">
					description = <"Partial hemianopia or complete quadrantanopia detected.">
				>
				["at0024"] = <
					text = <"Complete hemianopia">
					description = <"Complete hemianopia detected.">
				>
				["at0025"] = <
					text = <"Bilateral hemianopia">
					description = <"Blindness from any cause, including cortical blindness.">
				>
				["at0026"] = <
					text = <"Facial palsy">
					description = <"Facial palsy observation.">
				>
				["at0027"] = <
					text = <"Normal">
					description = <"Normal symmetrical movements.">
				>
				["at0028"] = <
					text = <"Minor paralysis">
					description = <"Flattened nasolabial fold, asymmetry on smiling.">
				>
				["at0029"] = <
					text = <"Partial paralysis">
					description = <"Total or near-total paralysis of lower face.">
				>
				["at0030"] = <
					text = <"Complete paralysis of one or both sides">
					description = <"Absence of facial movement in the upper and lower face.">
				>
				["at0067"] = <
					text = <"Sensory">
					description = <"Observation of sensory loss.">
				>
				["at0068"] = <
					text = <"Normal">
					description = <"No sensory loss.">
				>
				["at0069"] = <
					text = <"Mild-to-moderate sensory loss">
					description = <"Patient feels pinprick is less sharp or is dull on the affected side; or there is a loss of superficial pain with pinprick, but patient is aware of being touched.">
				>
				["at0070"] = <
					text = <"Severe or total sensory loss">
					description = <"Patient is not aware of being touched in the face, arm, and leg.">
				>
				["at0071"] = <
					text = <"Language">
					description = <"Observation of language capability.">
				>
				["at0072"] = <
					text = <"No aphasia">
					description = <"Normal">
				>
				["at0073"] = <
					text = <"Mild-to-moderate aphasia">
					description = <"Some obvious loss of fluency or facility of comprehension, without significant limitation on ideas expressed or form of expression.">
				>
				["at0074"] = <
					text = <"Severe aphasia">
					description = <"All communication is through fragmentary expression; great need for inference, questioning, and guessing by the listener.">
				>
				["at0075"] = <
					text = <"Mute, global aphasia">
					description = <"No usable speech or auditory comprehension.">
				>
				["at0082"] = <
					text = <"Extinction and inattention">
					description = <"Observation of any evidence of extinction or inattention.">
					comment = <"Also known as 'neglect'.">
				>
				["at0083"] = <
					text = <"No abnormality">
					description = <"No abnormality observed.">
				>
				["at0084"] = <
					text = <"Inattention, or bilateral extinction in one sensory modality">
					description = <"Visual, tactile, auditory, spatial, or personal inattention, or extinction to bilateral simultaneous stimulation in one of the sensory modalities.">
				>
				["at0085"] = <
					text = <"Profound hemi-inattention or extinction to more than one modality">
					description = <"Profound hemi-inattention or extinction to more than one modality; does not recognize own hand or orients to only one side of space.">
				>
				["at0086"] = <
					text = <"Total score">
					description = <"The total score is the arithmetic sum of all 15 responses, excluding any untestable responses.">
				>
				["at0092"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0031"] = <
					text = <"Motor - left arm">
					description = <"Observation of left arm motor function.">
					comment = <"Only in the case of amputation or joint fusion at the shoulder, the examiner should record the score as 'UNTESTABLE' and record the reason in the 'Reason left arm untestable' data element. Implementation note: while the UNTESTABLE value has been included in this ordinal to allow for inclusion in user interface picklists, the NIHSS applies no numerical score from this selection to the 'Total score'.">
				>
				["at0032"] = <
					text = <"No drift">
					description = <"Limb holds 90 (or 45) degrees for full 10 seconds.">
				>
				["at0033"] = <
					text = <"Drift">
					description = <"Limb holds 90 (or 45) degrees, but drifts down before full 10 seconds; does not hit bed or other support.">
				>
				["at0034"] = <
					text = <"Some effort against gravity">
					description = <"Limb cannot get to or maintain (if cued) 90 (or 45) degrees, drifts down to bed, but has some effort against gravity.">
				>
				["at0124"] = <
					text = <"No effort against gravity">
					description = <"Limb falls.">
				>
				["at0035"] = <
					text = <"No movement">
					description = <"There is no observed limb movement.">
				>
				["at0036"] = <
					text = <"UNTESTABLE">
					description = <"Amputation or joint fusion at the shoulder.">
				>
				["at0038"] = <
					text = <"Motor - right arm">
					description = <"Observation of right arm motor function.">
					comment = <"Only in the case of amputation or joint fusion at the shoulder, the examiner should record the score as 'UNTESTABLE' and record the reason in the 'Reason right arm untestable' data element. Implementation note: while the UNTESTABLE value has been included in this ordinal to allow for inclusion in user interface picklists, the NIHSS applies no numerical score from this selection to the 'Total score'.">
				>
				["at0039"] = <
					text = <"No drift">
					description = <"Limb holds 90 (or 45) degrees for full 10 seconds.">
				>
				["at0040"] = <
					text = <"Drift">
					description = <"Limb holds 90 (or 45) degrees, but drifts down before full 10 seconds; does not hit bed or other support.">
				>
				["at0041"] = <
					text = <"Some effort against gravity">
					description = <"Limb cannot get to or maintain (if cued) 90 (or 45) degrees, drifts down to bed, but has some effort against gravity.">
				>
				["at0042"] = <
					text = <"No effort against gravity">
					description = <"Limb falls.">
				>
				["at0043"] = <
					text = <"No movement">
					description = <"There is no observed limb movement.">
				>
				["at0044"] = <
					text = <"UNTESTABLE">
					description = <"Amputation or joint fusion at the shoulder.">
				>
				["at0046"] = <
					text = <"Motor - left leg">
					description = <"Observation of left leg motor function.">
					comment = <"Only in the case of amputation or joint fusion at the hip, the examiner should record the score as 'UNTESTABLE' and record the reason in the 'Reason left leg untestable' data element. Implementation note: while the UNTESTABLE value has been included in this ordinal to allow for inclusion in user interface picklists, the NIHSS applies no numerical score from this selection to the 'Total score'.">
				>
				["at0047"] = <
					text = <"No drift">
					description = <"Leg holds 30-degree position for full 5 seconds.">
				>
				["at0048"] = <
					text = <"Drift">
					description = <"Leg falls by the end of the 5-second period but does not hit the bed.">
				>
				["at0049"] = <
					text = <"Some effort against gravity">
					description = <"Leg falls to bed by 5 seconds but has some effort against gravity.">
				>
				["at0050"] = <
					text = <"No effort against gravity">
					description = <"Leg falls to bed immediately.">
				>
				["at0051"] = <
					text = <"No movement">
					description = <"There is no observed leg movement.">
				>
				["at0052"] = <
					text = <"UNTESTABLE">
					description = <"Amputation or joint fusion at the hip.">
				>
				["at0141"] = <
					text = <"Motor - right leg">
					description = <"Observation of right leg motor function.">
					comment = <"Only in the case of amputation or joint fusion at the hip, the examiner should record the score as 'UNTESTABLE' and record the reason in the 'Reason right leg untestable' data element Implementation note: while the UNTESTABLE value has been included in this ordinal to allow for inclusion in user interface picklists, the NIHSS applies no numerical score from this selection to the 'Total score'.">
				>
				["at0054"] = <
					text = <"No drift">
					description = <"Leg holds 30-degree position for full 5 seconds.">
				>
				["at0055"] = <
					text = <"Drift">
					description = <"Leg falls by the end of the 5-second period but does not hit the bed.">
				>
				["at0056"] = <
					text = <"Some effort against gravity">
