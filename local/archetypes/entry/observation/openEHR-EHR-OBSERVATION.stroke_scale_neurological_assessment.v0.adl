					text = <"No abnormality">
					description = <"*">
				>
				["at0090"] = <
					text = <"Visual, tactile, auditory, spatial, or personal inattention">
					description = <"Or extinction to bilateral simultaneous stimulation in one of the sensory modalities.">
				>
				["at0091"] = <
					text = <"Profound hemi-inattention or extinction to more than one modality">
					description = <"Does not recognize own hand or orients to only one side of space.">
				>
