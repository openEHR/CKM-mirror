definition
	OBSERVATION[at0000] matches {    -- Social context screening questionnaire
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {0..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Screening purpose
										value matches {
											DV_TEXT matches {*}
										}
									}
									CLUSTER[at0022] occurrences matches {0..*} matches {    -- Specific social context
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0004] matches {    -- Context
												value matches {
													DV_TEXT matches {*}
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0048,    -- Homeless
															at0049]    -- Interpreter needed
														}
													}
												}
											}
											ELEMENT[at0005] occurrences matches {0..1} matches {    -- Presence?
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0023,    -- Yes
															at0024,    -- No
															at0027,    -- Unknown
															at0050]    -- Unsure
														}
													}
													DV_TEXT matches {*}
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0051] occurrences matches {0..*} matches {    -- Timing
												value matches {
													DV_TEXT matches {*}
													DV_DATE_TIME matches {*}
													DV_DURATION matches {*}
													DV_INTERVAL<DV_DATE_TIME> matches {*}
