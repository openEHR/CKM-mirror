definition
	OBSERVATION[at0000] matches {    -- Measure of activity performance of the hand (MAP-Hand)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Point in time event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- 1. Buttoning buttons
										value matches {
											1|[local::at0103],
											2|[local::at0104],
											3|[local::at0105],
											4|[local::at0106]
										}
									}
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- 2. Putting on socks or tights
										value matches {
											1|[local::at0107],
											2|[local::at0108],
											3|[local::at0109],
											4|[local::at0110]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- 3. Tying shoelaces
										value matches {
											1|[local::at0111],
											2|[local::at0112],
											3|[local::at0113],
											4|[local::at0114]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- 4. Squeezing out of tubes (e.g. toothpaste)
										value matches {
											1|[local::at0115],
											2|[local::at0116],
											3|[local::at0117],
											4|[local::at0118]
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- 5. Brushing teeth
										value matches {
											1|[local::at0119],
											2|[local::at0120],
											3|[local::at0121],
											4|[local::at0122]
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {    -- 6. Wiping yourself after using the toilet
										value matches {
											1|[local::at0123],
											2|[local::at0124],
											3|[local::at0125],
											4|[local::at0126]
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {    -- 7. Opening bottle screw tops
										value matches {
											1|[local::at0127],
											2|[local::at0128],
											3|[local::at0129],
											4|[local::at0130]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- 8. Opening hermetic cans
										value matches {
											1|[local::at0131],
											2|[local::at0132],
											3|[local::at0133],
											4|[local::at0134]
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {    -- 9. Opening jam jars
										value matches {
											1|[local::at0135],
											2|[local::at0136],
											3|[local::at0137],
											4|[local::at0138]
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {    -- 10. Slicing bread using a knife
										value matches {
											1|[local::at0139],
											2|[local::at0140],
											3|[local::at0141],
											4|[local::at0142]
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- 11. Peeling raw vegetables
										value matches {
											1|[local::at0143],
											2|[local::at0144],
											3|[local::at0145],
											4|[local::at0146]
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {    -- 12. Stirring food in a pan
										value matches {
											1|[local::at0147],
											2|[local::at0148],
											3|[local::at0149],
											4|[local::at0150]
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- 13. Wringing out cloths
										value matches {
											1|[local::at0151],
											2|[local::at0152],
											3|[local::at0153],
											4|[local::at0154]
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- 14. Carrying shopping bags
										value matches {
											1|[local::at0155],
											2|[local::at0156],
											3|[local::at0157],
											4|[local::at0158]
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- 15. Writing by hand
										value matches {
											1|[local::at0159],
											2|[local::at0160],
											3|[local::at0161],
											4|[local::at0162]
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {    -- 16. Typing on a computer
										value matches {
											1|[local::at0163],
											2|[local::at0164],
											3|[local::at0165],
											4|[local::at0166]
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {    -- 17. Pushing with hands when getting up from a chair
										value matches {
											1|[local::at0167],
											2|[local::at0168],
											3|[local::at0169],
											4|[local::at0170]
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {    -- 18. Carrying heavy objects like suitcases and bags (over 5 kg/10lbs)
										value matches {
											1|[local::at0171],
											2|[local::at0172],
											3|[local::at0173],
											4|[local::at0174]
										}
									}
									ELEMENT[at0179] occurrences matches {0..1} matches {    -- Mean score of predefined activities
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
														magnitude = <|1.0..4.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									CLUSTER[at0097] occurrences matches {0..5} matches {    -- Self-defined activities
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0094] occurrences matches {0..1} matches {    -- Activity
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0096] occurrences matches {0..1} matches {    -- Grading
												value matches {
													1|[local::at0175],
													2|[local::at0176],
													3|[local::at0177],
													4|[local::at0178]
												}
											}
										}
									}
									ELEMENT[at0180] occurrences matches {0..1} matches {    -- Mean score of self-defined activites
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
														magnitude = <|1.0..4.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0095] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0102] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


