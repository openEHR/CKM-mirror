					comment = <"Bijvoorbeeld: lokale informatie-eisen of aanvullende metadata om af te stemmen op FHIR- of CIMI-equivalenten.">
				>
				["at0116"] = <
					text = <"Vereisten voor patiënt">
					description = <"Taal, vervoer of andere persoonlijke vereisten ter ondersteuning van de aanwezigheid of deelname van de patiënt aan de dienstverlening.">
				>
				["at0121"] = <
					text = <"Servicenaam">
					description = <"De naam van de gevraagde service of activiteit.">
					comment = <"Codering van de 'Servicenaam' met een coderingssysteem is wenselijk, indien beschikbaar. Bijvoorbeeld: 'verwijzing' naar een endocrinoloog voor diabetesmanagement.">
				>
				["at0127"] = <
					text = <"Status van verzoek">
					description = <"De status van het serviceverzoek zoals aangegeven door de aanvrager.">
					comment = <"Status wordt gebruikt om aan te geven of dit het eerste verzoek is, of een vervolgverzoek tot wijziging of het verstrekken van aanvullende informatie. Coderen met een terminologie heeft, waar mogelijk, de voorkeur.">
