definition
	EVALUATION[at0000] matches {    -- Advance intervention decisions
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Intent of care
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Curative
									at0004,    -- Restorative
									at0005,    -- Supportive
									at0006,    -- Terminal
									at0007]    -- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0056] occurrences matches {0..1} matches {    -- Rationale
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Decisions description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- CPR decision
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0010,    -- Full CPR
									at0011,    -- Limited CPR
									at0012,    -- No CPR
									at0013]    -- Unknown
								}
							}
						}
					}
					CLUSTER[at0014] occurrences matches {0..*} matches {    -- Per intervention
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] matches {    -- Intervention
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0016,    -- Chest compressions
											at0017,    -- Defibrillation
											at0018,    -- Intubation
											at0019,    -- Invasive ventilation
											at0020,    -- Non-invasive ventilation
											at0021,    -- Peripheral intravenous line
											at0022,    -- Central venous line or any arterial line
											at0023,    -- Parenteral fluids
											at0024,    -- Parenteral or artificial nutrition
											at0025,    -- Arrest medications
											at0026,    -- Circulatory regulation medications
											at0027,    -- Antibiotics, antiviral or antifungal agents
											at0028,    -- Blood products
											at0029,    -- Chest tube insertion
											at0030,    -- Cardiac pacemaker
											at0031,    -- Dialysis
											at0032,    -- Transfer to hospital
											at0033]    -- Transfer to Intensive Care
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0034] occurrences matches {0..1} matches {    -- Decision
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0035,    -- Recommended
											at0036,    -- Conditional recommendation
											at0037,    -- Not recommended
											at0038]    -- Unknown
										}
									}
								}
							}
							ELEMENT[at0039] occurrences matches {0..*} matches {    -- Precondition
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0040] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {    -- Patient awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {    -- Carer awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {    -- Overall comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0045] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0046] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Valid period start
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0048] occurrences matches {0..1} matches {    -- Valid period end
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Review due
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0050] occurrences matches {0..1} matches {    -- Mandate
						value matches {
							DV_TEXT matches {*}
							DV_EHR_URI matches {*}
						}
					}
					allow_archetype CLUSTER[at0051] occurrences matches {0..*} matches {    -- Digital representation
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia_source\.v0/}
					}
					CLUSTER[at0052] occurrences matches {0..1} matches {    -- Document location
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0053] occurrences matches {0..1} matches {    -- Location
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							allow_archetype CLUSTER[at0054] occurrences matches {0..*} matches {    -- Copy holder
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
					allow_archetype CLUSTER[at0055] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Advance intervention decisions">
					description = <"Anticipatory decisions about the overall intent of care and possible interventions (including treatments, activities, and diagnostic or therapeutic procedures), asserted by a clinician.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Intent of care">
					description = <"The overall intent for future care and treatment for the individual.">
					comment = <"The proposed value set is based on the MJA reference. 
Coding the intent of care with an external terminology is preferred, where possible.">
				>
				["at0003"] = <
					text = <"Curative">
					description = <"The condition is curable; all appropriate life-prolonging treatment will be offered.">
				>
				["at0004"] = <
					text = <"Restorative">
					description = <"The condition is treatable with potential for restoration to the previous state of health or a prolonged remission; all appropriate life-prolonging treatment will be offered.">
				>
				["at0005"] = <
					text = <"Supportive">
					description = <"The condition is incurable and progressive; active treatment of the underlying disease may be appropriate for specific symptom relief and/or short term life expectancy gains; focused on comfort, quality of life and dignity. Also referred to as 'Palliative'.">
				>
				["at0006"] = <
					text = <"Terminal">
					description = <"Life expectancy is short (hours or days); no active treatment of the underlying disease or treatment-related harm; focus on comfort, quality of life and dignity.">
				>
				["at0007"] = <
					text = <"Unknown">
					description = <"The intent of care decision is not known.">
				>
				["at0008"] = <
					text = <"Decisions description">
					description = <"Narrative description about all advance intervention decisions, including the CPR decision.">
					comment = <"May be used to record a narrative overview of the advance intervention decision that reflect the specific circumstances for the individual. This includes any limitations, or conditional circumstances applicable, to interventions. This description may or may not be supported by structured data, recorded using the ‘Per intervention’ cluster. If both this narrative description and ‘Per intervention’ structured data is recorded, caution is required as there is a potential risk of divergence. This data element may be used to capture legacy data that is not available in a structured format.">
				>
				["at0009"] = <
					text = <"CPR decision">
					description = <"Decision about the extent of cardiopulmonary resuscitation (CPR) intervention appropriate for this individual.">
				>
				["at0010"] = <
					text = <"Full CPR">
					description = <"All appropriate cardiopulmonary resuscitation treatments should be attempted to save or prolong life.">
				>
				["at0011"] = <
					text = <"Limited CPR">
					description = <"Cardiopulmonary resuscitation treatments should be attempted, as specified in the 'Decisions description' narrative or as structured data in the 'Per intervention' cluster.">
				>
				["at0012"] = <
					text = <"No CPR">
					description = <"No cardiopulmonary resuscitation treatments should be attempted to save or prolong life. Also known as 'Do not resuscitate (DNR)'; 'Do not attempt resuscitation (DNAR)'; 'Not for resuscitation (NFR)' or similar.">
				>
				["at0013"] = <
					text = <"Unknown">
					description = <"The CPR decision is not known.">
				>
				["at0014"] = <
					text = <"Per intervention">
					description = <"Details of the decision about possible treatments or activities that may be life-saving, life-prolonging or cause undesirable side effects.">
				>
				["at0015"] = <
					text = <"Intervention">
					description = <"Name of the possible treatment, procedure or activity.">
					comment = <"While this archetype is intended for all clinical decisions about possible future interventions, the proposed DV_CODED_TEXT value set for the 'Intervention' data element is deliberately not intended to be exhaustive. Instead it has been designed to be inclusive of most of the common life-saving treatments within an emergency context. The alternate choice of the DV_TEXT option can be used to add a local value set or bind to an alternative external value set within a template.">
				>
				["at0016"] = <
					text = <"Chest compressions">
					description = <"Technique used to manually pump blood around the body during a cardiac arrest.">
				>
				["at0017"] = <
					text = <"Defibrillation">
					description = <"Use of electric shock to reset the electrical state of the heart.">
				>
				["at0018"] = <
					text = <"Intubation">
					description = <"Placement of a tube into the trachea to maintain an open airway, including a tracheostomy.">
				>
				["at0019"] = <
					text = <"Invasive ventilation">
					description = <"Artificial ventilation where invasive means are used to assist or replace spontaneous breathing.">
				>
				["at0020"] = <
					text = <"Non-invasive ventilation">
					description = <"Artificial ventilation where non-invasive means are used to assist or replace spontaneous breathing.">
				>
				["at0021"] = <
					text = <"Peripheral intravenous line">
					description = <"Insertion of a peripheral intravenous line.">
				>
				["at0022"] = <
					text = <"Central venous line or any arterial line">
					description = <"Insertion of a central venous line or any arterial line.">
				>
				["at0023"] = <
					text = <"Parenteral fluids">
					description = <"Maintenance of hydration by means other than eating or drinking.">
				>
				["at0024"] = <
					text = <"Parenteral or artificial nutrition">
					description = <"Provision of nutrients by means other than eating or drinking.">
				>
				["at0025"] = <
					text = <"Arrest medications">
					description = <"Medications used during an emergency resuscitation to prolong life. For example: adrenaline, atropine, sodium bicarbonate, calcium, other vasoactive medications.">
				>
				["at0026"] = <
					text = <"Circulatory regulation medications">
					description = <"Medication that supports heart function, including but not limited to: vasopressors, inotropes and chronotropes.">
				>
				["at0027"] = <
					text = <"Antibiotics, antiviral or antifungal agents">
					description = <"Medications to treat infections.">
				>
				["at0028"] = <
					text = <"Blood products">
					description = <"Therapeutic substance prepared from human blood including, but not limited to: whole blood; blood components; and plasma derivatives.">
				>
				["at0029"] = <
					text = <"Chest tube insertion">
					description = <"Insertion of a tube into the pleural space.">
				>
				["at0030"] = <
					text = <"Cardiac pacemaker">
					description = <"Insertion of an artificial pacemaker to control the heart beat.">
				>
				["at0031"] = <
					text = <"Dialysis">
					description = <"Process of removing excess water, solutes, and toxins from the blood.">
				>
				["at0032"] = <
					text = <"Transfer to hospital">
					description = <"Transfer to hospital for assessment or admission.">
				>
				["at0033"] = <
					text = <"Transfer to Intensive Care">
					description = <"Transfer and admission to an intensive care or high dependency unit.">
				>
				["at0034"] = <
					text = <"Decision">
					description = <"Decision about the identified intervention.">
				>
				["at0035"] = <
					text = <"Recommended">
					description = <"The intervention is recommended.">
				>
				["at0036"] = <
					text = <"Conditional recommendation">
					description = <"The intervention is recommended only in specific circumstances, captured using the 'Preconditions' data element.">
				>
				["at0037"] = <
					text = <"Not recommended">
					description = <"The intervention is not recommended.">
				>
				["at0038"] = <
					text = <"Unknown">
					description = <"The intervention decision is not known.">
				>
				["at0039"] = <
					text = <"Precondition">
					description = <"Description of the circumstance/s in which a 'Conditional recommendation' is applicable.">
					comment = <"This data element is only relevant if the 'Conditional recommendation' option is selected in the 'Decision' data element. For example:' \"Transfer to hospital is only applicable in the circumstances where the individual suffers a fracture and requires immobilization for pain relief.#">
				>
				["at0040"] = <
					text = <"Comment">
					description = <"Additional narrative about the intervention decision, not captured in other fields.">
					comment = <"For example: the justification or rationale for the intervention decision.">
				>
				["at0041"] = <
					text = <"Additional details">
					description = <"Additional structured details about advance intervention decisions.">
					comment = <"For example: people involved in the decision-making process, if it is not possible to use the reference model participations; or additional locally relevant considerations.">
				>
				["at0042"] = <
					text = <"Patient awareness">
					description = <"Narrative description about awareness of the Advance intervention decisions by the individual.">
					comment = <"For example: the capacity of the patient to understand the decisions.">
				>
				["at0043"] = <
					text = <"Carer awareness">
					description = <"Narrative description about awareness of the Advance intervention decisions by family, carers or legal proxy.">
					comment = <"For example: the individual has no family.">
				>
				["at0044"] = <
					text = <"Overall comment">
					description = <"Additional narrative about all advance intervention decisions, not captured in other fields.">
				>
				["at0045"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0046"] = <
					text = <"Last updated">
					description = <"The date and/or time when the Advance intervention decisions were last updated.">
				>
				["at0047"] = <
					text = <"Valid period start">
					description = <"The date/time that marks the beginning of the valid period of time for the Advance intervention decisions.">
				>
				["at0048"] = <
					text = <"Valid period end">
					description = <"The date/time that marks the conclusion of the valid period of time for the Advance intervention decisions.">
					comment = <"'Valid period end' may often overlap with 'Review due date'. However, they may need to be recorded separately in circumstances where a document has an extended period of validity but requires an interim review. That may be due to changed personal circumstances/events or local policy.">
				>
				["at0049"] = <
					text = <"Review due">
					description = <"The date when review of the Advance intervention decisions are due.">
				>
				["at0050"] = <
					text = <"Mandate">
					description = <"Description of any legislation or other authoritative guidance that apply.">
					comment = <"For example: an advance care directive.">
				>
				["at0051"] = <
					text = <"Digital representation">
					description = <"Digital document, image or video representing these Advance intervention decisions.">
				>
				["at0052"] = <
					text = <"Document location">
					description = <"Information about the physical or digital location of the advance intervention decisions record.">
				>
				["at0053"] = <
					text = <"Location">
					description = <"Physical or digital location of the Advance intervention decisions record.">
				>
				["at0054"] = <
					text = <"Copy holder">
					description = <"Details of a person who has a copy of the Advance intervention decisions record.">
					comment = <"Comment: For example, 'John Smith, Lawyer'.">
				>
				["at0055"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0056"] = <
					text = <"Rationale">
					description = <"Narrative description about the logic and justification for the 'Intent of care' decision.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Behandlingsbegrensning">
					description = <"Forhåndsbeslutning om den overordnede intensjonen med omsorg/behandling og mulige intervensjoner (inkludert behandling, tiltak og diagnostiske eller terapeutiske prosedyrer), bestemt av en eller flere klinikere.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Intensjon med behandling">
					description = <"Den overordnede intensjonen med fremtidig behandling og omsorg for individet.">
					comment = <"Datatypen tillater enten fritekst, lokal eller ekstern terminologi eller det foreslåtte verdisettet. Sistnevnte er basert på artikkelen i MJA som er referert til i \"Header\" i denne arketypen. Når og om det blir en norsk omforent slik terminologi, er det anbefalt å bruke dette i norske implementasjoner.">
				>
				["at0003"] = <
					text = <"Kurativ">
					description = <"Tilstanden er mulig å kurere. All relevant livsforlengende behandling skal tilbys.">
				>
				["at0004"] = <
					text = <"Gjenopprettende">
					description = <"Tilstanden er mulig å kurere med potensiale for gjenoppretting til tidligere helsetilstand eller forlenget remisjon/bedring. All relevant livsforlengende behandling skal tilbys.">
				>
				["at0005"] = <
					text = <"Livsforlengende">
					description = <"Tilstanden er ikke mulig å kurere og er progressiv. Aktiv behandling av underliggende sykdom kan være hensiktsmessig for symptomlindring og/eller for å oppnå kortvarig livsforlengende mål. Fokus på lindring, livskvalitet og verdighet.">
				>
				["at0006"] = <
					text = <"Terminal">
					description = <"Forventet levetid er kort (timer eller dager). Ingen aktiv behandling av underliggende sykdom eller av bivirkninger av behandling. Fokus på lindring, livskvalitet og verdighet.">
				>
				["at0007"] = <
					text = <"Ukjent">
					description = <"Den overordnede intensjonen med behandling er ikke kjent.">
				>
				["at0008"] = <
					text = <"Beskrivelse av behandlingsbegrensning">
					description = <"Fritekstbeskrivelse av beslutninger i \"Behandlingsbegrensning\", inkludert beslutning om HLR.">
					comment = <"Kan bli brukt til en oversikt i fritekst av behandlingsbegrensningen, og som gjenspeiler den spesifikke situasjonen til individet. Dette inkluderer eventuelle begrensninger eller forbehold som gjelder for behandling. Denne beskrivelsen kan eventuelt komme i tillegg til strukturerte data, eller stå for seg selv. Detaljer om spesifikke typer behandling kan registreres strukturert i det interne Clusteret \"Per intervensjon\". Dersom både denne fritekstbeskrivelsen og det interne Clusteret \"Per intervensjon\" blir brukt, må man være oppmerksom på potensielle forskjeller mellom det som blir registrert som fritekst og strukturert. Dette dataelementet kan også bli brukt for å lagre data fra et annet system som ikke er tilgjengelig som strukturerte data.">
				>
				["at0009"] = <
					text = <"HLR-beslutning">
					description = <"Beslutning om grad av Hjerte- og Lungeredning (HLR) som er passende for dette individet.">
				>
				["at0010"] = <
					text = <"Full HLR">
					description = <"Alle relevante tiltak innen hjerte-lunge-redning bør forsøkes for å redde eller forlenge livet.">
				>
				["at0011"] = <
					text = <"Begrenset HLR">
					description = <"Hjerte-lungeredning bør forsøkes, slik det er nærmere spesifisert som fritekst i elementet \"Beskrivelse av behandlingsbegrensning\", eller som strukturerte data i det interne Clusteret \"Per intervensjon\".">
				>
				["at0012"] = <
					text = <"HLR minus">
					description = <"Ingen tiltak innen hjerte-lunge-redning bør utføres for å redde eller forlenge livet. Også kjent som \"R-\", \"Res minus\", \"Do not resuscitate (DNR)\", \"Do not attempt resuscitation (DNAR)\" eller lignende.">
				>
				["at0013"] = <
					text = <"Ukjent">
					description = <"Beslutning om Hjerte-lungeredning (HLR) er ikke kjent.">
				>
				["at0014"] = <
					text = <"Per intervensjon">
					description = <"Detaljer om beslutninger om mulige behandlinger eller tiltak som kan være livreddende, livsforlengende eller føre til uønskede konsekvenser for individet.">
				>
				["at0015"] = <
					text = <"Intervensjon">
					description = <"Navn på mulig intervensjon, prosedyre eller tiltak.">
					comment = <"Verdisettet i det foreslåtte DV_CODED_TEXT for \"Intervensjon\" er ikke ment å være uttømmende, men å ta med seg mange av de vanlige livreddende behandlingene. Som alternativ kan man bruke DV_TEXT for å lage et lokalt verdisett som er passende, eller å binde til et eksternt kodesett i templat.">
				>
				["at0016"] = <
					text = <"Brystkompresjoner">
					description = <"Teknikk for å manuelt pumpe blod rundt i kroppen under hjertestans.">
				>
				["at0017"] = <
					text = <"Defibrillering">
					description = <"Bruk av elektrisk støt for å normalisere hjerterytmen.">
				>
				["at0018"] = <
					text = <"Intubering">
					description = <"Plassering av et fleksibelt plastikkrør i luftrøret for å opprettholde åpne luftveier. Inkluderer tracheostomi.">
				>
				["at0019"] = <
					text = <"Invasiv ventilasjon">
					description = <"Kunstig åndedrett, der invasive tiltak er brukt for å assistere eller erstatte spontant åndedrett.">
				>
				["at0020"] = <
					text = <"Non-invasiv ventilasjon">
					description = <"Kunstig ventilasjon der ikke-invasive tiltak brukes for å assistere eller erstatte spontant åndedrett.">
				>
				["at0021"] = <
					text = <"Perifer venekanyle">
					description = <"Innleggelse av perifer intravenøs veneport.">
				>
				["at0022"] = <
					text = <"Sentralt venekateter eller arteriekran">
					description = <"Innlegging av sentralvenøst kateter eller arteriekran.">
				>
				["at0023"] = <
					text = <"Væskebehandling">
					description = <"Opprettholdelse av væskebalanse via andre metoder enn spising og drikking.">
				>
				["at0024"] = <
					text = <"Parenteral eller kunstig ernæring">
					description = <"Tilførsel av næringsstoffer via andre metoder enn spising og drikking.">
				>
				["at0025"] = <
					text = <"Hjertestansmedisin">
					description = <"Medikamenter som kan bli brukt under recusitering. For eksempel adrenalin, atropin, natriumbikarbonat, kalsium, andre vasoaktive stoffer.">
				>
				["at0026"] = <
					text = <"Sirkulasjonsregulerende medikasjon">
					description = <"Medikamenter som støtter hjertets funksjon, inkludert, men ikke begrenset til, vasopressorer, inotrope- og chronotrope medikamenter.">
				>
				["at0027"] = <
					text = <"Antibiotika, antiviralia og soppdrepende middel.">
					description = <"Medisiner for å behandle infeksjoner.">
				>
				["at0028"] = <
					text = <"Blodprodukter">
					description = <"Terapeutisk substans framstilt av humant blod. Inkluderer, men er ikke begrenset til: fullblod, blodkomponenter og plasmaderivater.">
				>
				["at0029"] = <
					text = <"Thoraxdren">
					description = <"Innleggelse av plastikkrør inn i lungesekken.">
				>
				["at0030"] = <
					text = <"Pacemaker">
					description = <"Innleggelse av pacemaker for å regulere hjerterytmen.">
				>
				["at0031"] = <
					text = <"Dialyse">
					description = <"Prosess som fjerne overskuddsvæske, avfallsstoffer og toksiner fra blodet.">
				>
				["at0032"] = <
					text = <"Overflytting til sykehus">
					description = <"Overflytting til sykehus for vurdering eller innleggelse.">
				>
				["at0033"] = <
					text = <"Overflytting til intensiv">
					description = <"Overflytting og innleggelse på en intensiv- eller overvåkingsenhet.">
				>
				["at0034"] = <
					text = <"Beslutning">
					description = <"Beslutning om den navngitte intervensjonen.">
				>
				["at0035"] = <
					text = <"Anbefalt">
					description = <"Intervensjonen er anbefalt.">
				>
				["at0036"] = <
					text = <"Betinget anbefaling">
					description = <"Intervensjonen er anbefalt bare i bestemte tilfeller og er nærmere beskrevet i elementet \"Forutsetninger\".">
				>
				["at0037"] = <
					text = <"Ikke anbefalt">
					description = <"Intervensjonen er ikke anbefalt.">
				>
				["at0038"] = <
					text = <"Ukjent">
					description = <"Beslutning om intervensjonen er ikke kjent.">
				>
				["at0039"] = <
					text = <"Forutsetninger">
					description = <"Fritekstlig beskrivelse av i hvilke omstendigheter en \"Betinget anbefaling\" er aktuell.">
					comment = <"Dette dataelemente er bare relevant dersom \"Betinget anbefaling\" er valgt i dataelementet \"Beslutning\". For eksempel om \"Overføring til sykehus\" er angitt med \"Betinget anbefaling\", kan omstendighetene være \"Kun ved eventuelt brudd og trenger immobilisering for smertelindring\".">
				>
				["at0040"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om beslutning om intervensjon, som ikke er fanget opp i andre felt.">
					comment = <"For eksempel rasjonale eller berettigelse for beslutning som er tatt om intervensjon.">
				>
				["at0041"] = <
					text = <"Ytterligere detaljer">
					description = <"Ytterligere strukturerte detaljer om behandlingsbegrensning.">
					comment = <"Dersom det ikke er nok å bruke elementet \"Participations\" i referansemodellen (RM), kan dette elementet brukes for å dokumentere hvem som var involvert i beslutningene og når. Det kan også brukes for andre lokale dokumentasjonsbehov, siden elementet er repeterbart.">
				>
				["at0042"] = <
					text = <"Pasient informert">
					description = <"Beskrivelse om individet er klar over beslutningene om behandlingbegrensning.">
					comment = <"Kan også inkludere pasientens forutsetninger for å forstå beslutningene.">
				>
				["at0043"] = <
					text = <"Pårørende informert">
					description = <"Beskrivelse om individets pårørende, omsorgsgivere eller verge er klar over beslutningene om behandlingbegrensning.">
					comment = <"Kan også inkludere om pasienten ikke har familie.">
				>
				["at0044"] = <
					text = <"Overordnet kommentar">
					description = <"Ytterligere fritekstlig beskrivelse om avgjørelser om begrensning av behandling, og som ikke er fanget opp i andre felter.">
				>
				["at0045"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0046"] = <
					text = <"Sist oppdatert">
					description = <"Dato og/eller tid da oppføringer i behandlingsbegrensningen sist ble oppdatert.">
				>
				["at0047"] = <
					text = <"Gyldighetsperiode start">
					description = <"Dato/Tid som markerer når gyldighetsperioden av behandlingsbegrensningen starter.">
				>
				["at0048"] = <
					text = <"Gyldighetsperiode slutt">
					description = <"Dato/Tid som markerer når gyldighetsperioden av behandlingsbegrensningen opphører.">
					comment = <"\"Gyldighetsperiode slutt\" vil ofte overlappe med \"Dato for fornyelse\". Det kan imidlertid være nødvendig å registrere disse separat i tilfelle en behandlingsbegrensning har en forlenget gyldighetsperiode, men trenger en midlertidig gjennomgang. Dette kan skyldes endringer i personlige forhold/hendelser eller lokale retningslinjer.">
				>
				["at0049"] = <
					text = <"Dato for fornyelse">
					description = <"Dato når behandlingsbegrensningen må fornyes.">
				>
				["at0050"] = <
					text = <"Grunnlag">
					description = <"Beskrivelse av hvilken lovgivning eller annen autorativ retningslinje som gjelder for det behandlingsbegrensningen.">
					comment = <"For eksempel \"Fremtidig behandlingsvalg\" eller et livstestamente.">
				>
				["at0051"] = <
					text = <"Digital representasjon">
					description = <"Digitalt dokument, bilde eller video som representerer det behandlingsbegrensningen.">
				>
				["at0052"] = <
					text = <"Dokumentets plassering">
					description = <"Informasjon om hvor behandlingsbegrensningen er lagret (dersom det er et separat dokument eller i et annet system), samt hvem som har en kopi av det.">
				>
				["at0053"] = <
					text = <"Plassering">
					description = <"Den fysiske eller digitale lokaliseringen av behandlingsbegrensningen.">
				>
				["at0054"] = <
					text = <"Kopiholder">
					description = <"Detaljer om person som har en kopi av behandlingsbegrensningen.">
					comment = <"For eksempel \"Advokat John Smith\".">
				>
				["at0055"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0056"] = <
					text = <"Begrunnelse">
					description = <"Fritekstlig begrunnelse om rasjonale og berettigelse for utsagnet om \"Intensjon med behandling\".">
				>
			>
		>
		["de"] = <
			items = <
				["at0000"] = <
					text = <"Frühzeitige Interventionsentscheidungen">
					description = <"Vorausschauende Entscheidungen über die allgemeine Absicht der Versorgung und mögliche Interventionen (einschließlich Behandlungen, Aktivitäten und diagnostische oder therapeutische Verfahren), die von einem Arzt getroffen werden.
">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Intention der Behandlung">
					description = <"Die allgemeine Absicht für die zukünftige Pflege und Behandlung der Person.">
					comment = <"Der vorgeschlagene Wertesatz basiert auf der MJA-Referenz. 
Die Kodierung des Versorgungszwecks mit einer externen Terminologie wird bevorzugt, wenn möglich.">
				>
				["at0003"] = <
					text = <"Kurativ">
					description = <"Die Erkrankung ist heilbar; es werden alle geeigneten lebensverlängernden Maßnahmen angeboten.">
				>
				["at0004"] = <
					text = <"Restaurativ">
					description = <"Die Erkrankung ist behandelbar mit der Möglichkeit der Wiederherstellung des vorherigen Gesundheitszustandes oder einer anhaltenden Remission; alle geeigneten lebensverlängernden Behandlungen werden angeboten.
">
				>
				["at0005"] = <
					text = <"Supportiv">
					description = <"Die Erkrankung ist unheilbar und fortschreitend; eine aktive Behandlung der zugrundeliegenden Krankheit kann für eine spezifische Symptomlinderung und/oder eine kurzfristige Verlängerung der Lebenserwartung angemessen sein; der Schwerpunkt liegt auf Komfort, Lebensqualität und Würde. Wird auch als \"Palliativ\" bezeichnet.
">
				>
				["at0006"] = <
					text = <"Terminal">
					description = <"Die Lebenserwartung ist kurz (Stunden oder Tage); keine aktive Behandlung der Grunderkrankung oder behandlungsbedingte Schäden; Fokus auf Komfort, Lebensqualität und Würde.">
				>
				["at0007"] = <
					text = <"Unbekannt">
					description = <"Die Intention der Behandlung ist unbekannt.">
				>
				["at0008"] = <
					text = <"Beschreibung der Entscheidungen">
					description = <"Narrative Beschreibung über alle vorausgehenden Interventionsentscheidungen, einschließlich der Entscheidung zur HLW.">
					comment = <"Kann verwendet werden, um einen narrativen Überblick über die Entscheidung über einen vorausgehenden Eingriff aufzuzeichnen, der die spezifischen Umstände für die Person widerspiegelt. Dies umfasst alle Einschränkungen oder bedingten Umstände, die für Interventionen gelten. Diese Beschreibung kann, muss aber nicht durch strukturierte Daten unterstützt werden, die mit dem Cluster \"Pro Eingriff\" erfasst werden. Wenn sowohl diese narrative Beschreibung als auch die strukturierten Daten \"Pro Intervention\" aufgezeichnet werden, ist Vorsicht geboten, da ein potenzielles Risiko der Divergenz besteht. Dieses Datenelement kann zur Erfassung von Altdaten verwendet werden, die nicht in einem strukturierten Format verfügbar sind.
">
				>
				["at0009"] = <
					text = <"CPR Entscheidung">
					description = <"Entscheidung über das Ausmaß der kardiopulmonalen Reanimation (CPR), die für diese Person angemessen ist.
">
				>
				["at0010"] = <
					text = <"Vollständige CRP">
					description = <"Es sollten alle geeigneten kardiopulmonalen Reanimationen versucht werden, um Leben zu retten oder zu verlängern.
">
				>
				["at0011"] = <
					text = <"Begrenzte CPR">
					description = <"Es sollten kardiopulmonale Reanimationen versucht werden, wie in \"Beschreibung der Entscheidungen\" oder als strukturierte Daten im Cluster \"Pro Intervention\" angegeben.">
				>
				["at0012"] = <
					text = <"Keine CPR">
					description = <"Es sollten keine kardiopulmonalen Reanimation versucht werden, um Leben zu retten oder zu verlängern. Alle Behandlungen sollen sich auf die Linderung von Symptomen und die Lebensqualität konzentrieren. Auch bekannt als 'Do not resuscitate (DNR)'; 'Do not attempt resuscitation (DNAR)'; 'Not for resuscitation (NFR)' oder ähnlich.">
				>
				["at0013"] = <
					text = <"Unbekannt">
					description = <"Die Entscheidung der CPR ist nicht bekannt.">
				>
				["at0014"] = <
					text = <"Pro Intervention">
					description = <"Details der Entscheidung über mögliche Behandlungen oder Aktivitäten, die lebensrettend oder lebensverlängernd sein können oder unerwünschte Nebenwirkungen verursachen.
">
				>
				["at0015"] = <
					text = <"Intervention">
					description = <"Bezeichnung der möglichen Behandlung, des Verfahrens oder der Tätigkeit.
">
					comment = <"Während dieser Archetyp für alle klinischen Entscheidungen über mögliche zukünftige Interventionen gedacht ist, soll der vorgeschlagene DV_CODED_TEXT-Wertesatz für das Datenelement \"Intervention\" bewusst nicht vollständig sein. Stattdessen wurde er so konzipiert, dass er die meisten der üblichen lebensrettenden Behandlungen in einem Notfallkontext umfasst. Die alternative Auswahl der DV_TEXT-Option kann verwendet werden, um einen lokalen Wertesatz hinzuzufügen oder an einen alternativen externen Wertesatz innerhalb einer Vorlage zu binden.
">
				>
				["at0016"] = <
					text = <"Herzdruckmassage">
					description = <"Technik, mit der das Blut während eines Herzstillstands manuell durch den Körper gepumpt wird.">
				>
				["at0017"] = <
					text = <"Defibrillation">
					description = <"Verwendung eines Elektroschocks, um den elektrischen Zustand des Herzens zurückzusetzen.
">
