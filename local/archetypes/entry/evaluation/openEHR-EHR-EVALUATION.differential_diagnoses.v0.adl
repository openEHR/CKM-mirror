	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"For å registrere en liste over en eller flere mistenkte eller sannsynlige tilstander eller diagnoser som kan være ansvarlig for den kliniske presentasjonen, undersøkelsesfunnene og testresultatene inntil en enkelt, arbeidsdiagnose er bestemt.">
			use = <"Brukes for å registrere en liste over en eller flere mistenkte eller sannsynlige tilstander eller diagnoser som kan være ansvarlig for den kliniske presentasjonen, undersøkelsesfunnene og testresultatene inntil en enkelt, arbeidsdiagnose er bestemt.

I praksis er intensjonen at når ytterligere evidens er samlet, vil forskjellige diagnoser bli eliminert én etter én til en diagnose blir mest sannsynlig.">
			keywords = <"differensial", "diagnose">
			misuse = <"Brukes ikke til å registrere detaljert informasjon om et kjent problem eller formell diagnose - bruk EVALUATION.problem_diagnosis til dette formålet.">
		>
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record a list of one or more suspected or likely conditions or diagnoses that may be responsible for the clinical presentation, examination findings and test results until a single, working diagnosis is determined.">
			use = <"To record a list of one or more suspected or likely conditions or diagnoses that may be responsible for the clinical presentation, examination findings and test results until a single, working diagnosis is determined.

In practice, the intent is that as further evidence is gathered differential diagnoses will be eliminated one by one until one diagnosis becomes most likely.">
			keywords = <"differential", "diagnosis">
			misuse = <"Not to be used to record detailed information about a known problem or formal diagnosis - use the EVALUATION.problem_diagnosis for this purpose.">
			copyright = <"© openEHR Foundation">
		>
	>
