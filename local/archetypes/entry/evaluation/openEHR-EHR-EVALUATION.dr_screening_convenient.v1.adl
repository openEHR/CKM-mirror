definition
	EVALUATION[at0000] matches {	-- Conveniencia del cribado de RD
		data matches {
			ITEM_TREE[at0001] matches {	-- *Arbol(es)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Admisión en el cribado
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008, 	-- Cribado necesario
									at0009, 	-- Cribado no necesario
									at0010]	-- Excluido del cribado
								}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Declaración de inclusión
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011, 	-- Excluído del cribado por ceguera
									at0012, 	-- Excluído del cribado por fallecimiento
									at0013, 	-- Excluído del cribado por discapacidad del aprendizaje
									at0014, 	-- Excluído del cribado por traslado
									at0015, 	-- Excluído del cribado por no disponer de datos de contacto actualizados
									at0016, 	-- Excluído del cribado por remisión de la diabetes
									at0017, 	-- Excluído del cribado por trastorno físico
									at0018, 	-- Excluído del cribado por enfermedad terminal
									at0019, 	-- Excluído del cribado por asignarle bajo el cuidado directo de un oftalmologo
									at0020]	-- Otro motivo
								}
							}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {	-- Comentarios
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0005] matches {	-- *Arbol(es)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Fecha última decisión
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


