definition
	EVALUATION[at0000.1] matches {    -- Biospecimen summary - semen
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..*} matches {    -- Biospecimen ID
