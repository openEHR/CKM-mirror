					ELEMENT[at0003.1] matches {    -- Biospecimen type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.1,    -- Semen
									at0.2]    -- Testicular tissue
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Description
