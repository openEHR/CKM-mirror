definition
	EVALUATION[at0000] matches {    -- Hand dominance
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Handedness
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Right Handed
									at0004,    -- Left Handed
									at0005,    -- Ambidextrous
									at0006]    -- Cross Dominant
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {    -- Assessment method
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0014] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0015] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Hand dominance">
					description = <"Hand dominance is an individual's natural preference for a specific hand for performing various motor tasks.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Handedness">
					description = <"Which hand the individual typically uses for common tasks.">
				>
				["at0003"] = <
					text = <"Right Handed">
					description = <"The individual primarily uses their right hand.">
				>
				["at0004"] = <
					text = <"Left Handed">
					description = <"The individual primarily uses their left hand.">
				>
				["at0005"] = <
					text = <"Ambidextrous">
					description = <"The individual uses both hands equally.">
				>
				["at0006"] = <
					text = <"Cross Dominant">
					description = <"The individual uses different hands depending on the task, such as writing with their right, while using a hammer with their left.">
				>
				["at0013"] = <
					text = <"Comment">
					description = <"Additional narrative about the hand dominance, not captured in other fields.">
				>
				["at0014"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0015"] = <
					text = <"Extension">
					description = <"Additional information required to capture local context or to align with other reference models/formalisms.">
					comment = <"For example: Local hospital departmental infomation or additional metadata to align with FHIR or CIMI equivalents.">
				>
				["at0016"] = <
					text = <"Assessment method">
					description = <"The method used to acertain the individual's handedness.">
					comment = <"For example: Edinburgh Handedness Inventory.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Hånddominans">
					description = <"Hånddominans er et individs naturlige preferanse for å bruke en bestemt hånd for å gjennomføre ulike motoriske handlinger.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Hånddominans">
					description = <"Hvilken hånd individet vanligvis bruker for vanlige oppgaver.">
				>
				["at0003"] = <
					text = <"Høyrehendt">
					description = <"Individet bruker hovedsakelig sin høyre hånd.">
				>
				["at0004"] = <
					text = <"Venstrehendt">
					description = <"Individet bruker hovedsakelig sin venstre hånd.">
				>
				["at0005"] = <
					text = <"Ambidextrøs">
					description = <"Individet bruker bruker begge hender like mye.">
				>
				["at0006"] = <
					text = <"Kryssdominant">
					description = <"Individet bruker ulike hender avhengig av oppgaven, som f.eks. skriver med høyre hånd, og bruker hammer med venstre.">
				>
				["at0013"] = <
					text = <"Kommentar">
					description = <"Tekstlig tilleggsinformasjon om hånddominans som ikke er registrert i andre felt.">
				>
				["at0014"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0015"] = <
					text = <"Utvidelse">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller eller formalismer.">
					comment = <"For eksempel: lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0016"] = <
					text = <"Vurderingsmetode">
					description = <"Hvilken metode som ble brukt for å fastslå individets hånddominans.">
					comment = <"For eksempel: Edinburgh Handedness Inventory">
				>
			>
		>
	>
