					description = <"Digital document, image or video representing the Ceilings of care directive.">
				>
				["at0035"] = <
					text = <"Comment">
					description = <"Additional narrative about the ceilings of care directives, not captured in other fields.">
				>
				["at0036"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0037"] = <
					text = <"Last updated">
					description = <"The date and/or time when this Ceilings of care directive was last updated.">
				>
				["at0038"] = <
					text = <"Valid period start">
					description = <"The date/time that marks the beginning of the valid period of time for this Ceilings of care directive.">
				>
				["at0039"] = <
					text = <"Valid period end">
					description = <"The date/time that marks the conclusion of the valid period of time for this Ceilings of care directive.">
					comment = <"'Valid period end' may often overlap with 'Review due date'. However, they may need to be recorded separately in circumstances where a document has an extended period of validity but requires an interim review. That may be due to changed personal circumstances/events or local policy.">
				>
				["at0040"] = <
					text = <"Review due">
					description = <"The date when review of this Ceilings of care directive is due.">
				>
				["at0041"] = <
					text = <"Mandate">
					description = <"Description of any legislation or other authoritative guidance that apply.">
				>
				["at0042"] = <
					text = <"Directive location">
					description = <"Information regarding where the ceilings of care directive is stored and who has a copy of it.">
				>
				["at0043"] = <
					text = <"Location">
					description = <"Information regarding where the Ceilings of care directive is stored.">
				>
				["at0044"] = <
					text = <"Copy holder">
					description = <"Details of a person who has a copy of the Ceilings of care directive.">
					comment = <"Comment: For example, 'John Smith, Lawyer'.">
				>
				["at0045"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Behandlingbegrensning">
					description = <"Forhåndsbeslutning om fremtidig formålstjenlig behandling, vanligvis bestemt av en autorisert kliniker.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Intensjon med behandling">
					description = <"Beskrivelse av den overordnede intensjonen med fremtidig behandling og omsorg for individet.">
					comment = <"For eksempel en norsk oversettelse av verdisett som foreslått i den refererte artikkelen i Medical Journal of Australia av Thomas et al: \"Curative\", \"Restorative\", \"Supportive\", \"Terminal\"; og \"Unknown\". Om det etableres en omforent slik liste på norsk, kan den tas inn i templat for deretter brukes i et elektronisk journalsystem.">
				>
				["at0003"] = <
					text = <"Begrunnelse">
					description = <"Fritekstlig begrunnelse om rasjonale og berettigelse for utsagnet om \"Intensjon med behandling\".">
				>
				["at0004"] = <
					text = <"HLR-anbefaling">
					description = <"Bestemmelse om fremtidig Hjerte- og Lungeredning (HLR).">
				>
				["at0005"] = <
					text = <"Forsøk HLR">
					description = <"Alle relevante tiltak innen hjerte-lunge-redning bør forsøkes for å redde eller forlenge livet.">
				>
				["at0006"] = <
					text = <"Forsøk begrenset HLR">
					description = <"Noen tiltak innen hjerte-lunge-redning bør forsøkes. Se detaljer i \"Modifisert HLR\".">
				>
				["at0007"] = <
					text = <"HLR minus">
					description = <"Ingen tiltak innen hjerte-lunge-redning bør utføres for å redde eller forlenge livet. All behandling skal være fokusert på symptomlindring og livskvalitet. Også kjent som \"Res minus\", \"Do not resuscitate (DNR)\"; 'Do not attempt resuscitation (DNAR)' eller lignende.">
				>
				["at0008"] = <
					text = <"Ukjent">
					description = <"Ingen kjent beslutning om hjerte- lungeredning (HLR).">
				>
				["at0009"] = <
					text = <"Modifisert HLR">
					description = <"Beskrivelse av hvilke modifikasjoner om HLR som er anbefalt.">
					comment = <"Dette dataelementet er bare relevant dersom \"Forsøk begrenset HLR\" er valgt i elementet \"HLR anbefaling\".">
				>
				["at0010"] = <
					text = <"Per intervensjon">
					description = <"Bestemmelse om en medisinsk intervensjon som kan redde eller forlenge livet.">
				>
				["at0011"] = <
