definition
	EVALUATION[at0000] matches {    -- Ceilings of treatment directive
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Intent of care
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Rationale
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- CPR recommendation
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0005,    -- Attempt CPR
									at0006,    -- Attempt modified CPR
									at0007,    -- Do not attempt CPR
									at0008]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- CPR modification
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0010] occurrences matches {0..*} matches {    -- Per intervention
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0011] matches {    -- Intervention
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0012,    -- Intubation
											at0013,    -- Mechanical ventilation
											at0014,    -- Non-invasive ventilation
											at0015,    -- Circulatory regulation
											at0016,    -- Dialysis
											at0017,    -- Blood products
											at0018,    -- Antibiotics, antiviral or antifungal agents
											at0019,    -- Parenteral or artificial nutrition
											at0020,    -- Transfer to Intensive Care
											at0021,    -- Transfer to hospital
											at0022,    -- Parenteral fluids
											at0023]    -- IV access
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {    -- Decision
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0025,    -- Recommended
											at0026,    -- Recommended, with caveat
											at0027,    -- Not recommended
											at0028]    -- Unknown
										}
									}
								}
							}
							ELEMENT[at0029] occurrences matches {0..1} matches {    -- Caveat
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0030] occurrences matches {0..1} matches {    -- Intervention comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0031] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0032] occurrences matches {0..1} matches {    -- Patient awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Carer awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0034] occurrences matches {0..*} matches {    -- Digital representation
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia_source\.v0/}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0036] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0037] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {    -- Valid period start
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0039] occurrences matches {0..1} matches {    -- Valid period end
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0040] occurrences matches {0..1} matches {    -- Review due
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {    -- Mandate
						value matches {
							DV_TEXT matches {*}
							DV_EHR_URI matches {*}
						}
					}
					CLUSTER[at0042] occurrences matches {0..1} matches {    -- Directive location
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0043] occurrences matches {0..1} matches {    -- Location
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Copy holder
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
					allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {    -- Extension
