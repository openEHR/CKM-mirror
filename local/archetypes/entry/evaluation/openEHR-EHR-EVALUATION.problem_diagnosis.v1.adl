					description = <"Diagnoosi on tunnistettu erittäin varmuudella.">
				>
				["at0076"] = <
					text = <"Varmistettu">
					description = <"Diagnoosi on vahvistettu tunnustettujen kriteerien perusteella.">
				>
				["at0077"] = <
					text = <"Alkamis pvm">
					description = <"Arvioitu tai todellinen päivämäärä / aika, jolloin ongelman / diagnoosin merkkejä tai oireita havaittiin ensimmäisen kerran.">
					comment = <"*Data captured/imported as \"Age at onset\" should be converted to a date using the subject's date of birth.(en)">
				>
