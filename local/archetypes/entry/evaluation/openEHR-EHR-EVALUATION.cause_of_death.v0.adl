definition
	EVALUATION[at0000] matches {    -- Cause of death
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] matches {    -- Direct cause of death
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {    -- Duration
						value matches {
							DV_DURATION matches {
								value matches {|>=PT0S|}
							}
							DV_INTERVAL<DV_DURATION> matches {
								lower matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
								upper matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
							}
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0003] occurrences matches {0..*} matches {    -- Chain of events
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] matches {    -- Indirect cause
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Order
								value matches {
									DV_TEXT matches {*}
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0006,    -- Contributory cause B
											at0007,    -- Contributory cause C
											at0008]    -- Contributory cause D
										}
									}
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {    -- Timing
								value matches {
									DV_TEXT matches {*}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
										upper matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
									}
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0012] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0018] occurrences matches {0..*} matches {    -- Other condition
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0014] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0016] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Cause of death">
					description = <"Details about the specific disease or condition that caused, or contributed to the death of the individual.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0014"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Direct cause of death">
					description = <"Cause leading to death.">
					comment = <"It is strongly recommended that 'Direct cause of death'  be coded with an external terminology. For example: Snomed CT, ICD-10.

This would be cause A in the example:
Cerebral haemorrhage (Direct cause, A) 
- due to Metastasis of the brain (B)
- due to Breast cancer (C)">
				>
				["at0003"] = <
					text = <"Chain of events">
					description = <"Causes and/or events including onset date, order and cause diagnosis that led to the individuals death.">
				>
				["at0004"] = <
					text = <"Indirect cause">
					description = <"Name of the condition or event indirectly contributing to death.">
					comment = <"It is strongly recommended that 'Indirect cause' be coded with an external terminology. For example: Snomed CT, ICD-10.

This would need two instances representing cause B and C in the example:
Cerebral haemorrhage (Direct cause, A) 
- due to Metastasis of the brain (B)
- due to Breast cancer (C)">
				>
				["at0005"] = <
					text = <"Order">
					description = <"Sequence of events leading to death, with the most recent first.">
					comment = <"If an alternative valueset is required, these values can be added to the DV_TEXT data type in a template.

As an example:
Cerebral haemorrhage (Direct cause, A) 
- due to Metastasis of the brain (B)
- due to Breast cancer (C)">
				>
				["at0006"] = <
					text = <"Contributory cause B">
					description = <"Due to or as a consequence of.">
				>
				["at0007"] = <
					text = <"Contributory cause C">
					description = <"Due to or as a consequence of.">
				>
				["at0008"] = <
					text = <"Contributory cause D">
					description = <"Due to or as a consequence of.">
				>
				["at0009"] = <
					text = <"Timing">
					description = <"Approximate time from onset of indirect cause to death.">
				>
				["at0015"] = <
					text = <"Last updated">
					description = <"The date cause of death was last updated.">
				>
				["at0013"] = <
					text = <"Comment">
					description = <"Additional narrative about the cause of death not captured in other fields.">
				>
				["at0012"] = <
					text = <"Additional details">
					description = <"Additional details about the cause of death.">
				>
				["at0016"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0017"] = <
					text = <"Duration">
					description = <"Approximate time from onset of direct cause to death.">
				>
				["at0018"] = <
					text = <"Other condition">
					description = <"Other significant conditions contributing to death.">
					comment = <"It is strongly recommended that 'Other condition' be coded with an external terminology. For example: Snomed CT, ICD-10.">
				>
				["at0019"] = <
					text = <"Description">
					description = <"Narrative description about the cause of death including events leading up to death.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Dødsårsak">
					description = <"Detaljer om spesifik sykdom eller tilstand som forårsaket eller medvirket til individets død.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0014"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Umiddelbar dødsårsak">
					description = <"Årsak som førte til død.">
					comment = <"Det anbefales på det sterkeste å kode \"Umiddelbar dødsårsak\" med en ekstern terminologi. For eksempel: Snomed CT eller ICD-10. 

Dette vil være årsak A i eksempelet:
Hjerneblødning (Umiddelbar dødsårsak, A) 
- som følge av Metastase i hjernen (B)
- som følge av Brystkreft (C)">
				>
				["at0003"] = <
					text = <"Hendelses sekvens">
					description = <"Årsak og / eller hendelser inklusiv dato for sykdom eller hendelse, rekkefølge og årsak som førte til individets død.">
				>
				["at0004"] = <
					text = <"Indirekte årsak">
					description = <"Navn på tilstand eller hendelse som indirekte medvirket til død.">
					comment = <"Det anbefales på det sterkeste å kode \"Umiddelbar dødsårsak\" med en ekstern terminologi. For eksempel: Snomed CT eller ICD-10. 

Her behøves to instanser for å representere årsak B og årsak C i dette eksempelet:
Hjerneblødning (Umiddelbar dødsårsak, A) 
- som følge av Metastase i hjernen (B)
- som følge av Brystkreft (C)">
				>
				["at0005"] = <
					text = <"Rekkefølge">
					description = <"Rekkefølge av hendelser, hvor den nyeste hendelsen settes øverst.">
					comment = <"Hvis et alternativt verdisett er nødvendig, kan disse verdiene legges til DV_TEXT-datatypen i et templat.

Som et eksempel:
Hjerneblødning (Umiddelbar dødsårsak, A) 
- som følge av Metastase i hjernen (B)
- som følge av Brystkreft (C)">
				>
				["at0006"] = <
					text = <"Medvirkende årsak B">
					description = <"På grunn av eller som følge av.">
				>
				["at0007"] = <
					text = <"Medvirkende årsak C">
					description = <"På grunn av eller som følge av.">
				>
				["at0008"] = <
					text = <"Medvirkende årsak D">
					description = <"På grunn av eller som følge av.">
				>
				["at0009"] = <
					text = <"Tidsberegning">
					description = <"Omtrentlig tid fra årsaksdebut til død.">
				>
				["at0015"] = <
					text = <"Sist oppdatert">
					description = <"Dato når dødsårsak sist ble oppdatert.">
				>
				["at0013"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om dødsårsak som ikke er fanget opp i andre felt.">
				>
				["at0012"] = <
					text = <"Ytterligere detaljer">
					description = <"Ytterligere detaljer om dødsårsak.">
				>
				["at0016"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0017"] = <
					text = <"Varighet">
					description = <"Omtrentlig tid fra årsaksdebut til død.">
				>
				["at0018"] = <
					text = <"Medvirkende årsak">
					description = <"Andre medvirkende årsaker til død.">
					comment = <"Det anbefales på det sterkeste å kode \"Medvirkende årsak\" med en terminologi. For eksempel: Snomed CT eller ICD-10.">
				>
				["at0019"] = <
					text = <"Beskrivelse">
					description = <"Fritekstlig beskrivelse om dødsårsak samt hendelsene før død.">
				>
			>
		>
	>
