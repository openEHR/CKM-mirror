definition
	EVALUATION[at0000] matches {    -- Cause of death
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Direct cause
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {    -- Direct time interval
						value matches {
							DV_DURATION matches {
								value matches {|>=PT0S|}
							}
							DV_INTERVAL<DV_DURATION> matches {
								lower matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
								upper matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
							}
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0003] occurrences matches {0..*} matches {    -- Intermediate cause(s)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] matches {    -- Intermediate cause
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Order
								value matches {
									DV_TEXT matches {*}
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0006,    -- B
											at0007,    -- C
											at0008]    -- D
										}
									}
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {    -- Time interval
								value matches {
									DV_TEXT matches {*}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
										upper matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
									}
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {    -- Underlying cause
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {    -- Underlying time interval
						value matches {
							DV_DURATION matches {
								value matches {|>=PT0S|}
							}
							DV_INTERVAL<DV_DURATION> matches {
								lower matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
								upper matches {
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
							}
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0020] occurrences matches {0..*} matches {    -- Contributing conditions
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0018] matches {    -- Condition name
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0021] occurrences matches {0..1} matches {    -- Time interval
								value matches {
									DV_TEXT matches {*}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
										upper matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
									}
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0012] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0014] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0016] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Cause of death">
					description = <"Details about specific diseases, injuries or conditions that directly caused or contributed to the death of an individual.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0014"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Direct cause">
					description = <"The disease, injury or condition that occurred immediately before death, or closest to the time of death.">
					comment = <"It is strongly recommended that 'Direct cause' be coded with an external terminology. 

This data element is considered as Contributory cause A' within a death certificate recording a chain of events leading to death. For example:
Cerebral haemorrhage (as 'Direct cause', A) 
- 'due to' Metastasis of the brain (B);
- 'due to' Breast cancer (as the underlying and earliest cause, C).">
				>
				["at0003"] = <
					text = <"Intermediate cause(s)">
					description = <"Details about one or more antecedent diseases, injuries or conditions in the chain of events that led to the death, in 'due to' order and excluding the 'Underlying cause'.">
					comment = <"In the following example, there would only need to be one instances of this 'Intermediate cause(s)' CLUSTER represented in a template':
Cerebral haemorrhage (as 'Direct cause', A) 
- 'due to' Metastasis of the brain (as 'Intermediate cause', B);
- 'due to' Breast cancer (as the earliest 'Underlying cause').
If there was more than one 'Intermediate cause', each would be represented as a separate instance of this CLUSTER, using unique order designations eg 'B', preceded by 'C' etc.">
				>
				["at0004"] = <
					text = <"Intermediate cause">
					description = <"Identification of an antecedent disease, injury or condition that directly contributed to the 'Direct cause', excluding the 'Underlying cause'.">
					comment = <"It is strongly recommended that 'Intermediate cause' be coded with an external terminology.">
				>
				["at0005"] = <
					text = <"Order">
					description = <"Order of the onset of the 'Intermediate cause' in the sequence of events leading to death.">
					comment = <"For example: the condition labelled 'B' is considered the closest cause directly contributing to the 'Direct cause'. A condition labelled as 'C' would be an earlier cause, contributing to the Intermediate cause labelled as 'B'.">
				>
				["at0006"] = <
					text = <"B">
					description = <"">
				>
				["at0007"] = <
					text = <"C">
					description = <"">
				>
				["at0008"] = <
					text = <"D">
					description = <"">
				>
				["at0009"] = <
					text = <"Time interval">
					description = <"The time interval from onset of the identified 'Intermediate cause' to death.">
				>
				["at0015"] = <
					text = <"Last updated">
					description = <"The date when the cause of death summary was last updated.">
				>
				["at0013"] = <
					text = <"Comment">
					description = <"Additional narrative about the diseases, injuries and conditions leading to death, not captured in other fields.">
				>
				["at0012"] = <
					text = <"Additional details">
					description = <"Additional details about the cause of death.">
				>
				["at0016"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0017"] = <
					text = <"Direct time interval">
					description = <"Time interval from onset of the identified 'Direct cause' to death.">
				>
				["at0018"] = <
					text = <"Condition name">
					description = <"Other significant condition(s) contributing, but not resulting in the underlying cause of death.">
					comment = <"It is strongly recommended that 'Other condition' be coded with an external terminology. For example: Snomed CT, ICD-10.">
				>
				["at0019"] = <
					text = <"Description">
					description = <"Narrative description about the contributing diseases, injuries, conditions and events leading to death.">
				>
				["at0020"] = <
					text = <"Contributing conditions">
					description = <"Details about other significant conditions that may have contributed to, but not had a caused in the underlying cause of death.">
				>
				["at0021"] = <
					text = <"Time interval">
					description = <"The time interval from onset of the identified disease, injury or condition to death.">
				>
				["at0022"] = <
					text = <"Underlying cause">
					description = <"The underlying or originating disease, injury or condition that started the chain of events leading to death.">
					comment = <"It is strongly recommended that 'Underlying cause' be coded with an external terminology.">
				>
				["at0023"] = <
					text = <"Underlying time interval">
					description = <"Time interval from onset of the identified 'Underlying cause' to death.">
				>
			>
		>
