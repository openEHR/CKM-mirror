					ELEMENT[at0016] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Oocyte maturation stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
