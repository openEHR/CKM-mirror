definition
	EVALUATION[at0000] matches {    -- Alcohol consumption summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0089] occurrences matches {0..1} matches {    -- Overall status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Current drinker
									at0005,    -- Former drinker
									at0006]    -- Lifetime non-drinker
								}
							}
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {    -- Overall description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Regular consumption commenced
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0080] occurrences matches {0..1} matches {    -- Daily consumption commenced
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0114] occurrences matches {0..1} matches {    -- Date first intoxicated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					CLUSTER[at0064] occurrences matches {0..*} matches {    -- Per episode
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0052] occurrences matches {0..1} matches {    -- Status
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0061,    -- Current drinker
											at0091]    -- Non-drinker
										}
									}
								}
							}
							ELEMENT[at0112] occurrences matches {0..1} matches {    -- Episode description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {    -- Episode label
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=1|}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0013] occurrences matches {0..1} matches {    -- Episode start date
								value matches {
									DV_DATE matches {*}
								}
							}
							ELEMENT[at0082] occurrences matches {0..1} matches {    -- Episode end date
								value matches {
									DV_DATE matches {*}
								}
							}
							ELEMENT[at0030] occurrences matches {0..1} matches {    -- Pattern
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0083,    -- Daily
											at0084]    -- Non-daily
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0097] occurrences matches {0..1} matches {    -- Binge drinking frequency
								value matches {
									C_DV_QUANTITY <
										
										list = <
											["1"] = <
												units = <"1/wk">
												magnitude = <|0.0..7.0|>
												precision = <|0|>
											>
											["2"] = <
												units = <"1/mo">
												magnitude = <|0.0..31.0|>
												precision = <|0|>
											>
											["3"] = <
												units = <"1/a">
												magnitude = <|0.0..365.0|>
												precision = <|0|>
											>
										>
									>
									DV_INTERVAL<DV_QUANTITY> matches {
										lower matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"1/wk">
														magnitude = <|0.0..7.0|>
														precision = <|0|>
													>
													["2"] = <
														units = <"1/mo">
														magnitude = <|0.0..31.0|>
														precision = <|0|>
													>
													["3"] = <
														units = <"1/a">
														magnitude = <|0.0..365.0|>
														precision = <|0|>
													>
												>
											>
										}
										upper matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"1/wk">
														magnitude = <|0.0..7.0|>
														precision = <|0|>
													>
													["2"] = <
														units = <"1/mo">
														magnitude = <|0.0..31.0|>
														precision = <|0|>
													>
													["3"] = <
														units = <"1/a">
														magnitude = <|0.0..365.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
								}
							}
							ELEMENT[at0113] occurrences matches {0..1} matches {    -- Binge drinking description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0110] occurrences matches {0..1} matches {    -- Alcohol free days
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::382]>
										list = <
											["1"] = <
												units = <"1/wk">
												magnitude = <|0.0..<7.0|>
												precision = <|0|>
											>
											["2"] = <
												units = <"1/mo">
												magnitude = <|0.0..<31.0|>
												precision = <|0|>
											>
										>
									>
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {    -- Typical consumption (alcohol units)
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::382]>
										list = <
											["1"] = <
												units = <"1/d">
												magnitude = <|>=0.0|>
												precision = <|1|>
											>
											["2"] = <
												units = <"1/wk">
												magnitude = <|>=0.0|>
												precision = <|1|>
											>
											["3"] = <
												units = <"1/mo">
												magnitude = <|>=0.0|>
												precision = <|1|>
											>
										>
									>
								}
							}
							CLUSTER[at0029] occurrences matches {0..*} matches {    -- Per type
								name matches {
									DV_TEXT matches {*}
								}
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0108] matches {    -- Type
										value matches {
											DV_TEXT matches {*}
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0115,    -- Beer
													at0116,    -- Wine
													at0117,    -- Cider
													at0118,    -- Mead
													at0119,    -- Pulque
													at0120,    -- Spirits
													at0121]    -- Fortified wine
												}
											}
										}
									}
									ELEMENT[at0053] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0111] occurrences matches {0..1} matches {    -- Typical consumption (alcohol units)
										value matches {
											C_DV_QUANTITY <
