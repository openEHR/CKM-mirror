										list = <
											["1"] = <
												units = <"Hz/s">
											>
										>
									>
								}
							}
							ELEMENT[at0083] occurrences matches {0..1} matches {	-- Start Frequency
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::382]>
										list = <
											["1"] = <
												units = <"Hz">
												magnitude = <|200.0..2000.0|>
												precision = <|0|>
