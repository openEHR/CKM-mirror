archetype (adl_version=1.4)
	openEHR-EHR-OBSERVATION.fluid_input.v1

concept
	[at0000]	-- Fluid Input
language
	original_language = <[ISO_639-1::en]>
	translations = <
		["sl"] = <
			language = <[ISO_639-1::sl]>
			author = <
				["name"] = <"?">
