	openEHR-EHR-OBSERVATION.cmas_score.v0

concept
	[at0000]	-- Childhood Myositis Assessment Scale
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Hildegard Franke">
		["organisation"] = <"freshEHR Clinical Informatics Ltd.">
