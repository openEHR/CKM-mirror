definition
	OBSERVATION[at0000] matches {	-- IIEF-5-Score
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {	-- Point in time
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Confidence
										value matches {
											1|[local::at0005], 	-- Very low
											2|[local::at0006], 	-- Low
											3|[local::at0007], 	-- Moderate
											4|[local::at0008], 	-- High
											5|[local::at0009]  	-- Very high
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Hard enough for penetration
										value matches {
											0|[local::at0011], 	-- No sexual activity
											1|[local::at0012], 	-- Almost never or never
											2|[local::at0013], 	-- A few times
											3|[local::at0014], 	-- Sometimes
											4|[local::at0015], 	-- Most times
											5|[local::at0016]  	-- Almost always or always
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Maintain after penetration
										value matches {
											0|[local::at0018], 	-- Did not attempt intercourse
											1|[local::at0019], 	-- Almost never or never
											2|[local::at0020], 	-- A few times
											3|[local::at0021], 	-- Sometimes
											4|[local::at0022], 	-- Most times
											5|[local::at0023]  	-- Almost always or always
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {	-- Maintain to completion
										value matches {
											0|[local::at0025], 	-- Did not attempt intercourse
											1|[local::at0026], 	-- Extremely difficult
											2|[local::at0027], 	-- Very difficult
											3|[local::at0028], 	-- Difficult
											4|[local::at0029], 	-- Slightly difficult
											5|[local::at0030]  	-- Not difficult
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {	-- Satisfactory
										value matches {
											0|[local::at0032], 	-- Did not attempt intercourse
											1|[local::at0033], 	-- Almost never or never
											2|[local::at0034], 	-- A few times
											3|[local::at0035], 	-- Sometimes
											4|[local::at0036], 	-- Most times
											5|[local::at0037]  	-- Almost always or always
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {	-- IIEF-5 Score
										value matches {
											DV_COUNT matches {
												magnitude matches {|1..25|}
											}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0039] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0040] occurrences matches {0..*} matches {	-- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0041] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0042] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



