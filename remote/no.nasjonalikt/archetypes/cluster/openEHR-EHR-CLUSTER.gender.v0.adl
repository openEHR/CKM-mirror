ontology
	term_definitions = <
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Kjønn">
					description = <"Detaljert beskrivelse av individets kjønn inkludert identitet, omtale, anatomi, fysiologi og genetikk.">
				>
				["at0001"] = <
					text = <"Opplevd kjønn">
					description = <"Kjønn slik individet opplever det selv.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0002"] = <
					text = <"Fysiologisk kjønn">
					description = <"Individets kjønn ut fra fysiologi, i praksis de dominerende kjønnshormoner som har påvirkning på vevsstrukturer og metabolisme.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0003"] = <
					text = <"Genetisk kjønn">
					description = <"Individets genetiske kjønn.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0014"] = <
					text = <"Beskrivelse">
					description = <"Fritekstbeskrivelse av individets kjønn.">
				>
				["at0019"] = <
					text = <"Anatomisk kjønn">
					description = <"Individets kjønn ut fra primære anatomiske kjønnskarakteristika.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0020"] = <
					text = <"Foretrukne pronomen">
					description = <"De personlige pronomen individet foretrekker å bli omtalt som.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0022"] = <
					text = <"Juridisk kjønn">
					description = <"Individets kjønn i henhold til offentlige registre eller andre juridiske forhold som f.eks. fødselsattest.">
					comment = <"Koding med en terminologi anbefales, der det er mulig.">
				>
				["at0023"] = <
					text = <"Detaljer">
					description = <"SLOT for ytterligere strukturerte detaljer knyttet til individets kjønn.">
					comment = <"For eksempel strukturerte data om gonader, eller detaljer knyttet til genitalia.">
				>
				["at0024"] = <
					text = <"Reseptorkjønn">
					description = <"Hvordan den komplekse sammensatte biologiske kjønnsinformasjonen, på gen-, gonadal  fysiologisk og metabolsk nivå, kan tas imot av en enkelt celle.">
				>
				["at0025"] = <
					text = <"Sosialt kjønn">
					description = <"Hvilket kjønn individet opptrer som i sosiale sammenhenger.">
					comment = <"F.eks \"Går i kvinneklær i private sammenhenger, men aldri på jobb\".">
				>
			>
		>
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Gender">
					description = <"Detailed description of the individual's gender, including identity, pronouns, anatomy, physiology and genetics.">
				>
				["at0001"] = <
					text = <"Gender identity">
					description = <"The individual's gender identity.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0002"] = <
					text = <"Physiological gender">
					description = <"The individual's physiological gender, in practice the dominating sex hormones that influence tissues and metabolism.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0003"] = <
					text = <"Genetic gender">
					description = <"The individual's genetic gender.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0014"] = <
					text = <"Description">
					description = <"Summary narrative description of the individual's gender.">
				>
				["at0019"] = <
					text = <"Anatomical gender">
					description = <"The indivdual's gender by primary anatomical gender characteristics.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0020"] = <
					text = <"Preferred pronouns">
					description = <"The personal pronouns the individual prefers to be referred to with.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0022"] = <
					text = <"Legal gender">
					description = <"The individual's gender according to public registries or other legal circumstances such as birth certificate.">
					comment = <"Coding with a terminology is recommended, where possible.">
				>
				["at0023"] = <
					text = <"Details">
					description = <"Additional structured details about the individuals gender.">
					comment = <"For example: Structured details about gonads, or details about genitalia.">
				>
				["at0024"] = <
					text = <"Receptor gender">
					description = <"How the complex compound biological gender information on a genome, gonad physiological and metabolical level, can be received by a single cell.">
				>
				["at0025"] = <
					text = <"Social gender">
					description = <"The gender the individual appears as in social settings.">
					comment = <"For example: 'Wears womens clothes in private settings, but never at work.'">
				>
			>
		>
	>
