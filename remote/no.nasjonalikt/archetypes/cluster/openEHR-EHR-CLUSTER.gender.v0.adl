	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the individual's gender, including identity, pronouns, anatomy, physiology , genetics, social and receptor gender.">
			use = <"Use to record the individual's gender, including identity, pronouns, anatomy, physiology and genetics.

This archetype has been designed to be used within an ENTRY archetype where relevant, for example within OBSERVATION.story or INSTRUCTION.laboratory_test.">
			misuse = <"Not to be used for recording information relating to sexual orientation or practice.">
		>
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"For å angi individets kjønn, inkludert identitet, pronomen, anatomi, fysiologi, sosialt, genetisk og grad av reseptorkjønn.">
			use = <"Brukes for å angi individets kjønn, inkludert identitet, pronomen, anatomi, fysiologi ,genetisk og grad av resptorkjønn.

Arketypen er laget for å brukes innenfor en ENTRY-arketype der det er relevant, f.eks. innenfor OBSERVATION.story (Anamnese) eller innenfor INSTRUCTION.laboratory_test (Laboratorierekvisisjon).">
			misuse = <"Skal ikke brukes til å registrere informasjon relatert til seksuell orientering eller adferd.">
			copyright = <"© Nasjonal IKT HF">
		>
	>
